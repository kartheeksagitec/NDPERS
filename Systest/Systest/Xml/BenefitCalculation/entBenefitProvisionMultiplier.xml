<?xml version="1.0" encoding="utf-8"?>
<entity ID="entBenefitProvisionMultiplier" sfwTableName="sgt_benefit_provision_multiplier">
  <queries>
    <query ID="GetBenefitProvisionMultiplier" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT * FROM SGT_BENEFIT_PROVISION_MULTIPLIER WITH(NOLOCK)&#xD;&#xA;WHERE EFFECTIVE_DATE=(SELECT TOP 1 EFFECTIVE_DATE&#xD;&#xA;FROM SGT_BENEFIT_PROVISION_MULTIPLIER B WITH(NOLOCK)&#xD;&#xA;WHERE B.EFFECTIVE_DATE &lt;= @CALCULATION_DATE&#xD;&#xA;AND B.BENEFIT_PROVISION_ID= CASE WHEN @ISCONVERSION ='N' THEN @BENEFITPROVISIONID ELSE BENEFIT_PROVISION_ID END&#xD;&#xA;AND ISNULL(B.BENEFIT_ACCOUNT_TYPE_VALUE,'')= @BENEFITACCOUNTTYPEVALUE &#xD;&#xA;AND ISCONVERSION=@ISCONVERSION ORDER BY B.EFFECTIVE_DATE DESC)&#xD;&#xA;AND BENEFIT_PROVISION_ID=CASE WHEN @ISCONVERSION ='N' THEN @BENEFITPROVISIONID ELSE BENEFIT_PROVISION_ID END&#xD;&#xA;AND ISNULL(BENEFIT_ACCOUNT_TYPE_VALUE,'')= @BENEFITACCOUNTTYPEVALUE &#xD;&#xA;AND ISCONVERSION=@ISCONVERSION&#xD;&#xA;ORDER BY TIER_NUMBER">
      <parameters>
        <parameter ID="@CALCULATION_DATE" sfwDataType="DateTime" />
        <parameter ID="@BENEFITPROVISIONID" sfwDataType="int" />
        <parameter ID="@BENEFITACCOUNTTYPEVALUE" sfwDataType="string" />
        <parameter ID="@ISCONVERSION" sfwDataType="string" />
      </parameters>
    </query>
    <query ID="GetLatestBenefitProvisionTypeByEffectiveDate" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT TOP 1 BENEFIT_MULTIPLIER_TYPE_VALUE FROM SGT_BENEFIT_PROVISION_MULTIPLIER BPM&#xD;&#xA;INNER JOIN SGS_CODE_VALUE CV ON BPM.BENEFIT_MULTIPLIER_TYPE_VALUE = CV.CODE_VALUE&#xD;&#xA;WHERE BENEFIT_PROVISION_ID = @BENEFITPROVISIONID AND BPM.EFFECTIVE_DATE &lt;= @RETIREMENT_DATE&#xD;&#xA;ORDER BY CV.CODE_VALUE_ORDER DESC">
      <parameters>
        <parameter ID="@BENEFITPROVISIONID" sfwDataType="int" />
        <parameter ID="@RETIREMENT_DATE" sfwDataType="DateTime" />
      </parameters>
    </query>
  </queries>
