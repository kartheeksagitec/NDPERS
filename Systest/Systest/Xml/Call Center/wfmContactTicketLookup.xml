<form ID="wfmContactTicketLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvCallCenter" sfwResource="900" sfwType="Lookup" sfwTitle="Contact Ticket Lookup" version="3.5" sfwEntity="entContactTicket" sfwGridCollection="busContactTicketLookup.icolContactTicket">
  <initialload>
    <query ID="cdoContactTicket" sfwQueryRef="entContactTicket.Lookup" sfwMethodName="LoadContactTickets" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblperson_id" Text="PERSLink ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txt_person_id" sfwDataField="ct.person_id" sfwDataType="Numeric" sfwQueryID="cdoContactTicket" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txt_person_id" sfwSelectedIndex="PersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblorg_id" Text="Org Code ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txt_org_id" sfwDataField="org.org_code" sfwOperator="=" sfwQueryID="cdoContactTicket" />
                <sfwImageButton ID="btnRetrieve1" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmOrganizationLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txt_org_id" sfwSelectedIndex="OrgCode" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreatedDate" Text="Ticket Date From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDate" sfwDataField="convert(datetime, convert(varchar(10), ct.created_date, 101))" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoContactTicket" sfwRelatedControl="txtModifiedDate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModifiedDate" Text="Ticket Date To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtModifiedDate" sfwDataField="convert(datetime, convert(varchar(10), ct.modified_date, 101))" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoContactTicket" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblcontact_type_value" Text="Contact Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddl_contact_type_value" sfwDataField="ct.contact_type_value" sfwQueryID="cdoContactTicket" sfwLoadType="CodeGroup" sfwLoadSource="1001" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblevent_type_value" Text="Event Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddl_event_type_value" sfwDataField="ct.event_type_value" sfwQueryID="cdoContactTicket" sfwLoadType="CodeGroup" sfwLoadSource="1006" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblcontact_method_value" Text="Contact Method : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddl_contact_method_value" sfwDataField="ct.contact_method_value" sfwQueryID="cdoContactTicket" sfwLoadType="CodeGroup" sfwLoadSource="1003" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblresponse_method_value" Text="Response Method : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddl_response_method_value" sfwDataField="ct.response_method_value" sfwQueryID="cdoContactTicket" sfwLoadType="CodeGroup" sfwLoadSource="1004" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lbloriginal_contact_ticket_id" Text="Ticket Number : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txt_original_contact_ticket_id" sfwDataField="ct.contact_ticket_id" sfwDataType="Numeric" sfwQueryID="cdoContactTicket" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatusValue" Text="Ticket Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="ct.status_value" sfwOperator="like" sfwQueryID="cdoContactTicket" sfwLoadType="CodeGroup" sfwLoadSource="1002" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capAssignToUserId" Text="Assign To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="user_id" DataValueField="user_serial_id" ID="ddlAssignToUserId" sfwDataField="ct.assign_to_user_id" sfwOperator="=" sfwQueryID="cdoContactTicket" sfwLoadType="Query" sfwLoadSource="entUser.LOADACTIVEUSER" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capAssignToUserId1" Text="User ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="user_id" DataValueField="user_id" ID="ddlAssignToUserId1" sfwDataField="ct.created_by" sfwOperator="=" sfwQueryID="cdoContactTicket" sfwLoadType="Query" sfwLoadSource="entUser.LOADACTIVEUSER" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmContactTicketMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmContactTicketMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintcontactticketid=ContactTicketId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" sfwResource="901" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many" sfwSortExpression="ContactTicketId desc" SortExpressionUpdated="true">
                  <Columns>
                    <TemplateField HeaderText="PERSLink ID" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PersonId" sfwVisibleRule="VisibleRulePerslinkIdIsZero" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Person Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPerson.FullName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Code ID" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="IstrOrgCodeID" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objOrganization.OrgName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText=" Ticket #">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ContactTicketId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Contact Type">
                      <ItemTemplate>
                        <sfwLabel sfwWrap="True" sfwEntityField="ContactTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Event Type">
                      <ItemTemplate>
                        <sfwLabel sfwWrap="True" sfwEntityField="EventTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Ticket Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Contact Method">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ContactMethodDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Response Method">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ResponseMethodDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="User ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>