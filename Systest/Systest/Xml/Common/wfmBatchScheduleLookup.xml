<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmBatchScheduleLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="405 " sfwType="Lookup" sfwTitle="Batch Schedule  Lookup" sfwHelpFile="Batch Overview=Batch Overview.htm;Create Batch Step=Create Batch Step.htm;Update Batch Step=Update Batch Step.htm;Delete Batch Step=Delete Batch Step.htm" version="3.5" sfwEntity="entBatchSchedule" sfwGridCollection="busBatchScheduleLookup.iclbLookupResult">
  <initialload>
    <query ID="cdoBatchSchedule" sfwQueryRef="entBatchSchedule.LookUp" sfwMethodName="LoadBatchSchedules" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capBatchScheduleID" Text="Batch Schedule ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtuser_id" sfwDataField="batch_schedule_id" sfwQueryID="cdoBatchSchedule" Width="250px" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStepNumber" Text="Step Number :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtlast_name" sfwDataField="step_no" sfwQueryID="cdoBatchSchedule" Width="250px" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capStepName" Text="Step Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtfirst_name" sfwDataField="step_name" sfwQueryID="cdoBatchSchedule" Width="250px" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStepDescription" Text="Step Description :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtStepDescription" sfwDataField="step_description" sfwQueryID="cdoBatchSchedule" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmBatchScheduleMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmBatchScheduleMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintBatchScheduleId=BatchScheduleId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="BatchScheduleId" sfwRelatedControl="dgrResult" Text="Delete" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" PageSize="100" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="BatchScheduleId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Step No">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StepNo" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Step Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StepName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Step Description">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StepDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Active Flag" ItemStyle.HorizontalAlign="Center">
                      <ItemTemplate>
                        <sfwCheckBox sfwTwoWayBinding="False" sfwEntityField="ActiveFlag" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Frequency In Days">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="FrequencyInDays" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Frequency In Months">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="FrequencyInMonths" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Next Run Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="NextRunDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Step Parameters">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StepParameters" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>