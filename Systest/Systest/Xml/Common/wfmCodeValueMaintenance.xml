<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmCodeValueMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="411 " sfwType="Maintenance" sfwTitle="Code Value Maintenance" version="3.5" sfwEntity="entCodeValue">
