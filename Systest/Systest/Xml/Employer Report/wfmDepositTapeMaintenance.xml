                <sfwLabel ID="lblPaymentMethodReq" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capDepositMethodValue" Text="Deposit Method : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlDepositMethodValue" sfwLoadType="CodeGroup" sfwLoadSource="1501" sfwEntityField="DepositMethodValue" sfwReadOnlyRule="Rule Check Deposit tape is frozen(Deposits are frozen 30 days after the Deposit Date)" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblDepositAmt" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capTotalAmount" Text="Deposit Total : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTotalAmount" sfwDataFormat="{0:C}" sfwEntityField="TotalAmount" sfwReadOnlyRule="Rule Check Deposit tape is frozen(Deposits are frozen 30 days after the Deposit Date)" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capLTotalDollarAmount" Text="Totals from Detail : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblTotalDepositAmount" sfwDataFormat="{0:C}" sfwEntityField="TotalDepositAmount" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capLdclTotalRemittanceAmount" Text="Total Remittances : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblIdecTotalRemittance" sfwDataFormat="{0:C}" sfwEntityField="IdecTotalRemittance" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblIintActivityInstanceID" Visible="False" sfwEntityField="IintActivityInstanceID" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
