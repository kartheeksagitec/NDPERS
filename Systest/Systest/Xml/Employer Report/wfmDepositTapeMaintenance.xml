<form ID="wfmDepositTapeMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvEmployerReport" sfwResource="1009" sfwType="Maintenance" sfwTitle="Deposit Tape Maintenance" version="3.5" sfwEntity="entDepositTape">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewDepositTape" />
    <callmethods sfwMode="Update" sfwMethodName="FindDepositTape" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="Aintdeposittapeid=DepositTapeId;aintActivityInstanceID=IintActivityInstanceID" sfwVisibleRule="Rule Save Button If Status is Review or Valid Deposit Date" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton ID="btnValidate" sfwCheckChanges="False" sfwMethodName="btnExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="ValidateDepositTape" sfwVisibleRule="Rule Check Visible Validate Button" Text="Validate" />
                <sfwButton ID="btnCorrespondence" sfwMethodName="btnCorrespondence_Click" Text="Correspondence" Visible="False" />
                <sfwLabel Font.Bold="True" ID="lblInfo" sfwMode="Update" SkinID="ReqInformation" Text="Deposit tape is frozen.Deposits are frozen 30 days after the Deposit Date." Visible="False" sfwIsCaption="True" />
                <sfwButton ID="btnACHPull" sfwMessageId="4161" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnACHPull_Click" sfwVisibleRule="VisibleRulePullACHIBSInsurance" Text="Pull ACH" />
                <sfwButton ID="btnACHPullRetirement" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnACHPullRetirement_Click" sfwVisibleRule="VisibleRulePulACHRetirement" Text="Pull ACH Retirement" />
                <sfwButton ID="btnPullACHInsurance" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnACHPullInsurance_Click" sfwVisibleRule="VisibleRulePullACHInsurance" Text="Pull ACH Insurance" />
                <sfwButton ID="btnPullACHDefComp" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnACHPullDefComp_Click" sfwVisibleRule="VisibleRullPullACHDeferredComp" Text="Pull ACH Def. Comp" />
                <sfwButton ID="btnPullACHServicePurchase" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnACHPullServicePurchase_Click" Text="Pull ACH Purchase" Visible="False" />
                <sfwButton ID="btnApply" OnClientClick="return confirm('“Are you sure you want to apply the Deposit and trigger the G/L?” when the ‘Deposit’ is changed to ‘APPLIED’’')" sfwMethodName="btnExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnApply_Click" sfwSecurityLevel="3" Text="Apply" sfwVisibleRule="Rule Check If Apply Button Visible" />
                <sfwButton ID="btnAllocateRemittanceTwo" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwMode="Update" sfwObjectMethod="btnAllocateRemittance_Click" Text="Allocate Remittances" sfwVisibleRule="VisibleRuleForBtnAllocateRemittance" Visible="False" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Deposit Tape Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblDepositDateReq" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capDepositDate" Text="Deposit Date : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDepositDate" sfwDataFormat="{0:d}" sfwEntityField="DepositDate" sfwReadOnlyRule="Rule Check Deposit tape is frozen(Deposits are frozen 30 days after the Deposit Date)" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capDepositTapeId" Text="Deposit Tape ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblDepositTapeId" sfwEntityField="DepositTapeId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblBankAccountReq" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capBankAccountValue" Text="Bank Account : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlBankAccountValue" sfwLoadType="CodeGroup" sfwLoadSource="1201" sfwEntityField="BankAccountValue" sfwReadOnlyRule="Rule Check Deposit tape is frozen(Deposits are frozen 30 days after the Deposit Date)" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capNoOfDepositDetails" Text="No Of Deposit Details : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblDepositsCount" sfwEntityField="DepositsCount" />
