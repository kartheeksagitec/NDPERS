<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmPlanMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvOrganization" sfwResource="309 " sfwType="Maintenance" sfwTitle="Plan Maintenance" version="3.5" sfwEntity="entPlan">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewPlan" />
    <callmethods sfwMode="Update" sfwMethodName="FindPlan" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="Aintplanid=PlanId" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton ID="btnCorrespondence" sfwMethodName="btnCorrespondence_Click" Text="Correspondence" Visible="False" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Plan Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPlanId" Text="Plan ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn HorizontalAlign="Left">
                <sfwLabel ID="lblPlanId" sfwEntityField="PlanId" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPlanName" Text="Plan Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPlanName" sfwEntityField="PlanName" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capBenefitTypeValue" Text="Benefit Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlBenefitTypeValue" sfwLoadType="CodeGroup" sfwLoadSource="250" sfwEntityField="BenefitTypeValue" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPlanCode" Text="Plan Code : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPlanCode" sfwMode="New" sfwEntityField="PlanCode" />
                <sfwLabel ID="lblPlanCode" sfwMode="Update" sfwEntityField="PlanCode" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capBankAccountTypeValue" Text="Bank Account Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlBankAccountTypeValue" sfwLoadType="CodeGroup" sfwLoadSource="1201" sfwEntityField="BankAccountTypeValue" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="chkIncludeInRetireeAnnualStatementFlag" ID="capIncludeInRetireeAnnualStatementFlag" Text="Include In Retiree Annual Statement Flag : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkIncludeInRetireeAnnualStatementFlag" sfwClientVisibility="controllist:ddlBenefitTypeValue,capBenefitTypeValue#RETR:ddlBenefitTypeValue,capBenefitTypeValue;default:ddlBenefitTypeValue,capBenefitTypeValue;" sfwDefaultValue="N" sfwEntityField="IncludeInRetireeAnnualStatementFlag" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capAllowDeposit" Text="Allow Deposit : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkAllowDeposit" sfwDefaultValue="N" sfwEntityField="AllowDepositFlag" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capAllowNewEmployer" Text="Allow New Employer : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkAllowNewEmployer" sfwDefaultValue="N" sfwEntityField="AllowNewEmployerFlag" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capAllowNewMemberEnroll" Text="Allow New Member Enroll : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkAllowNewMemberEnroll" sfwDefaultValue="N" sfwEntityField="AllowNewMemberEnrollFlag" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capAllowProvider" Text="Allow Provider : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkAllowProvider" sfwDefaultValue="N" sfwEntityField="AllowProviderFlag" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capRetirementTypeValue" Text="Retirement Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlRetirementTypeValue" sfwLoadType="CodeGroup" sfwLoadSource="251" sfwEntityField="RetirementTypeValue" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capApplyRhicFlag" Text="RHIC can be applied : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkApplyRhicFlag" sfwEntityField="ApplyRhicFlag" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlRetirementRates" sfwCaption="Retirement Rates" sfwVisibleRule="Visible Rule Check Benefit Type is Retirement">
          <sfwTable CssClass="Table" ID="tblRetirementRates">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNewRetirementRate" sfwActiveForm="wfmPlanRetirementRateMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="AintPlanid=lblPlanId" Text="New" />
                <sfwButton ID="btnOpenRetirementRate" sfwActiveForm="wfmPlanRetirementRateMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintplanrateid=PlanRateId" sfwRelatedControl="grvRetirementRate" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnExportToExcel" sfwMethodName="btnColumnsToExport_Click" Text="Export To Excel" sfwRelatedControl="grvRetirementRate" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" AutoGenerateColumns="False" ID="grvRetirementRate" sfwSelection="Many" sfwEntityField="lstRetirementRate" sfwGridEntity="entPlanRetirementRate">
                  <Columns>
                    <TemplateField HeaderText="Plan Rate ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenRetirementRate" sfwEntityField="PlanRateId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Plan Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPlan.PlanName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Member Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="MemberTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="EE Emp Pickup" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="EeEmpPickup" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="EE Post Tax" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="EePostTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="EE Pre Tax" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="EePreTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="EE Rhic" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="EeRhic" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Effective Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="ER Pre Tax" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="ErPostTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="ER RHIC" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="ErRhic" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="DC-DB EE Pre Tax" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="AddlEePreTax" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="DC-DB EE Post Tax" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="AddlEePostTax" sfwDataFormat="{0:#0.00}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="DC-DB EE Emp Pickup" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:#0.00}" sfwEntityField="AddlEeEmpPickup" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Additional EE Pre Tax">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="EePretaxAddl" sfwDataFormat="{0:#0.00'%}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Additional EE Post Tax">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="EePostTaxAddl" sfwDataFormat="{0:#0.00'%}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Er Pretax Match">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ErPretaxMatch" sfwDataFormat="{0:#0.00'%}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Adec">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Adec" sfwDataFormat="{0:#0.00'%}" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwTwoWayBinding="True" Visible="False" sfwEntityField="UpdateSeq" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="lstRetirementRate" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>