  <queries>
    <query ID="UpdateRolloverDetailStatusToProcessed" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="UPDATE PARD SET PARD.STATUS_VALUE='PRCD',MODIFIED_BY='PERSLink Batch'+' '+ convert(nvarchar,@batchscheduleid),MODIFIED_DATE=GETDATE(),&#xD;&#xA;   PARD.UPDATE_SEQ = PARD.UPDATE_SEQ+1&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL PARD&#xD;&#xA; INNER JOIN SGT_PAYMENT_HISTORY_HEADER PHH &#xD;&#xA; ON PARD.PAYEE_ACCOUNT_ID =PHH.PAYEE_ACCOUNT_ID&#xD;&#xA; AND PHH.ORG_ID = PARD.ROLLOVER_ORG_ID  &#xD;&#xA;INNER JOIN&#xD;&#xA; (&#xD;&#xA;  SELECT DISTINCT PAYMENT_HISTORY_HEADER_ID FROM SGT_PAYMENT_HISTORY_DISTRIBUTION WHERE &#xD;&#xA;  PAYMENT_METHOD_VALUE IN ('RCHK','RACH')&#xD;&#xA; ) AS PHDN&#xD;&#xA; ON PHDN.PAYMENT_HISTORY_HEADER_ID = PHH.PAYMENT_HISTORY_HEADER_ID&#xD;&#xA;INNER JOIN SGT_PAYEE_ACCOUNT PA on&#xD;&#xA;PARD.PAYEE_ACCOUNT_ID = PA.PAYEE_ACCOUNT_ID &#xD;&#xA;WHERE PHH.PAYMENT_SCHEDULE_ID=@PAYMENTSCHEDULEID&#xD;&#xA;and PA.BENEFIT_OPTION_VALUE &lt;&gt; '5YTL'&#xD;&#xA;AND PARD.STATUS_VALUE = 'ACTV' -- PIR 938 (CANCELED ROLLOVER DETAILS WERE ALSO GETTING PROCESSED)&#xD;&#xA;&#xD;&#xA;UPDATE PARD SET PARD.STATUS_VALUE='PRCD',MODIFIED_BY='PERSLink Batch',MODIFIED_DATE=GETDATE(),&#xD;&#xA;   PARD.UPDATE_SEQ = PARD.UPDATE_SEQ+1&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL PARD&#xD;&#xA; INNER JOIN SGT_PAYMENT_HISTORY_HEADER PHH &#xD;&#xA; ON PARD.PAYEE_ACCOUNT_ID =PHH.PAYEE_ACCOUNT_ID&#xD;&#xA; AND PHH.ORG_ID = PARD.ROLLOVER_ORG_ID  &#xD;&#xA;INNER JOIN&#xD;&#xA; (&#xD;&#xA;  SELECT DISTINCT PAYMENT_HISTORY_HEADER_ID FROM SGT_PAYMENT_HISTORY_DISTRIBUTION WHERE &#xD;&#xA;  PAYMENT_METHOD_VALUE IN ('RCHK','RACH')&#xD;&#xA; ) AS PHDN&#xD;&#xA; ON PHDN.PAYMENT_HISTORY_HEADER_ID = PHH.PAYMENT_HISTORY_HEADER_ID&#xD;&#xA;INNER JOIN SGT_PAYEE_ACCOUNT PA &#xD;&#xA; ON PARD.PAYEE_ACCOUNT_ID = PA.PAYEE_ACCOUNT_ID &#xD;&#xA;INNER JOIN SGT_PAYMENT_SCHEDULE PS&#xD;&#xA; ON PHH.PAYMENT_SCHEDULE_ID = PS.PAYMENT_SCHEDULE_ID&#xD;&#xA;WHERE PHH.PAYMENT_SCHEDULE_ID=@PAYMENTSCHEDULEID&#xD;&#xA;and PA.BENEFIT_OPTION_VALUE = '5YTL'&#xD;&#xA;and PA.BENEFIT_END_DATE &lt;= (DATEADD(M,1,PS.PAYMENT_DATE)-1 )&#xD;&#xA;AND PARD.STATUS_VALUE = 'ACTV' -- PIR 938 (CANCELED ROLLOVER DETAILS WERE ALSO GETTING PROCESSED)">
      <parameters>
        <parameter ID="@PAYMENTSCHEDULEID" sfwDataType="int" />
        <parameter ID="@batchscheduleid" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="LoadRolloverDetailsForNextPaymentDate" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT PARD.* FROM SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL PARD&#xD;&#xA; INNER JOIN &#xD;&#xA; (&#xD;&#xA;  SELECT PA.* FROM SGT_PAYEE_ACCOUNT PA &#xD;&#xA;  INNER JOIN SGT_PAYEE_ACCOUNT_STATUS PAS&#xD;&#xA;   ON PAS.PAYEE_ACCOUNT_ID=PA.PAYEE_ACCOUNT_ID &#xD;&#xA;  INNER JOIN SGS_CODE_VALUE CV WITH(NOLOCK)&#xD;&#xA;   ON PAS.STATUS_ID= CV.CODE_ID AND PAS.STATUS_VALUE = CV.CODE_VALUE&#xD;&#xA;  WHERE PA.BENEFIT_BEGIN_DATE&lt;=@PAYMENTDATE&#xD;&#xA;   AND (CV.DATA2 = 'RECV' OR CV.DATA2 = 'APRD' ) &#xD;&#xA;   AND PAS.PAYEE_ACCOUNT_STATUS_ID  = &#xD;&#xA;   (&#xD;&#xA;    SELECT TOP 1 PAYEE_ACCOUNT_STATUS_ID&#xD;&#xA;     FROM SGT_PAYEE_ACCOUNT_STATUS PAS1 (NOLOCK)&#xD;&#xA;     WHERE PAS1.PAYEE_ACCOUNT_ID = PA.PAYEE_ACCOUNT_ID&#xD;&#xA;      AND PAS1.STATUS_EFFECTIVE_DATE &lt;= @PAYMENTDATE&#xD;&#xA;     ORDER BY PAS1.STATUS_EFFECTIVE_DATE DESC,PAS1.PAYEE_ACCOUNT_STATUS_ID DESC&#xD;&#xA;   )&#xD;&#xA; )PAYEE &#xD;&#xA; ON PAYEE.PAYEE_ACCOUNT_ID=PARD.PAYEE_ACCOUNT_ID">
      <parameters>
        <parameter ID="@PAYMENTDATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="LoadTaxableAmountBasedOnRolloverId" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="select PAPIT.AMOUNT,PARD.ROLLOVER_ORG_ID from SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL PARD WITH(NOLOCK)&#xD;&#xA;inner join SGT_PAYEE_ACCOUNT_ROLLOVER_ITEM_DETAIL PARID WITH(NOLOCK)&#xD;&#xA;on PARD.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID = PARID.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID&#xD;&#xA;inner join SGT_PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE PAPIT WITH(NOLOCK)&#xD;&#xA;on PARD.PAYEE_ACCOUNT_ID = PAPIT.PAYEE_ACCOUNT_ID&#xD;&#xA;and PAPIT.PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE_ID = PARID.PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE_ID&#xD;&#xA;inner join SGT_PAYMENT_ITEM_TYPE PIT WITH(NOLOCK)&#xD;&#xA;ON PAPIT.PAYMENT_ITEM_TYPE_ID = PIT.PAYMENT_ITEM_TYPE_ID&#xD;&#xA;where PAPIT.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID&#xD;&#xA;and PARD.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID = @PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID &#xD;&#xA;and PIT.ITEM_TYPE_CODE in ('ITEM217','ITEM220')">
      <parameters>
        <parameter ID="@PAYEE_ACCOUNT_ID" sfwDataType="int" />
        <parameter ID="@PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="SetUpdateFlag" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="select * from SGT_PAYEE_ACCOUNT_ROLLOVER_DETAIL PARD WITH(NOLOCK)&#xD;&#xA;inner join SGT_PAYEE_ACCOUNT_ROLLOVER_ITEM_DETAIL PARID WITH(NOLOCK)&#xD;&#xA;on PARD.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID = PARID.PAYEE_ACCOUNT_ROLLOVER_DETAIL_ID&#xD;&#xA;inner join SGT_PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE PAPIT WITH(NOLOCK)&#xD;&#xA;on PARD.PAYEE_ACCOUNT_ID = PAPIT.PAYEE_ACCOUNT_ID&#xD;&#xA;and PAPIT.PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE_ID = PARID.PAYEE_ACCOUNT_PAYMENT_ITEM_TYPE_ID&#xD;&#xA;where PAPIT.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID&#xD;&#xA;and PARD.ROLLOVER_ORG_ID = @ROLLOVER_ORG_ID &#xD;&#xA;and PAPIT.PAYMENT_ITEM_TYPE_ID = @PAYMENT_ITEM_TYPE_ID">
      <parameters>
