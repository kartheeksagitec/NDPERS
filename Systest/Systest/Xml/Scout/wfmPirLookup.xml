<form ID="wfmPirLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvScout" sfwResource="801" sfwType="Lookup" sfwTitle="Problem Information Record Lookup" sfwMaxCount="1000" version="3.5" sfwEntity="entPir" sfwGridCollection="busPirLookup.iclbLookupResult">
  <initialload>
    <query ID="cdoPir" sfwQueryRef="entPir.Lookup" sfwMethodName="LoadPirs" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria" CssClass="ContainerPanel auto-width-panel">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Problem ID :" sfwIsCaption="True" ID="capProblemId" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPirKey" sfwDataField="pir_id" sfwQueryID="cdoPir" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel Text="Description :" sfwIsCaption="True" ID="capDescription" />
              </sfwColumn>
              <sfwColumn ColumnSpan="3">
                <sfwTextBox ID="txtShortDescription" sfwDataField="pir_description" sfwQueryID="cdoPir" Width="250px" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Status :" sfwIsCaption="True" ID="capStatus" />
              </sfwColumn>
              <sfwColumn>
                <sfwMultiSelectDropDownList ID="ddlStatusValue" sfwDataField="status_value" sfwQueryID="cdoPir" sfwLoadType="CodeGroup" sfwLoadSource="40" sfwOperator="in" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel Text="Severity :" sfwIsCaption="True" ID="capSeverity" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList sfwDataField="severity_value" sfwQueryID="cdoPir" sfwLoadType="CodeGroup" sfwLoadSource="42" ID="ddlSeverityValue" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Reported By :" sfwIsCaption="True" ID="capReportedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="full_name" DataValueField="user_serial_id" ID="ddlReportedBy" sfwDataField="reported_by_id" sfwQueryID="cdoPir" sfwLoadType="Query" sfwLoadSource="entPir.ListOfPirReporters" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel Text="Assigned To :" sfwIsCaption="True" ID="capAssignedTo" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="full_name" DataValueField="user_serial_id" ID="ddlAssignedTo" sfwDataField="assigned_to_id" sfwQueryID="cdoPir" sfwLoadType="Query" sfwLoadSource="entPir.ListOfPirFixers" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Testcase :" sfwIsCaption="True" ID="capTestCase" />
              </sfwColumn>
              <sfwColumn ColumnSpan="5">
                <sfwDropDownList DataTextField="testcase_desc" DataValueField="testcase_id" ID="ddlTestcaseScenario" sfwDataField="testcase_scenario" sfwQueryID="cdoPir" Width="600px" sfwLoadType="Query" sfwLoadSource="entTestcase.ListOfTC" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel sfwIsCaption="True" Visible="False" ID="capText" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList sfwDataField="priority_value" sfwQueryID="cdoPir" sfwLoadType="CodeGroup" sfwLoadSource="41" ID="ddlPriorityValue" Visible="False" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text=" Screen Affected :" sfwIsCaption="True" ID="capScreenAffected" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="istrScreenName" DataValueField="istrScreenName" sfwDataField="screen_affected" sfwQueryID="cdoPir" sfwLoadType="ServerMethod" sfwLoadSource="GetScreens" ID="ddlScreenAffected" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capReferentId" Text="Referent : " AssociatedControlID="ddlReferentId" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlReferentId" sfwDataField="referent_id" sfwQueryID="cdoPir" sfwDataType="Numeric" sfwLoadType="Query" DataTextField="full_name" sfwLoadSource="entPir.ListOfPirReporters" DataValueField="user_serial_id" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Release Info :" sfwIsCaption="True" ID="capReleaseInfo" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox sfwDataField="release_info" sfwQueryID="cdoPir" sfwOperator="like" ID="txtReleaseInfo" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCreatedBy" Text="Created By : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwMultiSelectDropDownList ID="ddlCreatedBy" sfwDataField="created_by" DataTextField="user_id" DataValueField="user_id" sfwOperator="in" sfwQueryID="cdoPir" sfwLoadType="Query" sfwLoadSource="entUser.LOADACTIVEUSER" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtCriticalPirPriority" ID="capCriticalPirPriority" Text="Critical PIR Priority : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCriticalPirPriority" sfwDataField="critical_pir_priority" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoPir" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="txtPriorityDate" ID="capPriorityDate" Text="Priority Date : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPriorityDate" sfwDataField="RIGHT(CONVERT(VARCHAR(10), priority_date, 103), 7)" sfwDataFormat="{0:MM/YYYY}" sfwOperator="=" sfwQueryID="cdoPir" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capUpdateDocFlag" Text="Update Documentation : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkUpdateDocFlag" sfwDataField="update_doc_flag" sfwOperator="like" sfwQueryID="cdoPir" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capPIRType" sfwIsCaption="True" Text="PIR Type : " />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlPirTypeValue" sfwDataField="pir_type_value" sfwQueryID="cdoPir" sfwLoadType="CodeGroup" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateFrom" Text="Created Date From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateFrom" sfwDataField="created_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="&gt;=" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateTo" Text="Created Date To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateTo" sfwDataField="created_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="&lt;=" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmPirMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmPirMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPirId=PirId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="PirId" sfwRelatedControl="dgrResult" Text="Delete" Visible="False" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
                <sfwButton ID="btnReadyforSystest" sfwMessageId="8308" sfwMethodName="btnExecuteServerMethodFromLookup_Click" sfwObjectID="busPir" sfwObjectMethod="btnReleaseToSystest" sfwRelatedControl="dgrResult" Text="Release to SysTest" Visible="False" />
                <sfwButton ID="btnReleaseToUAT" sfwMessageId="8309" sfwMethodName="btnExecuteServerMethodFromLookup_Click" sfwObjectID="busPir" sfwObjectMethod="btnReleaseToUAT" sfwRelatedControl="dgrResult" Text="Release to UAT" />
                <sfwButton ID="btnReleasetoProd" sfwMessageId="8310" sfwMethodName="btnExecuteServerMethodFromLookup_Click" sfwObjectID="busPir" sfwObjectMethod="btnReleaseToPROD" sfwRelatedControl="dgrResult" Text="Release to PROD" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" PageSize="200" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Pir ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PirId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Priority" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PriorityDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Test Case ">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objTestcase.TestcaseKey" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Description">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PirDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Severity">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="SeverityDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status ">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Assigned To">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objAssignedTo.UserFullName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Reported By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objReportedBy.UserFullName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Release">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ReleaseInfo" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created Date">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Screen Affected">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ScreenAffected" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Referent" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objReferentBy.UserFullName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Priority Date" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PriorityDate" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>