<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmRequirementLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvScout" sfwResource="802 " sfwType="Lookup" sfwTitle="Requirement Lookup" sfwMaxCount="8000" version="3.5" sfwEntity="entRequirement" sfwGridCollection="busRequirementLookup.iclbLookupResult">
