        <sfwLiteral ID="ltrRepReqPayments" Text="&lt;b&gt;Payroll Reports requiring payment.  Click on the link for the report you want to pay.&lt;/b&gt;" Visible="False" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHeaderTab" sfwCaption="Search Parameters" CssClass="hideControl">
      <items>
        <sfwTable ID="tblHeaderTab" />
        <sfwTabContainer ID="tctHeader">
          <Tabs>
            <sfwTabSheet HeaderText="Criteria" ID="tshCriteria">
              <items>
                <sfwTable ID="tblCriteria" />
                <sfwLabel ID="capEmployerPayrollHeaderId" Text="Report ID :" sfwIsCaption="True" />
                <sfwTextBox ID="txtEmployerPayrollHeaderId" sfwDataField="temp.employer_payroll_header_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoEmployerPayrollHeader" />
                <sfwLabel ID="capPayrollPaidDate" Text="Reporting Month From : " sfwIsCaption="True" />
                <sfwTextBox ID="txtPayrollPaidDateFrom" sfwDataField="payroll_paid_date" sfwDataFormat="{0:MM/YYYY}" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwDataType="DateTime" sfwRelatedControl="txtPayrollPaidDateTo" />
                <sfwLabel ID="lblPayrollPaidDateTo" Text="Reporting Month To : " sfwIsCaption="True" />
                <sfwTextBox ID="txtPayrollPaidDateTo" sfwDataField="payroll_paid_date" sfwDataFormat="{0:MM/YYYY}" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwDataType="DateTime" sfwRelatedControl="txtPayrollPaidDateFrom" />
                <sfwLabel ID="capReceivedDate" Text="Submitted Date From : " sfwIsCaption="True" />
                <sfwTextBox ID="txtReceivedDateFrom" sfwDataField="submitted_date" sfwDataFormat="{0:d}" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwDataType="DateTime" sfwRelatedControl="txtReceivedDateTo" />
                <sfwLabel ID="capSubmittedDateTo" Text="Submitted Date To : " sfwIsCaption="True" />
                <sfwTextBox ID="txtReceivedDateTo" sfwDataField="submitted_date" sfwDataFormat="{0:d}" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwDataType="DateTime" sfwRelatedControl="txtReceivedDateFrom" />
                <sfwLabel ID="lblpay_period_start_date" Text="Pay Period Start Date : " sfwIsCaption="True" />
                <sfwTextBox ID="txt_pay_period_start_date" sfwDataField="pay_period_start_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwRelatedControl="txt_pay_period_end_date" />
                <sfwLabel ID="lblpay_period_end_date" Text="Pay Period End Date : " sfwIsCaption="True" />
                <sfwTextBox ID="txt_pay_period_end_date" sfwDataField="pay_period_end_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwRelatedControl="txt_pay_period_start_date" />
                <sfwLabel ID="capPayCheckDate" Text="Pay Check Date From :" sfwIsCaption="True" />
                <sfwTextBox ID="txtPayCheckDate" sfwDataField="pay_check_date" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwDataFormat="{0:d}" sfwRelatedControl="txtPayCheckDateTo" />
                <sfwLabel ID="capPayCheckDateTo" Text="Pay Check Date To : " sfwIsCaption="True" />
                <sfwTextBox ID="txtPayCheckDateTo" sfwDataField="pay_check_date" sfwDataType="DateTime" sfwOperator="between" sfwDataFormat="{0:d}" sfwRelatedControl="txtPayCheckDate" sfwQueryID="cdoEmployerPayrollHeader" />
                <sfwLabel ID="capPostedDate" Text="Posted Date From : " Visible="False" sfwIsCaption="True" />
                <sfwTextBox ID="txtPostedDateFrom" sfwDataField="posted_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwRelatedControl="txtPostedDateTo" Visible="False" />
                <sfwLabel ID="lblLabel2" Text="Posted Date To : " Visible="False" sfwIsCaption="True" />
                <sfwTextBox ID="txtPostedDateTo" sfwDataField="posted_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoEmployerPayrollHeader" sfwRelatedControl="txtPostedDateFrom" Visible="False" />
                <sfwLabel ID="lblLabel" Text="Limit Results To :" Visible="False" sfwIsCaption="True" />
                <sfwTextBox ID="txtMaxCount" sfwTwoWayBinding="False" Visible="False" />
                <sfwLabel ID="lblLabel3" Text="Last Validated Date : " Visible="False" sfwIsCaption="True" />
                <sfwTextBox ID="txtTextBox" sfwDataField="CONVERT(VARCHAR(10),validated_date, 101)" sfwDataFormat="{0:d}" sfwOperator="=" sfwQueryID="cdoEmployerPayrollHeader" Visible="False" />
                <sfwTextBox ID="txbOrgId" sfwDataField="Temp.org_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoEmployerPayrollHeader" />
                <sfwTextBox ID="txtBalancingValue" sfwDataField="interest_waiver_flag" sfwQueryID="cdoEmployerPayrollHeader" />
                <sfwLabel ID="lblLabel5" Text="To display ALL, click ‘Search’ without entering any criteria" sfwIsCaption="True" />
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" OnClientClick="TriggerChange" sfwExecuteAfterSuccess="btnRefreshReview" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
                <sfwButton ID="btnOpenLookup" sfwMethodName="btnOpen_Click" Text="Create new Payroll Report" sfwActiveForm="wfmCreatePayrollMaintenance" />
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" CssClass="hideControl" />
              </items>
            </sfwTabSheet>
            <sfwTabSheet HeaderText="Query" ID="tshSql" Visible="False">
              <items>
                <sfwTable ID="tblSql" />
                <sfwTextBox ID="txbSql" Rows="5" TextMode="MultiLine" Width="500px" Visible="False" />
                <sfwButton ID="btnExecuteQuery" sfwMethodName="btnSearch_Click" sfwRelatedControl="dgrResult" Text="Execute Query" Visible="False" />
              </items>
            </sfwTabSheet>
          </Tabs>
        </sfwTabContainer>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlResult" sfwCaption="Search Results">
      <items>
        <sfwTable ID="tblResult" />
        <sfwButton ID="btnNew" sfwActiveForm="wfmESSEmployerPayrollHeaderMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="aintOrgID=txbOrgId;aintContactID=txbContactId;astrBenefitType=ddl_benefit_type_value" Text="New" Visible="False" />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmESSEmployerPayrollHeaderMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintemployerpayrollheaderid=EmployerPayrollHeaderId;aintIsReload=#1" sfwRelatedControl="dgrResult" Text="Open" />
        <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" CssClass="hideControl" />
        <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
        <sfwButton ID="btnSearchRefreshButton" sfwMethodName="btnSearchRefreshButton_Click" Text="Refresh" Visible="False" CssClass="hideControl" />
        <sfwButton ID="btnRefresh" OnClientClick="RefreshEmployerPayrollHeaderLookup(e); return false;" sfwMethodName="btnOpen_Click" />
        <sfwButton ID="btnRefreshReview" sfwMethodName="btnSearch_Click" Text="Search" OnClientClick="TriggerChangeReview" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
          <Columns>
            <TemplateField HeaderText="Report ID" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="EmployerPayrollHeaderId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Benefit" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="HeaderTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Report Type" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ReportTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Reporting Month" SortExpression="IstrEmptyPayPeriod" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrEmptyPayPeriod" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Pay Period Start Date" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="PayPeriodStartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Pay Period End Date" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="PayPeriodEndDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status" ItemStyle.HorizontalAlign="Left" >
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" CssClass="StatusReview" sfwVisibleRule="VisibleRuleHeaderStatusReview" />
                <sfwLabel sfwEntityField="StatusDescription" sfwVisibleRule="VisibleRuleHeaderStatusNotReview" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Balancing Status" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="BalancingStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Remaining Balance" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecRemainingBalance" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payment Status" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrPaymentStatus" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Remittance" sfwShowInExportToExcel="False" Visible="False">
              <ItemTemplate>
                <sfwLinkButton ID="btnRemittanceReport" sfwMethodName="btn_OpenPDFFromNavigationParam" sfwNavigationParameter="aintEmployerPayrollHeaderId=icdoEmployerPayrollHeader.employer_payroll_header_id" sfwObjectMethod="CreateRemittanceReport" CssClass="hideControl" />
                <sfwLinkButton ID="btnOpenRemittanceReport" sfwMethodName="btnOpenLookup_Click" Text="Remittance" sfwRelatedControl="dgrResult" OnClientClick="fnOpenPDFFromNavigationParam(e);" Font.Underline="True" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
          <sfwLabel ID="capLblText" Text="Please allow up to 48 hours for NDPERS to process payroll reports in review after comments are entered by the employer identifying the reason for the errors." sfwIsCaption="True" />
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails />
  <ExtraFields />
</form>