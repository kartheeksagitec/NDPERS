<form ID="wfmESSEmployerPayrollHeaderHomeLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2004" sfwType="FormLinkLookup" sfwTitle="Employer Payroll Header Lookup" version="3.5" sfwEntity="entEmployerPayrollHeader" sfwGridCollection="busEmployerPayrollHeaderLookup.icolEmployerPayrollHeader" sfwTemplate="ESSEmployerPayrollHeaderHomeLookup.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=htxESSEmployerPayrollHeaderHomeLookup" isBasedOnWfm="true" sfwDefaultButtonID="btnSearch">
