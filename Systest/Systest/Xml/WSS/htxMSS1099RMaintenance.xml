<form ID="wfmMSSOneZeroNineNineRMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="1099R Maintenance" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSOneZeroNineNineRMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSS1099RLookup" sfwFormObjectType="None" isBasedOnWfm="true">
