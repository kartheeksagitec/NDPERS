<form ID="wfmMSSBenefitPlanInfoMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Your Other NDPERS Plans" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSBenefitPlanInfoMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSBenefitPlanInfoLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSAllBenefitPlans" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Your Other NDPERS Plans" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage" sfwCaption="Benefit Plan Information">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvBenefitPlan" sfwEntityField="lstBenefitPlanAccounts" sfwGridEntity="entPersonAccount">
          <Columns>
            <TemplateField HeaderText="Plan Name">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="objPlan.MssPlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Provider">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrProviderOrgName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Start Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="StartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanParticipationStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Premium Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecMSSMonthlyPremiumAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Account ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonAccountId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person ID">
              <ItemTemplate>
                <sfwLabel sfwDataField="icdoPersonAccount.person_id" sfwEntityField="PersonId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Bank Name" sfwVisibleRule="VisibleRuleForBankDetailsColumn">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrBankName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Account Number" sfwVisibleRule="VisibleRuleForBankDetailsColumn">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrAccountNumber" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Account Type" sfwVisibleRule="VisibleRuleForBankDetailsColumn">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrAccountType" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Start Date" sfwVisibleRule="VisibleRuleForBankDetailsColumn">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IdtACHStartDate" sfwDataFormat="{0:d}" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLiteral ID="ltrHyperLink1" Text="Select a Plan to view Plan Details." />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmMSSBenefitPlanDetailsMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPersonAccountId=PersonAccountId" sfwRelatedControl="grvBenefitPlan" Text="OpenBenefitPlan" Visible="False" />
        <br />
        <sfwLinkButton ID="btnLinkButton" sfwActiveForm="wfmUpdatePaymentMethodWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="AintPersonID=objPerson.PersonId" sfwVisibleRule="VisibleRuleForPaymentMethodLink" />
        <sfwLabel ID="lblLabel1" sfwObjectField="ibusPerson.icdoPerson.person_id" sfwObjectID="busMSSHome" sfwEntityField="objPerson.PersonId" CssClass="hideControl" />
        <sfwLabel ID="lblLabel" Text="&lt;font style=&quot;padding-left:5px&quot;&gt;Note:  At this time we can only deduct from U.S. banks.&lt;/font&gt;" sfwVisibleRule="VisibleRuleForPaymentMethodLink" />
        <sfwLabel ID="lblIsExistsACHDetail" Text="lblIsExistsACHDetail" CssClass="hideControl" sfwEntityField="IblnIsACHDetailExixts" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage1" sfwCaption="Retirement Plan Information" sfwVisibleRule="VisibleRuleForRetirementAccount">
      <items>
        <sfwTable ID="tblNewPage1" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvMSSRetirementPersonAccount" sfwEntityField="lstMSSRetirementPersonAccount" sfwGridEntity="entPersonAccount">
          <Columns>
            <TemplateField HeaderText="Plan">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPlan.PlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Employer">
              <ItemTemplate>
                <sfwLabel sfwEntityField="EmployerName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Start Date">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrStartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanParticipationStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Account ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonAccountId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLiteral ID="ltrLiteral2" Text="Retirement Plan Details will be available soon." />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstBenefitPlanAccounts" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objPerson" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
    <entityfield ID="lstMSSRetirementPersonAccount" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>