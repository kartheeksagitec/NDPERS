<form ID="wfmMSSContactMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Contact Maintenance" version="3.5" sfwEntity="entMSSPersonContact" sfwTemplate="MSSContactMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSContactLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewMSSPersonContact" />
    <callmethods sfwMode="Update" sfwMethodName="FindMSSPersonContact" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Spouse or Designated Contact Maintenance" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnltoolba" sfwCollapsible="False">
      <items>
        <sfwTable ID="toolbar" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlContactsPersonalDetails" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblMain" />
        <sfwPanel ID="pnlContactPersonalDetails">
          <items>
            <sfwTable ID="tblNewPage1" />
            <sfwLabel ID="capIstrMSSNamePrefix" sfwMode="New" Text="Name Prefix : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlIstrMSSNamePrefix" sfwMode="New" sfwLoadType="CodeGroup" sfwLoadSource="301" sfwEntityField="IstrMSSNamePrefix" />
            <sfwLabel ID="capIstrMSSNameSuffix" sfwMode="New" Text="Name Suffix : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlIstrMSSNameSuffix" sfwMode="New" sfwLoadType="CodeGroup" sfwLoadSource="302" sfwEntityField="IstrMSSNameSuffix" />
            <sfwLabel ID="capIstrMSSLastName" sfwMode="New" Text="Last Name : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIstrMSSLastName" sfwMode="New" sfwEntityField="IstrMSSLastName" sfwValidationRules="IsMSSLastNameNull" />
            <sfwLabel ID="lblLabel5" sfwMode="New" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capIstrMSSFirstName" sfwMode="New" Text="First Name : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIstrMSSFirstName" sfwMode="New" sfwEntityField="IstrMSSFirstName" sfwValidationRules="IsMSSFirstNameNull" />
            <sfwLabel ID="lblLabel6" sfwMode="New" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capIstrMSSMiddleName" sfwMode="New" Text="Middle Name : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIstrMSSMiddleName" sfwMode="New" sfwEntityField="IstrMSSMiddleName" />
            <sfwLabel ID="capIstrMSSGender" sfwMode="New" Text="Gender : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlIstrMSSGender" sfwMode="New" sfwLoadType="Query" sfwLoadSource="entWssPersonContact.RemoveUnknownGender" sfwEntityField="IstrMSSGender" sfwValidationRules="IsMSSGenderNull" />
            <sfwLabel ID="lblLabel7" sfwMode="New" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capRelationshipDescription" Text="Relationship to Member : " sfwIsCaption="True" />
            <sfwDropDownList ID="ddlRelationshipValue" sfwClientVisibility="controllist:lblSSNMand,lblDOBMand#SPOU:lblSSNMand,lblDOBMand;" sfwLoadType="CodeGroup" sfwLoadSource="304" sfwEntityField="RelationshipValue" sfwValidationRules="RULE CHECK CONTACT TYPE IS BLANK;IsSpouseAlreadyExists" DataTextField="description" DataValueField="code_value" sfwData1EqualTo="Y" />
            <sfwLabel ID="lblLabel11" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capIstrMSSSSN" sfwMode="New" Text="SSN : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIstrMSSSSN1" sfwDataFormat="{0:000-##-####}" sfwMode="New" sfwEntityField="IstrMSSSSN" sfwValidationRules="IsSSNEnteredForSpouse" />
            <sfwLabel ID="lblSSNMand" sfwMode="New" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capIdtMSSContactDOB1" sfwMode="New" Text="Date of Birth : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIdtMSSContactDOB" sfwDataFormat="{0:d}" sfwMode="New" sfwEntityField="IdtMSSContactDOB" sfwValidationRules="IsMSSContactDOBNull" />
            <sfwLabel ID="lblDOBMand" sfwMode="New" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="capWorkPhoneNo" Text="Contact Telephone : " sfwIsCaption="True" />
            <sfwTextBox ID="txtTextBox5" sfwDataFormat="{0:(###)###-####}" sfwEntityField="ContactPhoneNo" />
            <sfwLabel ID="capEmailAddress" Text="Email : " sfwIsCaption="True" />
            <sfwTextBox ID="txtTextBox6" sfwEntityField="EmailAddress" />
            <sfwLabel ID="capStatusValue" Text="Status : " sfwIsCaption="True" Visible="False" />
            <sfwDropDownList ID="ddlStatusValue" sfwDefaultValue="ACT" sfwLoadType="CodeGroup" sfwLoadSource="308" sfwEntityField="StatusValue" CssClass="hideControl" sfwValidationRules="RULE CHECK STATUS IS BLANK" />
            <sfwLabel ID="capIstrSuppressWarning" Text="Suppress Warning : " Visible="False" sfwIsCaption="True" />
            <sfwCheckBox ID="chkIstrSuppressWarning" sfwDefaultValue="N" Visible="False" sfwEntityField="IstrSuppressWarning" />
            <sfwLabel ID="lblLabel" Visible="False" sfwEntityField="IstrOrgCodeID" />
            <sfwLabel ID="lblPersonContactId" Visible="False" sfwEntityField="PersonContactId" />
            <sfwLabel ID="lblLabel4" Visible="False" sfwEntityField="PersonId" />
            <sfwLabel ID="lblLabel1" Visible="False" sfwEntityField="ContactPersonId" />
          </items>
        </sfwPanel>
        <sfwLabel ID="lblLabel2" Text="Does this contact have the same address as the member?  " sfwIsCaption="True" />
        <sfwDropDownList ID="ddlDropDownList" sfwClientVisibility="controllist:pnlContactsAddress#N:pnlContactsAddress;" sfwEntityField="SameAsMemberAddress">
          <ListItem />
          <ListItem Text="Yes" Value="Y" />
          <ListItem Text="No" Value="N" />
        </sfwDropDownList>
        <sfwPanel ID="pnlContactsAddress" sfwCaption="Contact's Address">
          <items>
            <sfwTable ID="tblNewPage" />
            <sfwLabel ID="capAddressLine1" Text="Address Line 1 : " sfwIsCaption="True" />
            <sfwTextBox ID="txtAddrLine1" sfwEntityField="AddressLine1" sfwValidationRules="RULE CHECK ADDRESS LINE 1 IS NULL" />
            <sfwLabel ID="lbladdrline1Mand" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="lblAddrLine1" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressStreet1CAPS1" />
            <sfwLabel ID="capAddressLine2" Text="Address Line 2 : " sfwIsCaption="True" />
            <sfwTextBox ID="txtaddressLine2" sfwEntityField="AddressLine2" />
            <sfwLabel ID="lbladdressLine2" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressStreet2CAPS1" />
            <sfwLabel ID="capAddressCity" Text="City : " sfwIsCaption="True" />
            <sfwTextBox ID="txtCity" sfwEntityField="AddressCity" sfwValidationRules="RULE CHECK CITY IS NULL" />
            <sfwLabel ID="lblCityMand" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="lblCity" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressCityCAPS1" />
            <sfwLabel ID="capAddressStateValue" Text="State : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlAddressStateValue" sfwDefaultValue="ND" sfwLoadType="CodeGroup" sfwLoadSource="150" sfwEntityField="AddressStateValue" sfwValidationRules="RULE CHECK STATE IS BLANK" />
            <sfwLabel ID="lblStateMand" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="lbladdressState" sfwEntityField="objPerson.objPersonCurrentAddress.IstrStateDescriptionCAPS1" />
            <sfwLabel ID="capAddressCountryValue" Text="Address Country : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlAddressCountryValue" sfwClientVisibility="controllist:lblStateMand,lblZipCodeMand#0001:lblStateMand,lblZipCodeMand;" sfwDefaultValue="0001" sfwLoadType="CodeGroup" sfwLoadSource="151" sfwEntityField="AddressCountryValue" />
            <sfwLabel ID="lblCountry" sfwEntityField="objPerson.objPersonCurrentAddress.IstrCountryDescriptionCAPS1" />
            <sfwLabel ID="capAddressZip4Code" Text=" Zip Code : " sfwIsCaption="True" />
            <sfwTextBox ID="txtAddressZipCode" sfwEntityField="AddressZipCode" sfwCustomAttributes="maxlength=5" sfwValidationRules="RULE CHECK ZIP CODE NULL;RULE CHECK ZIP CODE NOT NUMERIC" />
            <sfwTextBox ID="txtZip4Code" sfwEntityField="AddressZip4Code" sfwCustomAttributes="maxlength=4" />
            <sfwLabel ID="lblZipCodeMand" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
            <sfwLabel ID="lblZipCode" sfwEntityField="objPerson.objPersonCurrentAddress.ZipDescription" />
          </items>
        </sfwPanel>
        <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintPersonContactID=PersonContactId" Text="Save" sfwExecuteAfterSuccess="btnOpen" />
        <sfwButton ID="btnRefreshData" sfwMethodName="btnCancel_Click" Text="Cancel" />
        <sfwButton ID="btnOpen" sfwMethodName="btnOpen_Click" Text="Open" sfwActiveForm="wfmMSSProfileMaintenance" sfwNavigationParameter="aintPersonID=PersonId" CssClass="hideControl" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPerson.objPersonCurrentAddress" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>