<form ID="wfmMSSContactNDPERSMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Contact NDPERS" version="3.5" sfwEntity="entContactTicket" sfwTemplate="MSSContactNDPERSMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSContactNDPERSLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
  <initialload>
    <callmethods sfwMethodName="FindMSSContactNDPERS" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Contact NDPERS" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewMsgPanelForContact" sfwCaption="Successfully Submitted" sfwVisibleRule="VisibilityOfPanelOnMSS">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwLiteral ID="ltrLiteral" Text="&lt;font style=&quot;font-size:12px; text-align:center; font-weight:bold;&quot;&gt;Thank you for your message. You can view the response to your request by opening the Ticket Number below. &lt;/br&gt;&lt;/br&gt;If you have provided an email address, NDPERS will notify you through email when you have received an online response through this portal. &lt;/br&gt;&lt;/br&gt;Thank you for letting us help.&lt;/font&gt;" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnllMSSSecureMessage" sfwCaption="Send us a secure message" sfwVisibleRule="VisibityOfMSSSecureMessagePanel">
      <items>
        <sfwTable ID="Section0" />
        <sfwLiteral ID="ltrLiteral1" Text="If you have questions regarding your personal account, please complete the following information and include a short message. NDPERS will contact you through a secure message on this portal or call you within 3 business days." />
        <sfwLabel ID="lblContactTicketId" sfwEntityField="ContactTicketId" />
        <sfwLabel ID="capCallbackPhone" Text="Callback Telephone Number : " sfwIsCaption="True" />
        <sfwTextBox ID="txtCallbackPhone" sfwDataFormat="{0:(###)###-####}" sfwEntityField="CallbackPhone" sfwValidationRules="Rule IF CallBack Phone In ContactNdpers Is Empty" />
        <sfwLabel ID="lblCallBackRqd" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
        <sfwLabel ID="capTimeOfDayValue" Text="Time Of Day to Contact : " sfwIsCaption="True" Visible="False" />
        <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlTimeOfDayValue" sfwLoadType="CodeGroup" sfwLoadSource="5006" sfwEntityField="TimeOfDayValue" Visible="False" />
        <sfwLabel ID="capNotes" Text="My message : " sfwIsCaption="True" />
        <sfwTextBox ID="txtNotes" Rows="4" TextMode="MultiLine" Width="800px" sfwEntityField="Notes" />
        <sfwLabel ID="lblWSSIsHistoryInserted" sfwEntityField="lblnWSSIsHistoryInserted" />
        <sfwLiteral ID="ltrLabel1" Text="&lt;br/&gt;&lt;font style=&quot;padding-left:5px&quot; &gt;If you require immediate assistance, please call our offices at 800-803-7377.&lt;/font&gt;&lt;br/&gt;&lt;br/&gt;" />
        <sfwButton ID="btnSave" sfwMethodName="btnNoChangesSave_Click" sfwNavigationParameter="aintPersonId=ContactTicketId" Text="Save" />
        <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Cancel" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHistory" sfwCaption="History">
      <items>
        <sfwTable ID="tblHistory3" />
        <sfwButton sfwMethodName="btnOpen_Click" Text="Open" ID="btnOpen" sfwRelatedControl="grvContactTicket" sfwActiveForm="wfmWSSContactTicketDetailMaintenance" sfwResource="2001" sfwCustomAttributes="ContactTicketId" sfwNavigationParameter="aintContactTicketId=ContactTicketId" Visible="False" />
        <sfwGridView AllowPaging="True" ID="grvContactTicket" sfwEntityField="lstContactTicket" sfwGridEntity="grvContactTicket" AllowSorting="True">
          <Columns>
            <TemplateField HeaderText="Ticket Number" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactTicketId" sfwLinkable="True" sfwRelatedControl="btnOpen" CssClass="alignleft fakelink" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Created Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Created By" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="CreatedBy" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Message">
              <ItemTemplate>
                <sfwLabel sfwEntityField="Notes" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstContactTicket" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>