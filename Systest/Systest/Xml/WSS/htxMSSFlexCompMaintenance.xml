<form ID="wfmMSSFlexCompMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Flex Comp Maintenance" version="3.5" sfwEntity="entMSSFlexCompWeb" sfwTemplate="MSSFlexCompMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSFlexCompLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindInsurancePlanFlexEnrolled" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage3" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
        <sfwImageButton ID="btnPrint" ImageUrl="~/Images/print.png" ImageAlign="AbsMiddle" OnClientClick="PrintCenterMiddle(e)" sfwMethodName="btnOpen_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage2" />
        <sfwLiteral ID="ltrlMessage" Text="FlexComp Plan Info" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlSpendingAccount" sfwCaption="Plan Details">
      <items>
        <sfwTable ID="tblNewPage4" />
        <sfwPanel GroupingText="&lt;b&gt;Plan Details&lt;/b&gt;" ID="pnlPlanDetails" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage9" />
            <sfwLabel ID="capStartDate" Text="Start Date :" sfwIsCaption="True" />
            <sfwLabel ID="lblIdtStartDate" sfwDataFormat="{0:d}" sfwEntityField="IdtStartDate" />
            <sfwLabel ID="capPlanParticipationStatus" Text="Plan Participation Status : " sfwIsCaption="True" />
            <sfwLabel ID="lblIstrPlanParticipation" sfwEntityField="IstrPlanParticipation" />
            <sfwLabel ID="capEndDate" Text="End Date :" sfwIsCaption="True" />
            <sfwLabel ID="lblIdtEndDate" sfwDataFormat="{0:d}" sfwEntityField="IdtEndDate" />
          </items>
        </sfwPanel>
        <sfwPanel GroupingText="&lt;b&gt;Spending Accounts Annual Pledge&lt;/b&gt;" ID="pnlSpendingAccounts" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage7" />
            <sfwLabel AssociatedControlID="lblMsraPledgeAmount" ID="capMsraPledgeAmount" Text="Medical Spending : " sfwIsCaption="True" />
            <sfwLabel ID="lblMsraPledgeAmount" sfwDataFormat="{0:C}" sfwEntityField="MsraPledgeAmount" />
            <sfwLabel AssociatedControlID="lblDcraPledgeAmount" ID="capDcraPledgeAmount" Text="Dependent Care : " sfwIsCaption="True" />
            <sfwLabel ID="lblDcraPledgeAmount" sfwDataFormat="{0:C}" sfwEntityField="DcraPledgeAmount" />
          </items>
        </sfwPanel>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlPremiumConversion" sfwCaption="Pretax Premium Election(s)">
      <items>
        <sfwTable ID="tblNewPage5" />
        <sfwPanel GroupingText="&lt;b&gt;NDPERS Plans Eligible to Pretax&lt;/b&gt;" ID="pnlNDPERSPlan" sfwCaption="NDPERS Plans" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage" />
            <sfwGridView AllowPaging="True" ID="grvPretaxPremiumElection" sfwEntityField="lstPretaxPremiumElection">
              <Columns>
                <TemplateField HeaderText="Code Serial ID" SortExpression="code_serial_id" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="CodeSerialId" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Plan" SortExpression="data1">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="Data1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Status" SortExpression="data2">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="Data2" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Pretax Premium" SortExpression="data3">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="Data3" />
                  </ItemTemplate>
                </TemplateField>
              </Columns>
            </sfwGridView>
          </items>
        </sfwPanel>
        <sfwLabel ID="lblLabel" Text="&lt;br/&gt;" sfwIsCaption="True" />
        <sfwPanel GroupingText="&lt;b&gt;Non-NDPERS Plans Eligible to Pretax&lt;/b&gt;" ID="pnlNonNDpersPlan" sfwCaption="Non-NDPERS Plans" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage6" />
            <sfwGridView AllowPaging="True" ID="grvGridView" PageSize="25" sfwEntityField="lstPersonAccountFlexCompConversion" sfwGridEntity="entPersonAccountFlexcompConversion">
              <Columns>
                <TemplateField HeaderText="Company Name">
                  <ItemTemplate>
                    <sfwLabel sfwRelatedControl="btnOpenFlexcompConversion" sfwEntityField="objProvider.OrgName" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Pretax Premium">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IsPretaxPremium" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Org Code ID" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="objProvider.OrgCode" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Effective Start Date" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveStartDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Effective End Date" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveEndDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Person Account Flex Comp Conversion ID" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="PersonAccountFlexCompConversionId" />
                  </ItemTemplate>
                </TemplateField>
              </Columns>
            </sfwGridView>
          </items>
        </sfwPanel>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHistory" sfwCaption="History">
      <items>
        <sfwTable ID="tblNewPage8" />
        <sfwGridView AllowPaging="True" ID="grvPreviousHistory" sfwEntityField="lstPreviousHistory" sfwGridEntity="entPersonAccountFlexCompHistory">
          <Columns>
            <TemplateField HeaderText="Plan Participation Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanParticipationStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective Start Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveStartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective End Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveEndDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Account Flex Comp History ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonAccountFlexCompHistoryId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Annual Pledge Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="AnnualPledgeAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Level Of Coverage">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LevelOfCoverageDescription" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage1" sfwCaption="Update Enrollment" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage1" />
        <sfwButton ID="btnOpen1" sfwActiveForm="wfmFlexEnrollmentWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintRequestId=IintEnrollmentRequestID" sfwVisibleRule="IblnIsEnrollmentRequestRejectedOrPending" Text="Update your enrollment" />
        <sfwButton ID="btnNew" sfwActiveForm="wfmFlexEnrollmentWizard" sfwMethodName="btnNew_Click" sfwNavigationParameter="aintPlanId=lblPlanId;aintPersonId=lblPersonId;aintPersonEmploymentDetailId=lblPersonEmploymentDtlId" sfwVisibleRule="IblnNoEnrollmentRequestExits" Text="Update your enrollment" />
        <sfwLabel ID="lblIintEnrollmentRequestID" Visible="False" sfwEntityField="IintEnrollmentRequestID" />
        <sfwLabel ID="lblPersonEmploymentDtlId" sfwEntityField="PersonEmploymentDtlId" CssClass="hideControl" />
        <sfwLabel ID="lblPersonId" sfwEntityField="PersonId" CssClass="hideControl" />
        <sfwLabel ID="lblPlanId" sfwEntityField="PlanId" CssClass="hideControl" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstPretaxPremiumElection" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
    <entityfield ID="lstPersonAccountFlexCompConversion" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstPreviousHistory" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>