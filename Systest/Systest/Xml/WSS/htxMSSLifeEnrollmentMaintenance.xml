            <sfwDropDownList ID="ddlDependentCoverageOptionValue" sfwLoadType="CodeGroup" sfwLoadSource="1027" sfwEntityField="objMSSLifeOption.DependentCoverageOptionValue" />
            <sfwLabel ID="lblLabel19" Text="Dependent Waiver :" sfwIsCaption="True" />
            <sfwCheckBox ID="chkCheckBox" sfwEntityField="objMSSLifeOption.DependentWaiverFlag" />
          </items>
        </sfwPanel>
        <hr />
        <sfwLabel ID="lblLabel6" Text="Supplemental Spouse Life Election" sfwIsCaption="True" />
        <sfwLabel ID="lblLabel7" Text="Only available if you elected dependent coverage of $2,000 or $5,000 in Part D.  When you are initially eligible for dependent spouse coverage, you can elect up to $50,000 in coverage without providing Life Insurance Evidence of Insurability.  Total spouse coverage up to $100,000 is available if your spouse completes a Life Insurance Evidence of Insurability form for approval by ING Employee Benefits.  Supplemental spouse coverage is limited to 50% of the employee’s coverage amount." sfwIsCaption="True" />
        <sfwPanel ID="pnlNewPage1" sfwCaption="New Page" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage4" />
