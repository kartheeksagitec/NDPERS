            <TemplateField HeaderText="Acknowledgement ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="AcknowledgementId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Acknowledgement Text">
              <ItemTemplate>
                <sfwLabel sfwEntityField="AcknowledgementText" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLabel ID="lblIstrConfirmationText" sfwEntityField="IstrConfirmationText" />
        <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintRequestId=WssPersonAccountEnrollmentRequestId" Text="Save" />
        <sfwLabel ID="lblLabel9" Visible="False" sfwEntityField="WssPersonAccountEnrollmentRequestId" />
      </items>
