  </items>
  <loaddetails>
    <entityfield ID="objMSSLifeOption" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstAcknowledgementCheck" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
  </loaddetails>
