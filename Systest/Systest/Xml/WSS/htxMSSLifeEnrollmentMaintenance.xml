  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlNewPage2" sfwCaption="New Page" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage5" />
        <sfwLiteral ID="Life" Text="Life Insurance Plan" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlEligible" sfwCaption="Plan Enrollment Details">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwLabel ID="capRejectionReason" sfwVisibleRule="IsStatusRejected" Text="Rejection Reason : " sfwIsCaption="True" />
        <sfwLabel ID="lblRejectionReason" sfwVisibleRule="IsStatusRejected" sfwEntityField="RejectionReason" />
        <hr />
        <sfwPanel ID="pnlNewPage" sfwCaption="New Page" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage1" />
            <sfwLabel ID="capReasonValue" Text="Change Reason : " sfwIsCaption="True" />
            <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlDropDownList1" sfwData1EqualTo="N" sfwData2EqualTo="Y" sfwLoadType="CodeGroup" sfwLoadSource="332" sfwEntityField="ReasonValue" />
            <sfwLabel ID="capDateOfChange" Text="Change Date : " sfwIsCaption="True" />
            <sfwTextBox ID="txtLabel17" sfwDataFormat="{0:d}" sfwEntityField="DateOfChange" />
            <sfwLinkButton ID="btnLinkButton1" sfwMethodName="btnDownloadAttachment_Click" sfwTriggerPostBack="True" />
            <sfwLinkButton ID="btnPdfCorrespondenceClick2" sfwMethodName="btnPdfCorrespondence_Click" sfwNavigationParameter="sfwLaunchNewWindow=&quot;True&quot;" />
          </items>
        </sfwPanel>
        <hr />
        <sfwLabel ID="lblLabel" Text="Basic Life " sfwIsCaption="True" />
        <sfwLabel ID="lblLabel17" Text="Employee Only—Employer Provides $3,500 of Basic Life Coverage at no expense to you." sfwIsCaption="True" />
        <sfwTextBox ID="txtTextBox" sfwDataFormat="{0:C}" sfwDefaultValue="1300" Visible="False" sfwEntityField="objMSSLifeOption.CoverageAmount" />
        <hr />
        <sfwLabel ID="lblLabel1" Text="Supplemental Life and AD&amp;D Election" sfwIsCaption="True" />
        <sfwLabel ID="lblLabel3" Text="When you are first eligible for supplemental life coverage, you can elect up to the Guaranteed Issue (GI) Limit of $200,000 without providing Life Insurance Evidence of Insurability. After first eligibility, a Life Insurance Evidence of Insurability form (EOI) must be completed." sfwIsCaption="True" />
        <sfwPanel ID="pnlSupplemental" sfwCaption="New Page" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage2" />
            <sfwLabel ID="capIdecMSSSupplementalAmount" Text="Supplemental Amount : " sfwIsCaption="True" />
            <sfwTextBox ID="txtIdecMSSSupplementalAmount" sfwDataFormat="{0:C}" sfwEntityField="objMSSLifeOption.SupplementalAmount" />
            <sfwLinkButton ID="btnPdfCorrespondenceClick1" sfwNavigationParameter="pdf_file_name=#PDFCorrespondence\Templates\SFN-58859.pdf" sfwMethodName="btnPdfCorrespondence_Click" sfwVisibleRule="IsSFN58859Visible" />
            <sfwLabel ID="capIstrMSSSupplementalIsWaived" Text="Supplemental Waiver : " sfwIsCaption="True" />
            <sfwCheckBox ID="chkIstrMSSSupplementalIsWaived" sfwEntityField="objMSSLifeOption.SupplementalWaiverFlag" />
          </items>
        </sfwPanel>
        <hr />
        <sfwLabel ID="lblLabel4" Text="Supplemental Dependent Life Insurance Election" sfwIsCaption="True" />
        <sfwLabel ID="lblLabel5" Text="When you are initially eligible for dependent coverage, you can elect it without providing Life Insurance Evidence of Insurability.  After initial eligibility, a Life Insurance Evidence of Insurability form (EOI) must be completed for approval by ING Employee Benefits " sfwIsCaption="True" />
        <sfwPanel ID="pnlDependent" sfwCaption="New Page" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage3" />
