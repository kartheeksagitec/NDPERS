            <sfwLinkButton ID="btnLinkButton" sfwNavigationParameter="pdf_file_name=#PDFCorrespondence\Templates\SFN-58859.pdf" sfwMethodName="btnPdfCorrespondence_Click" sfwVisibleRule="IsSFN58859Visible" />
            <sfwLabel ID="capIstrMSSSpouseSupplementalIsWaived" Text="Waive Supplemental Spouse Coverage : " sfwIsCaption="True" />
            <sfwCheckBox ID="chkIstrMSSSpouseSupplementalIsWaived" sfwEntityField="objMSSLifeOption.SpouseSupplementalWaiverFlag" />
