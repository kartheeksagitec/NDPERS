<form ID="wfmMSSLifeEnrollmentMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Life Enrollment Maintenance" version="3.5" sfwEntity="entWssPersonAccountEnrollmentRequest" sfwTemplate="MSSLifeEnrollmentMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSLifeEnrollmentLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
