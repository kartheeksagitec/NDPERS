<form ID="wfmMSSMemberDetailMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Member Detail Maintenance" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSMemberDetailMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSMemberDetailLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
  <initialload>
    <callmethods sfwMethodName="FindMSSMemberDetail" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCaption="Personal Information" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblPI" />
        <sfwLiteral Text="Personal Information" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHeadng" sfwCaption="Heading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Member Detail">
      <items>
        <sfwTable ID="tblMain" />
        <sfwLabel ID="capPersonId" Text="Member ID : " sfwIsCaption="True" />
        <sfwLabel ID="lblPersonId" sfwEntityField="objPerson.PersonId" />
        <sfwLabel ID="capDateOfBirth" Text="Date Of Birth : " sfwIsCaption="True" />
        <sfwTextBox ID="txtDateOfBirth" sfwDataFormat="{0:d}" sfwVisibleRule="IsDateOfBirthEditable" sfwEntityField="objPerson.DateOfBirth" />
        <sfwLabel ID="lblTextBox" sfwDataFormat="{0:d}" sfwVisibleRule="IsDateOfBirthNotEditable" sfwEntityField="objPerson.DateOfBirth" />
        <sfwLabel ID="capGenderDescription" Text="Gender : " sfwIsCaption="True" />
        <sfwLabel ID="lblGenderDescription" sfwEntityField="objPerson.GenderDescription" />
        <sfwLabel ID="capFullName" Text="Name Prefix: " sfwIsCaption="True" />
        <sfwDropDownList ID="ddlFullName" sfwLoadType="CodeGroup" sfwLoadSource="301" sfwEntityField="objPerson.NamePrefixValue" />
        <sfwLabel ID="capNameSuffixValue" Text="Name Suffix : " sfwIsCaption="True" />
        <sfwDropDownList ID="ddlNameSuffixValue" sfwLoadType="CodeGroup" sfwLoadSource="302" sfwEntityField="objPerson.NameSuffixValue" />
        <sfwLabel ID="capFirstName" Text="First Name : " sfwIsCaption="True" />
        <sfwTextBox ID="txtFirstName" sfwEntityField="objPerson.FirstName" />
        <sfwLabel ID="capMiddleName" Text="Middle Name : " sfwIsCaption="True" />
        <sfwTextBox ID="txtMiddleName" sfwEntityField="objPerson.MiddleName" />
        <sfwLabel ID="capLastName" Text="Last Name : " sfwIsCaption="True" />
        <sfwTextBox ID="txtLastName" sfwEntityField="objPerson.LastName" />
        <sfwLabel ID="capMaritalStatusDescription" Text="Marital Status : " sfwIsCaption="True" />
        <sfwLabel ID="lblMaritalStatusDescription" sfwEntityField="objPerson.MaritalStatusDescription" />
        <sfwLinkButton ID="btnLinkButton22" sfwActiveForm="wfmMaritalChangeWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPersonID=objPerson.PersonId" />
        <sfwButton ID="btnButton1" sfwNavigationParameter="pdf_file_name=#PDFCorrespondence\Templates\SFN 53405.pdf" sfwMethodName="btnPdfCorrespondence_Click" sfwVisibleRule="IsSFN53405Visible" Text="SFN 53405 - Benefit Plan - DB - Elected Official" />
        <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintPersonID=objPerson.PersonId" Text="Save" />
        <sfwButton ID="btnRefreshData" sfwMethodName="btnCancel_Click" Text="Cancel" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPerson" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>