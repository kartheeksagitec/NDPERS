<form ID="wfmMSSViewBenefitEstimateMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="MSSView Benefit Estimate Maintenance" version="3.5" WebSite="NeoSpinWSS" sfwEntity="entMSSHome" sfwTemplate="MSSViewBenefitEstimateMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSViewBenefitEstimateLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSAllBenefitEstimate" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar">
      <items>
        <sfwTable ID="toolbar" />
        <sfwLiteral ID="ltrlMessage" Text="Your Benefit Estimates&lt;br/&gt;&lt;br/&gt;" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlBack" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Benefit Estimates">
      <items>
        <sfwTable ID="tblMain" />
        <sfwButton ID="btnOpenMSSBenefitEstimates" sfwActiveForm="wfmBenefitCalculatorSummaryMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintBenefitCalculatorId=WssBenefitCalculatorId;aintPersonId=objRetirementBenefitCalculation.PersonId" sfwRelatedControl="grvMSSBenefitEstimates" Text="Open" Visible="False" />
        <sfwGridView AllowPaging="True" ID="grvMSSBenefitEstimates" sfwEntityField="lstMSSBenefitEstimates" sfwGridEntity="entWssBenefitCalculator">
          <Columns>
            <TemplateField HeaderText="Benefit Calculation ID" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenMSSBenefitEstimates" sfwEntityField="objRetirementBenefitCalculation.BenefitCalculationId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Benefit Calculation ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenMSSBenefitEstimates" sfwEntityField="WssBenefitCalculatorId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Plan Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPlan.PlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Benefit Account Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objRetirementBenefitCalculation.BenefitAccountTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objRetirementBenefitCalculation.StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Action Status" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objRetirementBenefitCalculation.ActionStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Retirement Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objRetirementBenefitCalculation.RetirementDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Final Monthly Benefit" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="objRetirementBenefitCalculation.FinalMonthlyBenefit" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objRetirementBenefitCalculation.PersonId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLabel ID="lblLabel111" Text="&lt;br/&gt;Calculations performed within past 30 days are listed here." sfwIsCaption="True" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstMSSBenefitEstimates" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>