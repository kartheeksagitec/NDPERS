<form ID="wfmServicePurchaseMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Service Purchase Maintenance" version="3.5" WebSite="NeoSpinWSS" sfwEntity="entServicePurchaseWeb" sfwTemplate="ServicePurchaseMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmServicePurchaseLookup" sfwFormObjectType="None" isBasedOnWfm="true">
