<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmDuesRateMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="1920 " sfwType="Maintenance" sfwTitle="Dues Rate Maintenance" version="3.5" sfwEntity="entDuesRateChangeRequest">
