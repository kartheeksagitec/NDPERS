<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmRHICCombiningLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvBenefitCalculation" sfwResource="1309" sfwType="Lookup" sfwTitle="RHIC Maintenance Lookup" version="3.5" sfwEntity="entBenefitRhicCombine" sfwGridCollection="busRHICCombiningLookup.iclbRHICCombining">
  <initialload>
    <query ID="Lookup" sfwQueryRef="entBenefitRhicCombine.Lookup" sfwMethodName="LoadRHICCombining" />
    <query ID="LookupSubQueryCombineDetail" sfwQueryRef="entBenefitRhicCombine.LookupSubQueryRhicCombineDetail" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPersonId" Text="Receiver's PERSLink ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId" sfwDataField="sbrc.person_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="Lookup" />
                <sfwImageButton ID="btnRetrievePerson" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtPersonId" sfwSelectedIndex="PersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capPersonId2" Text="Donor's PERSLink ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId2" sfwDataField="payee_perslink_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="LookupSubQueryCombineDetail" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtPersonId2" sfwSelectedIndex="PersonId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capLastName" Text="Receiver's Last Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtLastName" sfwDataField="per.last_name" sfwOperator="like" sfwQueryID="Lookup" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capFirstName" Text="Receiver's First Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtFirstName" sfwDataField="per.first_name" sfwOperator="like" sfwQueryID="Lookup" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capRequestDateFrom" Text="Request Date From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtRequestDateFrom" sfwDataField="CONVERT(VARCHAR(10), sbrc.request_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="Lookup" sfwRelatedControl="txtRequestDateTo" />
                <CompareValidator ControlToValidate="txtRequestDateFrom" ErrorMessage="Request Date From must be in mm/dd/yyyy format." ID="valRequestDateFrom" Operator="DataTypeCheck" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capRequestDateTo" Text="Request Date To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtRequestDateTo" sfwDataField="CONVERT(VARCHAR(10), sbrc.request_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="Lookup" sfwRelatedControl="txtRequestDateFrom" />
                <CompareValidator ControlToValidate="txtRequestDateTo" ErrorMessage="Request Date To must be in mm/dd/yyyy format." ID="valRequestDateTo" Operator="DataTypeCheck" Text="*" Type="Date" />
                <CompareValidator ControlToCompare="txtRequestDateFrom" ControlToValidate="txtRequestDateTo" ErrorMessage="Request Date To cannot be less than Request Date From." ID="valRequestDateRange" Operator="GreaterThanEqual" Text="*" Type="Date" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capActionStatusValue" Text="Action Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlActionStatusValue" sfwDataField="sbrc.action_status_value" sfwOperator="like" sfwQueryID="Lookup" sfwLoadType="CodeGroup" sfwLoadSource="2306" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatusValue" Text="Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlStatusValue" sfwDataField="status_value" sfwOperator="like" sfwQueryID="Lookup" sfwLoadType="CodeGroup" sfwLoadSource="2305" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capBenefitRhicCombineId" Text="RHIC Maintenance  ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtBenefitRhicCombineId" sfwDataField="sbrc.benefit_rhic_combine_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="Lookup" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmRHICCombiningMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnNew_Click" sfwNavigationParameter="aintPersonID=txtPersonId" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmRHICCombiningMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintBenefitRHICcombineID=BenefitRhicCombineId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" Visible="False" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many" sfwSortExpression="BenefitRhicCombineId desc" SortExpressionUpdated="true">
                  <Columns>
                    <TemplateField HeaderText="RHIC ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="BenefitRhicCombineId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Receiver's PERSLink ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Receiver's Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPerson.FullName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Request Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="RequestDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Effective Start Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="StartDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Effective End Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EndDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Apply to Health Enrollment">
                      <ItemTemplate>
                        <sfwCheckBox sfwTwoWayBinding="False" sfwEntityField="ApplyToValue" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Action Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ActionStatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Approved By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ApprovedBy" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>