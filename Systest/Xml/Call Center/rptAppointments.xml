<?xml version="1.0" encoding="utf-8"?>
<form ID="rptAppointments" sfwActive="True" sfwDescription="Appointment Report" sfwStatus="Review" sfwType="Report" sfwReportName="rptAppointments.rpt" sfwReportCategory="701" version="4.0" sfwResource="701">
  <initialload>
    <query ID="cdoAppointmentSchedule" sfwQueryRef="entAppointmentSchedule.rptAppointments" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwLabel Text="Start Date (mm/dd/yyyy) :" sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="start_date" Width="100px" sfwDataFormat="{0:d}" />
        <RequiredFieldValidator ControlToValidate="start_date" ErrorMessage="Start Date is Required" Text="*" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel Text="End Date (mm/dd/yyyy) :" sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="end_date" Width="100px" sfwDataFormat="{0:d}" />
        <RequiredFieldValidator ControlToValidate="end_date" ErrorMessage="End Date is Required" Text="*" />
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwLabel ID="Counselor" Text="Counselor : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList ID="ddlCounselor" DataTextField="user_id" DataValueField="user_serial_id" sfwLoadType="Query" sfwLoadSource="entUser.LOADACTIVEUSER" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel ID="AppointmentType" Text="Appointment Type : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList ID="ddlAppointmentType" sfwLoadType="CodeGroup" sfwLoadSource="1007" />
      </sfwColumn>
    </sfwRow>
  </sfwTable>
</form>