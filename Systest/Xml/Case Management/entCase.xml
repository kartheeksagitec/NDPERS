    <query ID="IncomeVerificationBatch" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT * FROM SGT_CASE CS WITH(NOLOCK)  &#xD;&#xA;INNER JOIN SGT_PAYEE_ACCOUNT SPA WITH(NOLOCK) ON SPA.PAYEE_ACCOUNT_ID=CS.PAYEE_ACCOUNT_ID&#xD;&#xA;WHERE &#xD;&#xA;MONTH(CS.INCOME_VERIFICATION_DATE) = MONTH(DATEADD(m,1,@CURR_DATE)) AND &#xD;&#xA;YEAR(CS.INCOME_VERIFICATION_DATE) = YEAR(DATEADD(m,1,@CURR_DATE)) AND &#xD;&#xA;CS.INCOME_VERIFICATION_DATE  &lt;&gt; CS.RECERTIFICATION_DATE AND&#xD;&#xA;CS.INCOME_VERIFICATION_FLAG IS NULL AND &#xD;&#xA;CS.CASE_TYPE_VALUE = 'DISR'">
      <parameters>
        <parameter ID="@CURR_DATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="DisabilityRecertificationBatch" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT PAY.*&#xD;&#xA;FROM &#xD;&#xA;(SELECT PAY.PAYEE_ACCOUNT_ID,BENEFIT_BEGIN_DATE,&#xD;&#xA;(SELECT TOP 1 STATUS_VALUE FROM SGT_PAYEE_ACCOUNT_STATUS PAYSTAT WITH(NOLOCK)&#xD;&#xA;WHERE PAYSTAT.PAYEE_ACCOUNT_ID=PAY.PAYEE_ACCOUNT_ID &#xD;&#xA;ORDER BY STATUS_EFFECTIVE_DATE DESC, PAYEE_ACCOUNT_STATUS_ID DESC) AS STATUS_VALUE FROM SGT_PAYEE_ACCOUNT PAY  &#xD;&#xA;WHERE BENEFIT_ACCOUNT_TYPE_VALUE='DISA' AND RECERTIFICATION_DATE IS NOT NULL AND ISNULL(IS_DISABILITY_BATCH_LETTER_SENT_FLAG,'N') &lt;&gt; 'Y' &#xD;&#xA;AND DATEDIFF ( MONTH ,  @CURR_DATE, RECERTIFICATION_DATE) = 4) AS INNERTAB&#xD;&#xA;INNER JOIN SGT_PAYEE_ACCOUNT PAY WITH(NOLOCK) ON PAY.PAYEE_ACCOUNT_ID=INNERTAB.PAYEE_ACCOUNT_ID&#xD;&#xA;AND INNERTAB.STATUS_VALUE='RCDS'">
      <parameters>
        <parameter ID="@CURR_DATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="DisabilityRecertificationBatchGetIdentifier" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT 1 as RowIdentifier, INNERTAB.*,CAS.*&#xD;&#xA;FROM&#xD;&#xA;(SELECT PAY.PAYEE_ACCOUNT_ID,BENEFIT_BEGIN_DATE,&#xD;&#xA;(SELECT TOP 1 STATUS_VALUE FROM SGT_PAYEE_ACCOUNT_STATUS PAYSTAT WITH(NOLOCK) &#xD;&#xA;WHERE PAYSTAT.PAYEE_ACCOUNT_ID=PAY.PAYEE_ACCOUNT_ID ORDER BY STATUS_EFFECTIVE_DATE DESC) AS STATUS_VALUE&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT PAY WHERE BENEFIT_ACCOUNT_TYPE_VALUE='DISA' &#xD;&#xA;) AS INNERTAB&#xD;&#xA;INNER JOIN SGT_CASE CAS WITH(NOLOCK) ON CAS.PAYEE_ACCOUNT_ID=INNERTAB.PAYEE_ACCOUNT_ID &#xD;&#xA;INNER JOIN (SELECT CASE_ID FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK) WHERE CASE_STEP_DETAIL_ID&#xD;&#xA;IN (SELECT MIN(CASE_STEP_DETAIL_ID) FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK)&#xD;&#xA;GROUP BY CASE_ID) AND END_DATE IS NULL) AS TEMP ON TEMP.CASE_ID=CAS.CASE_ID&#xD;&#xA;AND CAS.CASE_TYPE_VALUE IN ('DISR','PRED')&#xD;&#xA;AND INNERTAB.STATUS_VALUE='RCDS'&#xD;&#xA;AND MONTH(DATEADD(M,-2,CAS.RECERTIFICATION_DATE))=MONTH(@CURR_DATE)&#xD;&#xA;AND YEAR(DATEADD(M,-2,CAS.RECERTIFICATION_DATE))=YEAR(@CURR_DATE)&#xD;&#xA;and ISNULL(THIRD_NOTIFICATION_FLAG,'N')&lt;&gt;'Y'&#xD;&#xA;and ISNULL(CAS.SECOND_NOTIFICATION_FLAG,'N') = 'Y'&#xD;&#xA;&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;SELECT 2 as RowIdentifier, INNERTAB.*,CAS.*&#xD;&#xA;FROM&#xD;&#xA;(SELECT PAY.PAYEE_ACCOUNT_ID,BENEFIT_BEGIN_DATE,(SELECT TOP 1 STATUS_VALUE FROM SGT_PAYEE_ACCOUNT_STATUS PAYSTAT WITH(NOLOCK)&#xD;&#xA;WHERE PAYSTAT.PAYEE_ACCOUNT_ID=PAY.PAYEE_ACCOUNT_ID ORDER BY STATUS_EFFECTIVE_DATE DESC) AS STATUS_VALUE&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT PAY WHERE BENEFIT_ACCOUNT_TYPE_VALUE='DISA' &#xD;&#xA;) AS INNERTAB&#xD;&#xA;INNER JOIN SGT_CASE CAS WITH(NOLOCK) ON CAS.PAYEE_ACCOUNT_ID=INNERTAB.PAYEE_ACCOUNT_ID &#xD;&#xA;INNER JOIN (SELECT CASE_ID FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK) WHERE CASE_STEP_DETAIL_ID&#xD;&#xA;IN (SELECT MIN(CASE_STEP_DETAIL_ID) FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK)&#xD;&#xA;GROUP BY CASE_ID) AND END_DATE IS NULL) AS TEMP ON TEMP.CASE_ID=CAS.CASE_ID&#xD;&#xA;AND CAS.CASE_TYPE_VALUE IN ('DISR','PRED')&#xD;&#xA;AND INNERTAB.STATUS_VALUE='RCDS'&#xD;&#xA;AND MONTH(DATEADD(M,-3,CAS.RECERTIFICATION_DATE))=MONTH(@CURR_DATE)&#xD;&#xA;AND YEAR(DATEADD(M,-3,CAS.RECERTIFICATION_DATE))=YEAR(@CURR_DATE)&#xD;&#xA;and ISNULL(SECOND_NOTIFICATION_FLAG,'N')&lt;&gt;'Y'&#xD;&#xA;and ISNULL(CAS.FIRST_NOTIFICATION_FLAG,'N') = 'Y'&#xD;&#xA;&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;SELECT 3 as RowIdentifier, INNERTAB.*,CAS.*&#xD;&#xA;FROM&#xD;&#xA;(SELECT PAY.PAYEE_ACCOUNT_ID,BENEFIT_BEGIN_DATE,(SELECT TOP 1 STATUS_VALUE FROM SGT_PAYEE_ACCOUNT_STATUS PAYSTAT WITH(NOLOCK)&#xD;&#xA;WHERE PAYSTAT.PAYEE_ACCOUNT_ID=PAY.PAYEE_ACCOUNT_ID ORDER BY STATUS_EFFECTIVE_DATE DESC) AS STATUS_VALUE&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT PAY WHERE BENEFIT_ACCOUNT_TYPE_VALUE='DISA' &#xD;&#xA;) AS INNERTAB&#xD;&#xA;INNER JOIN SGT_CASE CAS WITH(NOLOCK) ON CAS.PAYEE_ACCOUNT_ID=INNERTAB.PAYEE_ACCOUNT_ID &#xD;&#xA;INNER JOIN (SELECT CASE_ID FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK) WHERE CASE_STEP_DETAIL_ID&#xD;&#xA;IN (SELECT MIN(CASE_STEP_DETAIL_ID) FROM SGT_CASE_STEP_DETAIL WITH(NOLOCK)&#xD;&#xA;GROUP BY CASE_ID) AND END_DATE IS NULL) AS TEMP ON TEMP.CASE_ID=CAS.CASE_ID&#xD;&#xA;AND CAS.CASE_TYPE_VALUE IN ('DISR','PRED')&#xD;&#xA;AND INNERTAB.STATUS_VALUE='RCDS'&#xD;&#xA;AND MONTH(DATEADD(M,-4,CAS.RECERTIFICATION_DATE))=MONTH(@CURR_DATE)&#xD;&#xA;AND YEAR(DATEADD(M,-4,CAS.RECERTIFICATION_DATE))=YEAR(@CURR_DATE)&#xD;&#xA;and ISNULL(FIRST_NOTIFICATION_FLAG,'N')&lt;&gt;'Y'">
      <parameters>
        <parameter ID="@CURR_DATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="GetRetDisAppsForCorres" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT * FROM SGT_BENEFIT_APPLICATION WHERE MEMBER_PERSON_ID = @PersonID&#xD;&#xA;AND BENEFIT_ACCOUNT_TYPE_VALUE IN ('RETR', 'DISA') AND ACTION_STATUS_VALUE &lt;&gt; 'CANC'&#xD;&#xA;ORDER BY BENEFIT_APPLICATION_ID DESC">
      <parameters>
