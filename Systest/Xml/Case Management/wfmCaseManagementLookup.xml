<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmCaseManagementLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvCase" sfwResource="1909" sfwType="Lookup" sfwTitle="Case Management Lookup" sfwMaxCount="300" version="3.5" sfwEntity="entCase" sfwGridCollection="busCaseLookup.iclbCase">
  <initialload>
    <query ID="cdoCase" sfwQueryRef="entCase.Lookup" sfwMethodName="LoadCases" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPersonId" Text="PERSLink ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId" sfwDataField="cas.person_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoCase" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtPersonId" sfwSelectedIndex="PersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCaseId" Text="Case ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCaseId" sfwDataField="case_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoCase" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCaseTypeValue" Text="Case Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlCaseTypeValue" sfwDataField="case_type_value" sfwOperator="like" sfwQueryID="cdoCase" sfwLoadType="CodeGroup" sfwLoadSource="2254" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capAppealTypeValue" Text="Appeal Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlAppealTypeValue" sfwDataField="appeal_type_value" sfwOperator="like" sfwQueryID="cdoCase" sfwLoadType="CodeGroup" sfwLoadSource="2256" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCaseStatusValue" Text="Case Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlCaseStatusValue" sfwDataField="case_status_value" sfwOperator="like" sfwQueryID="cdoCase" sfwLoadType="CodeGroup" sfwLoadSource="2257" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capActionBy" Text="Performed By : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="full_name" DataValueField="user_serial_id" ID="ddlActionBy" sfwDataField="action_by" sfwOperator="like" sfwQueryID="cdoCase" sfwLoadType="Query" sfwLoadSource="entUser.ListUsersOrderByFirstName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateFrom" Text="Case Started From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateFrom" sfwDataField="CONVERT(VARCHAR(10), cas.created_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoCase" sfwRelatedControl="txtCreatedDateTo" />
                <CompareValidator ControlToValidate="txtCreatedDateFrom" ErrorMessage="Created Date From must be in mm/dd/yyyy format." ID="valCreatedDateFrom" Operator="DataTypeCheck" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateTo" Text="Case Started To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateTo" sfwDataField="CONVERT(VARCHAR(10), cas.created_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoCase" sfwRelatedControl="txtCreatedDateFrom" />
                <CompareValidator ControlToValidate="txtCreatedDateTo" ErrorMessage="Created Date To must be in mm/dd/yyyy format." ID="valCreatedDateTo" Operator="DataTypeCheck" Text="*" Type="Date" />
                <CompareValidator ControlToCompare="txtCreatedDateFrom" ControlToValidate="txtCreatedDateTo" ErrorMessage="Created Date To cannot be less than Created Date From." ID="valCreatedDateRange" Operator="GreaterThanEqual" Text="*" Type="Date" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmCaseManagementMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnNew_Click" sfwNavigationParameter="AintPersonId=txtPersonId;astrCaseType=ddlCaseTypeValue;astrAppealType=ddlAppealTypeValue" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmCaseManagementMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintcaseid=CaseId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many" sfwSortExpression="CaseId desc" SortExpressionUpdated="true">
                  <Columns>
                    <TemplateField HeaderText="Person ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PersonId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="First Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPerson.FirstName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Last Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPerson.LastName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Case ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CaseId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Case Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CaseTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Appeal Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="AppealTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Case Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CaseStatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Case Start Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Overall Days">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OverallTime" sfwShowZero="True" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Performed By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objUser.UserFullName" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>