<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmAuditMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvAudit" sfwResource="506 " sfwType="Maintenance" sfwTitle="Audit history" version="3.5">
  <initialload>
    <custommethod sfwMethodName="GetAuditDetails" sfwReturnType="Sagitec.BusinessObjects.busAuditLookup">
      <parameter ID="aintPrimaryKey" sfwField="primary_key" />
      <parameter ID="astrFormName" sfwField="form_name" />
    </custommethod>
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Summary audit information">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrHdr" PageSize="5" sfwObjectID="busAuditLookup.iclbLookupResult">
                  <Columns>
                    <TemplateField HeaderText="Transaction ID" SortExpression="icdoAuditLog.audit_log_id">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.audit_log_id" sfwLinkable="True" sfwRelatedControl="dgrDtl" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Form Name" SortExpression="icdoAuditLog.form_name">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.form_name" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Table Name" SortExpression="icdoAuditLog.table_name">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.table_name" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Primary Key" SortExpression="icdoAuditLog.primary_key">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.primary_key" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Person ID" SortExpression="icdoAuditLog.person_id">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.person_id" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified By" SortExpression="icdoAuditLog.modified_by">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.modified_by" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified Date" SortExpression="icdoAuditLog.modified_date">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.modified_date" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Change" SortExpression="icdoAuditLog.change_description">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="icdoAuditLog.change_description" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Detail audit information">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrDtl" sfwDefaultBinding="False" sfwObjectID="iclcAuditLogDetail">
                  <Columns>
                    <TemplateField HeaderText="Transaction ID" SortExpression="audit_log_id">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="audit_log_id" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Column Name" SortExpression="column_name">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="column_name" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Old Value" SortExpression="old_value">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="old_value" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="New Value" SortExpression="new_value">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="new_value" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
</form>