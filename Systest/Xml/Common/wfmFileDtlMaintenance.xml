<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmFileDtlMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="410 " sfwType="Maintenance" sfwTitle="File Detail Maintenance" sfwHelpFile="Batch Overview=Batch Overview.htm;Lookup Processed Records=Lookup Processed Records.htm" version="3.5" sfwEntity="entFileDtl">
