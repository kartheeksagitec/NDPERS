    <query ID="Lookup" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="select * from sgt_deposit d WITH(NOLOCK)&#xD;&#xA;left join sgt_deposit_tape sdt WITH(NOLOCK) on d.deposit_tape_id = sdt.deposit_tape_id&#xD;&#xA;order by deposit_id desc">
