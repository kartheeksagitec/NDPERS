    <query ID="GetBenefitTypeByOrganization" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="Select cv.CODE_VALUE, cv.DESCRIPTION from SGS_CODE_VALUE cv where cv.CODE_VALUE in (&#xD;&#xA;select Distinct p.BANK_ACCOUNT_TYPE_VALUE from SGT_ORG_PLAN sp &#xD;&#xA;inner join  sgt_plan p on sp.PLAN_ID = p.PLAN_ID&#xD;&#xA;where sp.ORG_ID= @org_id) and cv.CODE_ID =  1201 and cv.CODE_VALUE &lt;&gt; 'FLEX'">
      <parameters>
        <parameter ID="@org_id" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="ESSLookup" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="select * from sgt_deposit d WITH(NOLOCK)&#xD;&#xA;left join sgt_deposit_tape sdt WITH(NOLOCK) on d.deposit_tape_id = sdt.deposit_tape_id&#xD;&#xA;where d.DEPOSIT_AMOUNT &gt; = 0  &#xD;&#xA;order by sdt.deposit_date desc">
      <parameters />
    </query>
    <query ID="GetPaymentDateForEmployer" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT CASE WHEN SDT.DEPOSIT_DATE IS NULL THEN (CASE WHEN D.PAYMENT_DATE IS NULL THEN D.PAYMENT_DATE ELSE CONVERT(DATE,D.PAYMENT_DATE) END) ELSE CONVERT(DATE,SDT.DEPOSIT_DATE) END AS PAYMENT_DATE,* FROM SGT_DEPOSIT D WITH(NOLOCK)INNER JOIN  SGT_REMITTANCE RE WITH(NOLOCK) on D.DEPOSIT_ID =RE.DEPOSIT_ID &#xD;&#xA;INNER JOIN SGT_EMPLOYER_REMITTANCE_ALLOCATION ERA WITH(NOLOCK)  on RE.REMITTANCE_ID = ERA.REMITTANCE_ID&#xD;&#xA;INNER JOIN SGT_EMPLOYER_PAYROLL_DETAIL EPD WITH(NOLOCK) on EPD.EMPLOYER_PAYROLL_HEADER_ID = ERA.EMPLOYER_PAYROLL_HEADER_ID &#xD;&#xA;LEFT JOIN SGT_DEPOSIT_TAPE SDT WITH(NOLOCK) ON SDT.DEPOSIT_TAPE_ID = D.DEPOSIT_TAPE_ID&#xD;&#xA; where EPD.EMPLOYER_PAYROLL_DETAIL_ID = @EMPLOYER_PAYROLL_DETAIL_ID">
      <parameters>
        <parameter ID="@EMPLOYER_PAYROLL_DETAIL_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="LookupBenefitType" sfwQueryType="SubSelectQuery" sfwSql="SELECT * FROM SGT_PLAN PLN with(nolock) INNER JOIN&#xD;&#xA;SGT_REMITTANCE R WITH(NOLOCK) ON PLN.PLAN_ID=R.PLAN_ID WHERE R.DEPOSIT_ID=D.DEPOSIT_ID">
      <parameters />
    </query>
    <query ID="LoadBenfitTypeValue" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT CV.DESCRIPTION FROM SGT_PLAN PLN with(nolock) INNER JOIN&#xD;&#xA;SGT_REMITTANCE R WITH(NOLOCK) ON PLN.PLAN_ID=R.PLAN_ID &#xD;&#xA;JOIN SGS_CODE_VALUE CV ON CV.CODE_VALUE=PLN.BENEFIT_TYPE_VALUE AND CV.CODE_ID=PLN.BENEFIT_TYPE_ID&#xD;&#xA; WHERE R.DEPOSIT_ID=@DEPOSIT_ID">
      <parameters>
        <parameter ID="@DEPOSIT_ID" sfwDataType="int" />
      </parameters>
    </query>
  </queries>
