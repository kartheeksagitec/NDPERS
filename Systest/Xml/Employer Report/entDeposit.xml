    <query ID="LoadOtherDeposits" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="--Old Query&#xD;&#xA;--SELECT * FROM SGT_DEPOSIT&#xD;&#xA;--WHERE DEPOSIT_TAPE_ID=@DEPOSIT_TAPE_ID AND DEPOSIT_ID &lt;&gt; @DEPOSIT_ID&#xD;&#xA;&#xD;&#xA;SELECT DEP.DEPOSIT_ID,ORG.ORG_CODE,SUM(REM.REMITTANCE_AMOUNT) AS AMOUNT INTO #TEMP&#xD;&#xA;FROM SGT_DEPOSIT DEP WITH (NOLOCK)&#xD;&#xA;INNER JOIN SGT_REMITTANCE REM WITH (NOLOCK)&#xD;&#xA;ON REM.DEPOSIT_ID = DEP.DEPOSIT_ID&#xD;&#xA;LEFT OUTER JOIN SGT_ORGANIZATION ORG WITH (NOLOCK)&#xD;&#xA;ON DEP.ORG_ID = ORG.ORG_ID&#xD;&#xA;GROUP BY DEP.DEPOSIT_ID,ORG.ORG_CODE&#xD;&#xA;&#xD;&#xA;SELECT DEP.*,TEM.ORG_CODE AS istrOrgCode,TEM.AMOUNT AS idecRemittanceAmount FROM SGT_DEPOSIT DEP WITH (NOLOCK) LEFT OUTER JOIN #TEMP TEM&#xD;&#xA;ON TEM.DEPOSIT_ID = DEP.DEPOSIT_ID&#xD;&#xA;WHERE DEPOSIT_TAPE_ID=@DEPOSIT_TAPE_ID AND DEP.DEPOSIT_ID &lt;&gt; @DEPOSIT_ID&#xD;&#xA;&#xD;&#xA;DROP TABLE #TEMP">
      <parameters>
        <parameter ID="@DEPOSIT_TAPE_ID" sfwDataType="int" />
        <parameter ID="@DEPOSIT_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="LookupDepositTape" sfwQueryType="SubSelectQuery" sfwSql="SELECT * FROM SGT_DEPOSIT_TAPE SDT&#xD;&#xA;WHERE SDT.DEPOSIT_TAPE_ID=d.DEPOSIT_TAPE_ID">
      <parameters />
    </query>
    <query ID="CreateDeposit" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="INSERT INTO SGT_DEPOSIT&#xD;&#xA;(&#xD;&#xA;DEPOSIT_TAPE_ID, PERSON_ID, ORG_ID, REFERENCE_NO, PAYMENT_DATE, DEPOSIT_AMOUNT, STATUS_ID, STATUS_VALUE, ACH_RETRIEVE_FLAG, DEPOSIT_SOURCE_ID, DEPOSIT_SOURCE_VALUE, PAYMENT_HISTORY_HEADER_ID, CREATED_BY, CREATED_DATE, MODIFIED_BY, MODIFIED_DATE, UPDATE_SEQ,DEPOSIT_DATE&#xD;&#xA;)&#xD;&#xA;SELECT @DEPOSIT_TAPE_ID AS DEPOSIT_TAPE_ID,&#xD;&#xA;&#xD;&#xA;            PERSON_ID, &#xD;&#xA;&#xD;&#xA;            NULL AS ORG_ID,&#xD;&#xA;&#xD;&#xA;            REFERENCE_NO,&#xD;&#xA;&#xD;&#xA;            PAYMENT_DATE,&#xD;&#xA;&#xD;&#xA;            DEPOSIT_AMOUNT,&#xD;&#xA;&#xD;&#xA;            1500 AS STATUS_ID,&#xD;&#xA;&#xD;&#xA;            'APLD' AS STATUS_VALUE,&#xD;&#xA;&#xD;&#xA;            NULL AS ACH_RETRIEVE_FLAG,&#xD;&#xA;&#xD;&#xA;            1211 AS DEPOSIT_SOURCE_ID,&#xD;&#xA;&#xD;&#xA;            'RD' AS DEPOSIT_SOURCE_VALUE,&#xD;&#xA;            &#xD;&#xA;    REFERENCE_NO,&#xD;&#xA;    &#xD;&#xA;            'PERSLink Batch'+' '+ convert(nvarchar,@batchscheduleid) AS CREATED_BY,&#xD;&#xA;&#xD;&#xA;            GETDATE() AS CREATED_DATE,&#xD;&#xA;&#xD;&#xA;            'PERSLink Batch'+' '+ convert(nvarchar,@batchscheduleid) AS MODIFIED_BY,&#xD;&#xA;&#xD;&#xA;            GETDATE() AS MODIFIED_DATE,&#xD;&#xA;&#xD;&#xA;            0 AS UPDATE_SEQ, @DEPOSIT_DATE AS DEPOSIT_DATE&#xD;&#xA;&#xD;&#xA;FROM &#xD;&#xA;&#xD;&#xA;(&#xD;&#xA;&#xD;&#xA;            SELECT PHH.PAYMENT_HISTORY_HEADER_ID AS REFERENCE_NO,PS.EFFECTIVE_DATE AS PAYMENT_DATE ,PERSON_ID,SUM(AMOUNT) AS DEPOSIT_AMOUNT&#xD;&#xA;&#xD;&#xA;            FROM SGT_PAYMENT_HISTORY_DETAIL PHD&#xD;&#xA;&#xD;&#xA;            INNER JOIN SGT_PAYMENT_HISTORY_HEADER  PHH&#xD;&#xA;&#xD;&#xA;                        ON PHH.PAYMENT_HISTORY_HEADER_ID = PHD.PAYMENT_HISTORY_HEADER_ID&#xD;&#xA;&#xD;&#xA;            INNER JOIN SGT_PAYMENT_ITEM_TYPE PIT&#xD;&#xA;&#xD;&#xA;                        ON PHD.PAYMENT_ITEM_TYPE_ID = PIT.PAYMENT_ITEM_TYPE_ID&#xD;&#xA;&#xD;&#xA;            INNER JOIN SGT_PAYMENT_SCHEDULE PS &#xD;&#xA;&#xD;&#xA;                        ON PHH.PAYMENT_SCHEDULE_ID = PS.PAYMENT_SCHEDULE_ID&#xD;&#xA;&#xD;&#xA;            WHERE PHH.PAYMENT_SCHEDULE_ID = @PAYMENTSCHEDULEID&#xD;&#xA;&#xD;&#xA;                        AND ITEM_TYPE_DIRECTION = -1 &#xD;&#xA;&#xD;&#xA;                        AND  ITEM_USAGE_VALUE = 'MNTP'&#xD;&#xA;&#xD;&#xA;                        AND PLSO_FLAG = 'N'&#xD;&#xA;&#xD;&#xA;                        AND RETRO_PAYMENT_TYPE_VALUE IS NULL&#xD;&#xA;&#xD;&#xA;                        AND DEDUCTION_VALUE='RONL'&#xD;&#xA;&#xD;&#xA;            GROUP BY PHH.PAYMENT_HISTORY_HEADER_ID,EFFECTIVE_DATE ,PERSON_ID&#xD;&#xA;&#xD;&#xA;) AS DEPOSIT">
      <parameters>
        <parameter ID="@DEPOSIT_TAPE_ID" sfwDataType="int" />
        <parameter ID="@PAYMENTSCHEDULEID" sfwDataType="int" />
        <parameter ID="@DEPOSIT_DATE" sfwDataType="DateTime" />
        <parameter ID="@batchscheduleid" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetTotalRemittance" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT ISNULL(SUM(REMITTANCE_AMOUNT),0) AS TOTAL_REMITTANCE FROM SGT_REMITTANCE A WITH(NOLOCK)&#xD;&#xA;INNER JOIN SGT_DEPOSIT B WITH(NOLOCK)&#xD;&#xA;ON A.DEPOSIT_ID=B.DEPOSIT_ID&#xD;&#xA;WHERE B.DEPOSIT_TAPE_ID=@DEPOSIT_ID&#xD;&#xA;AND B.STATUS_VALUE != 'IVLD'&#xD;&#xA;AND B.STATUS_VALUE != 'NSF'">
      <parameters>
