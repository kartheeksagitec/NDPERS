    <rule ID="Rule Check If Deposit Amount Greater than DepositTapeAmt" sfwMessageId="4159" sfwExpression="(this.icdoDeposit.deposit_amount &gt; RFunc.GetValue&lt;Decimal&gt;(this, &quot;ibusDepositTape.icdoDepositTape.total_amount&quot;))" sfwObjectBased="True" />
    <rule ID="Rule Check if Deposit Amount is Less than equals Zero" sfwMessageId="4113" sfwExpression="(this.icdoDeposit.deposit_amount &lt;= 0)" sfwObjectBased="True" />
    <rule ID="Rule Check If Deposit Status is NOT Review" sfwMessageId="4124" sfwExpression="(this.icdoDeposit.status_value != &quot;REVW&quot;) &amp;&amp; &#xA;(this.icdoDeposit.status_value != &quot;VALD&quot;)" sfwObjectBased="True" />
    <rule ID="Rule Check If OrgId Valid" sfwMessageId="4115" sfwExpression="(RFunc.ExecuteQuery&lt;Int32&gt;(&quot;entDeposit.GetOrgCountByOrgID&quot;, this.icdoDeposit.istrOrgCode) &lt; 1) &amp;&amp; &#xA;(!string.IsNullOrEmpty(this.icdoDeposit.istrOrgCode))" sfwObjectBased="True" />
    <rule ID="Rule Check If Payment Date is Null" sfwMessageId="4112" sfwExpression="(this.icdoDeposit.payment_date == DateTime.MinValue)" sfwObjectBased="True" />
    <rule ID="Rule Check If PERSLinkID Valid" sfwMessageId="4114" sfwExpression="(RFunc.ExecuteQuery&lt;Int32&gt;(&quot;entPerson.VALIDATE_PERSLINK_ID&quot;, this.icdoDeposit.person_id) &lt; 1) &amp;&amp; &#xA;(this.icdoDeposit.person_id != 0)" sfwObjectBased="True" />
    <rule ID="Rule Check If Reference No is Null" sfwMessageId="4111" sfwExpression="(string.IsNullOrEmpty(this.icdoDeposit.reference_no))" sfwObjectBased="True" />
    <rule ID="Rule Check New Button Visible" sfwExpression="(this.IsNewButtonVisible() == True)" sfwObjectBased="True" />
    <rule ID="Rule Check NSF, Invalidate Button Visibility" sfwExpression="(this.IsInvalidateNSFButtonVisible() == True)" sfwObjectBased="True" />
    <rule ID="Rule Check Save Button Visibility" sfwExpression="(this.IsSaveButtonVisible() == True)" sfwObjectBased="True" />
    <rule ID="Rule Check Validate Button Visibilty" sfwExpression="(this.icdoDeposit.status_value == &quot;CORD&quot;)" sfwObjectBased="True" />
