    </query>
    <query ID="ESSLookupOrgCode" sfwQueryType="SubSelectQuery" sfwSql="SELECT * FROM SGT_ORGANIZATION ORG &#xD;&#xA;WHERE ORG.ORG_ID=d.org_id and d.status_value = 'APLD' and sdt.DEPOSIT_METHOD_VALUE != 'WITR'">
