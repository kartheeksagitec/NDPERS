    <query ID="GetDepositCountInNewMode" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT COUNT(*) FROM SGT_DEPOSIT &#xD;&#xA;WHERE &#xD;&#xA;(((person_id=@PERSLINK_ID)AND(person_id&lt;&gt;0)) OR (@PERSLINK_ID=0))&#xD;&#xA;AND (((ORG_ID=@ORG_ID)AND(ORG_ID&lt;&gt;0)) OR (@ORG_ID=0))&#xD;&#xA;AND PAYMENT_DATE=@PAYMENT_DATE&#xD;&#xA;AND REFERENCE_NO=@REFERENCE_NO&#xD;&#xA;AND ( STATUS_VALUE&lt;&gt;'IVLD')&#xD;&#xA;AND ( STATUS_VALUE&lt;&gt;'NSF')">
      <parameters>
        <parameter ID="@REFERENCE_NO" sfwDataType="string" />
        <parameter ID="@PAYMENT_DATE" sfwDataType="DateTime" />
        <parameter ID="@ORG_ID" sfwDataType="int" />
        <parameter ID="@PERSLINK_ID" sfwDataType="int" />
      </parameters>
    </query>
    <query ID="GetDepositCountInUpdateMode" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT COUNT(*) FROM SGT_DEPOSIT &#xD;&#xA;WHERE &#xD;&#xA;(((person_id=@PERSLINK_ID)AND(person_id&lt;&gt;0)) OR (@PERSLINK_ID=0))&#xD;&#xA;AND (((ORG_ID=@ORG_ID)AND(ORG_ID&lt;&gt;0)) OR (@ORG_ID=0))&#xD;&#xA;AND PAYMENT_DATE=@PAYMENT_DATE&#xD;&#xA;AND REFERENCE_NO=@REFERENCE_NO&#xD;&#xA;AND DEPOSIT_ID &lt;&gt; @DEPOSIT_ID&#xD;&#xA;AND ( STATUS_VALUE&lt;&gt;'IVLD')&#xD;&#xA;AND ( STATUS_VALUE&lt;&gt;'NSF')">
      <parameters>
