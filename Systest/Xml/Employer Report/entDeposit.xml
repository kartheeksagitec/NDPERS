    <rule ID="Rule Check If Deposit Already Exists New Mode" sfwMessageId="4116" sfwExpression="(this.icdoDeposit.payment_date != DateTime.MinValue) &amp;&amp; &#xA;(!string.IsNullOrEmpty(this.icdoDeposit.reference_no)) &amp;&amp; &#xA;(RFunc.ExecuteQuery&lt;Int32&gt;(&quot;entDeposit.GetDepositCountInNewMode&quot;, this.icdoDeposit.reference_no, this.icdoDeposit.payment_date, this.icdoDeposit.org_id, this.icdoDeposit.person_id) &gt; 0)" sfwObjectBased="True" />
