      </parameters>
    </query>
    <query ID="GetInvalidDepositCountByDepositTapeID" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT COUNT(*) FROM SGT_DEPOSIT &#xD;&#xA;WHERE DEPOSIT_TAPE_ID=@DEPOSIT_TAPE_ID&#xD;&#xA;AND STATUS_VALUE='IVLD'">
