<form ID="wfmDepositTapeLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvEmployerReport" sfwResource="1009" sfwType="Lookup" sfwTitle="Deposit Tape Lookup" version="3.5" sfwMaxCount="1000" sfwEntity="entDepositTape" sfwGridCollection="busDepositTapeLookup.icolDepositTape">
  <initialload>
    <query ID="cdoDepositTape" sfwQueryRef="entDepositTape.Lookup" sfwMethodName="LoadDepositTape" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capDepositTapeId" Text="Deposit Tape ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDepositTapeId" sfwDataField="deposit_tape_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoDepositTape" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capDepositDateFrom" Text="Deposit Date From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDepositDateFrom" sfwDataField="CONVERT(VARCHAR(10), deposit_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoDepositTape" sfwRelatedControl="txtDepositDateTo" />
                <CompareValidator ControlToValidate="txtDepositDateFrom" ErrorMessage="Deposit Date From must be in mm/dd/yyyy format." ID="valDepositDateFrom" Operator="DataTypeCheck" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capDepositDateTo" Text="Deposit Date To : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtDepositDateTo" sfwDataField="CONVERT(VARCHAR(10), deposit_date, 101)" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoDepositTape" sfwRelatedControl="txtDepositDateFrom" />
                <CompareValidator ControlToValidate="txtDepositDateTo" ErrorMessage="Deposit Date To must be in mm/dd/yyyy format." ID="valDepositDateTo" Operator="DataTypeCheck" Text="*" Type="Date" />
                <CompareValidator ControlToCompare="txtDepositDateFrom" ControlToValidate="txtDepositDateTo" ErrorMessage="Deposit Date To cannot be less than Deposit Date From." ID="valDepositDateRange" Operator="GreaterThanEqual" Text="*" Type="Date" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capStatusValue" Text="Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="status_value" sfwOperator="like" sfwQueryID="cdoDepositTape" sfwLoadType="CodeGroup" sfwLoadSource="1502" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capDepositMethodValue" Text="Deposit Method : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlDepositMethodValue" sfwDataField="deposit_method_value" sfwOperator="like" sfwQueryID="cdoDepositTape" sfwLoadType="CodeGroup" sfwLoadSource="1501" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capBankAccountValue" Text="Bank Account : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlBankAccountValue" sfwDataField="bank_account_value" sfwOperator="like" sfwQueryID="cdoDepositTape" sfwLoadType="CodeGroup" sfwLoadSource="1201" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capTotalAmount" Text="Deposit Total : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtTotalAmount" sfwDataField="total_amount" sfwDataType="Decimal" sfwOperator="=" sfwQueryID="cdoDepositTape" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmDepositTapeMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmDepositTapeMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintdeposittapeid=DepositTapeId;aintActivityInstanceID=#0" sfwRelatedControl="grvDepositTape" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="grvDepositTape" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="grvDepositTape" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" AutoGenerateColumns="False" ID="grvDepositTape" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Deposit Tape ID" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="DepositTapeId" CssClass="ItemRightAlign" Font.Underline="True" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Deposit Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="DepositDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="No of Deposit Details" ItemStyle.HorizontalAlign="Center">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="DepositsCount" CssClass="ItemCenterAlign" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Deposit Method">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="DepositMethodDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Total Amount" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="TotalAmount" CssClass="ItemRightAlign" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Bank Account">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="BankAccountDescription" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>