<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmCenterLeftOrganizationLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvOrganization" sfwResource="300 " sfwType="Lookup" sfwTitle="Organization Lookup" sfwMaxCount="1000" version="3.5" sfwEntity="entOrganization" sfwGridCollection="busOrganizationLookup.iclbOrganization">
  <initialload>
    <query ID="cdoOrganization" sfwQueryRef="entOrganization.Lookup" sfwMethodName="LoadOrganizations" />
    <query ID="LookupSubQuery" sfwQueryRef="entOrganization.LookupSubQuery" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCLOrgCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCLOrgCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwTextBox ID="txtCLOrgOrgCode" sfwDataField="org_code" sfwOperator="like" sfwQueryID="cdoOrganization" sfwTwoWayBinding="False" sfwWatermark="Org Code" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwTextBox ID="txtCLOrgOrgName" sfwDataField="org_name" sfwOperator="like" sfwQueryID="cdoOrganization" sfwTwoWayBinding="False" sfwWatermark="Name" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwDropDownList ID="txtCLOrgOrgTypeValue" sfwDataField="org_type_value" sfwFirstItemText="Org Type" sfwOperator="like" sfwQueryID="cdoOrganization" sfwTwoWayBinding="False" sfwLoadType="CodeGroup" sfwLoadSource="201" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnCLOrgSearch" sfwMethodName="btnSearch_Click" Text="Search" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCLOrgResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblCLOrgResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnCLOrgOpen" sfwActiveForm="wfmOrganizationMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintorgid=OrgId" sfwRefreshUpdatePanels="uppCenterMiddle" sfwRelatedControl="dgrCLOrgResult" Text="Open" Visible="False" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrCLOrgResult">
                  <Columns>
                    <TemplateField HeaderText="Org ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Code ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnCLOrgOpen" sfwEntityField="OrgCode" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgName" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>