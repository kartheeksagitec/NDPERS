<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmPlanLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvOrganization" sfwResource="309 " sfwType="Lookup" sfwTitle="Plan Lookup" version="3.5" sfwEntity="entPlan" sfwGridCollection="busPlanLookup.icolPlan">
  <initialload>
    <query ID="cdoPlan" sfwQueryRef="entPlan.Lookup" sfwMethodName="LoadPlans" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblplan_id" Text="Plan ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txt_plan_id" sfwDataField="plan_id" sfwDataType="Numeric" sfwQueryID="cdoPlan" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblplan_name" Text="Plan Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txt_plan_name" sfwDataField="plan_name" sfwQueryID="cdoPlan" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblbenefit_type_value" Text="Benefit Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddl_benefit_type_value" sfwDataField="benefit_type_value" sfwQueryID="cdoPlan" sfwLoadType="CodeGroup" sfwLoadSource="250" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capBankAccountTypeValue" Text="Bank Account Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlBankAccountTypeValue" sfwDataField="bank_account_type_value" sfwOperator="like" sfwQueryID="cdoPlan" sfwLoadType="CodeGroup" sfwLoadSource="1201" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmPlanMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmPlanMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintplanid=PlanId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="PlanId" sfwRelatedControl="dgrResult" Text="Delete" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Plan ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PlanId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Plan Name">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PlanName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Benefit Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="BenefitTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>