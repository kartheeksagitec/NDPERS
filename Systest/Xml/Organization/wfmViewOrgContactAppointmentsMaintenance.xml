<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmViewOrgContactAppointmentsMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvOrganization" sfwResource="900 " sfwType="Maintenance" sfwTitle="View Org Contact/Appointments Maintenance" version="3.5" sfwEntity="entOrganization">
  <initialload>
    <callmethods sfwMode="Update" sfwMethodName="FindOrgContactAppointment" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Other Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNewOrganizationContactTicket" sfwActiveForm="wfmContactTicketMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="AintOrgCodeId=lblOrgCode" Text="New" />
                <sfwButton ID="btnOpenOrganizationContactTicket" sfwActiveForm="wfmContactTicketMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintcontactticketid=ContactTicketId" sfwRelatedControl="grvOrganizationContactTicket" sfwResource="900" Text="Open" sfwDisableViewOnly="True" />
                <sfwLabel ID="lblOrgCode" sfwEntityField="OrgCode" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" AutoGenerateColumns="False" ID="grvOrganizationContactTicket" sfwSelection="Many" sfwEntityField="lstOrganizationContactTicket" sfwGridEntity="entContactTicket">
                  <Columns>
                    <TemplateField HeaderText="Ticket Number">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ContactTicketId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Contact Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ContactTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Assigned to User ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objUser.UserId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Ticket Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Appointment Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objAppointmentSchedule.AppointmentTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Counselor">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objCounselor.UserId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Appointment Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objAppointmentSchedule.AppointmentDate" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwTwoWayBinding="True" Visible="False" sfwEntityField="UpdateSeq" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="lstOrganizationContactTicket" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>