              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="PaymentSchedule Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPaymentDate" Text="Benefit Payment Date : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPaymentDate" sfwDataFormat="{0:d}" sfwVisibleRule="VisibleRuleFieldsNotReadOnly" sfwEntityField="PaymentDate" />
                <sfwLabel ID="lblPaymentDate" sfwDataFormat="{0:d}" sfwVisibleRule="VisibleRuleFieldsReadonly" sfwEntityField="PaymentDate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capEffectiveDate" Text="Check/ACH Effective Date : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEffectiveDate" sfwDataFormat="{0:d}" sfwVisibleRule="VisibleRuleFieldsNotReadOnly" sfwEntityField="EffectiveDate" />
                <sfwLabel ID="lblEffectiveDate" sfwDataFormat="{0:d}" sfwVisibleRule="VisibleRuleFieldsReadonly" sfwEntityField="EffectiveDate" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capScheduleTypeValue" Text="Schedule Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlScheduleTypeValue" sfwMode="New" sfwLoadType="Query" sfwLoadSource="entPaymentSchedule.LoadSheduleTypes" sfwEntityField="ScheduleTypeValue" />
                <sfwLabel ID="lblScheduleTypeDescription" sfwMode="Update" sfwEntityField="ScheduleTypeDescription" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatusDescription" Text="Status / Action Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblStatusDescription" sfwEntityField="StatusDescription" />
                <sfwLiteral ID="l" Text=" / " />
                <sfwLabel ID="lblActionStatusDescription" sfwEntityField="ActionStatusDescription" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCheckMessage" Text="Check Message : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCheckMessage" sfwVisibleRule="VisibleRuleFieldsNotReadOnly" Width="200px" sfwEntityField="CheckMessage" />
                <sfwLabel ID="lblCheckMessage" sfwVisibleRule="VisibleRuleFieldsReadonly" sfwEntityField="CheckMessage" />
