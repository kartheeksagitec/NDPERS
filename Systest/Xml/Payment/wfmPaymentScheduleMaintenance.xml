              <sfwColumn>
                <sfwLabel ID="capProcessDate" Text="Processed Date : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblProcessDate" sfwDataFormat="{0:d}" sfwEntityField="ProcessDate" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capPaymentScheduleId1" Text="Payment Schedule ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPaymentScheduleId1" sfwEntityField="PaymentScheduleId" />
