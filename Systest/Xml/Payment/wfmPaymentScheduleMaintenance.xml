              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlPaymentStepRefs" sfwCaption="Payment Schedule Steps">
          <sfwTable CssClass="Table" ID="tblPaymentStepRefs">
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" AutoGenerateColumns="False" ID="grvPaymentScheduleStep" sfwEntityField="lstPaymentScheduleStep" sfwGridEntity="entPaymentScheduleStep">
                  <Columns>
                    <TemplateField HeaderText="Step Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPaymentStep.StepName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Active">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="objPaymentStep.ActiveFlag" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Step Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Batch Schedule ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="BatchScheduleId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Payment Schedule Step ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PaymentScheduleStepId" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
