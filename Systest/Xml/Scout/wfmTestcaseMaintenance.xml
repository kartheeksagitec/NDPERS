<form ID="wfmTestcaseMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvScout" sfwResource="805" sfwType="Maintenance" sfwTitle="Testcase Maintenance" version="3.5" sfwEntity="entTestcase">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewTestcase" />
    <callmethods sfwMode="Update" sfwMethodName="FindTestcase" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable CellPadding="3" ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintTestcaseId=TestcaseId" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Test Case Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text=" Testcase Key :" sfwIsCaption="True" ID="capTestcaseKey" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox sfwEntityField="TestcaseKey" ID="txtTestcaseKey" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel Text=" Status :" sfwIsCaption="True" ID="capStatus" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList sfwLoadType="CodeGroup" sfwLoadSource="43" sfwEntityField="StatusValue" ID="ddlStatusValue" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Description :" sfwIsCaption="True" ID="capDescription" />
              </sfwColumn>
              <sfwColumn ColumnSpan="3">
                <sfwTextBox Rows="2" Width="400px" sfwEntityField="TestcaseDesc" ID="txtTestcaseDesc" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Usecase :" sfwIsCaption="True" ID="capUsecase" />
              </sfwColumn>
              <sfwColumn ColumnSpan="6">
                <sfwDropDownList DataTextField="usecase_desc" DataValueField="usecase_id" sfwLoadType="Query" sfwLoadSource="entUsecase.Lookup" sfwEntityField="UsecaseId" ID="ddlUsecaseId" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Notes :" sfwIsCaption="True" ID="capNotes" />
              </sfwColumn>
              <sfwColumn ColumnSpan="3">
                <sfwTextBox Rows="5" TextMode="MultiLine" Width="400px" sfwEntityField="Notes" ID="txtNotes" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel Visible="False" sfwEntityField="TestcaseId" ID="lblTestcaseId" />
              </sfwColumn>
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlTestCaseDetails" sfwCaption="Testcase Details">
          <sfwTable ID="tblTestCaseDetails">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnOpenTestcaseDetail" sfwActiveForm="wfmTestcaseDetailMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintTestcaseDtlId=TestcaseDtlId" sfwRelatedControl="dgrTestcaseDetail" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnNewTestcaseDetail" sfwActiveForm="wfmTestcaseDetailMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="aintTestcaseId=txtTestcaseId" Text="New" />
                <sfwTextBox ID="txtTestcaseId" sfwEntityField="TestcaseId" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView ID="dgrTestcaseDetail" sfwSelection="Many" sfwEntityField="lstTestcaseDetail" sfwGridEntity="entTestcaseDetail">
                  <Columns>
                    <TemplateField HeaderText="Detail Key">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenTestcaseDetail" sfwEntityField="TestcaseDtlKey" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Description">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="TestcaseDtlDesc" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified By">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ModifiedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="ModifiedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Testcase Dtl Id" Visible="False">
                      <ItemTemplate>
                        <sfwLabel Visible="False" sfwEntityField="TestcaseDtlId" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwTwoWayBinding="True" Visible="False" sfwEntityField="UpdateSeq" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="lstTestcaseDetail" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>