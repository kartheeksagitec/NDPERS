<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmUseCaseFlowLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvScout" sfwResource="810 " sfwType="Lookup" sfwTitle="Usecase Flow Lookup" sfwMaxCount="100" version="3.5" sfwEntity="entUsecaseFlow" sfwGridCollection="busUseCaseFlowLookup.iclbLookupResult">
