<form ID="wfmESSLastThreeMonthsMessageMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2003" sfwType="FormLinkMaintenance" sfwTitle="Last Three Months Message Details" version="3.5" sfwEntity="entESSHome" sfwTemplate="ESSLastThreeMonthsMessageMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSLastThreeMonthsMessageLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="LoadLastThreeMonthMessages" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar">
      <items>
        <sfwTable ID="toolbar" />
        <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
        <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
        <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Message Details">
      <items>
        <sfwTable ID="tblMain" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvWSSMessageDetails" sfwEntityField="lstWSSMessageDetails" sfwGridEntity="entWssMessageDetail">
          <Columns>
            <TemplateField HeaderText="Message">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objWSSMessageHeader.MessageText" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Priority">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objWSSMessageHeader.PriorityDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Created Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objWSSMessageHeader.CreatedDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Web Link">
              <ItemTemplate>
                <sfwLabel sfwEntityField="WebLink" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Correspondence Link">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrCorrespondenceLink" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Wss Message Detail ID" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="WssMessageDetailId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstWSSMessageDetails" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>