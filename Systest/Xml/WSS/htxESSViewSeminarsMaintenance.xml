              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Seminar Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="SeminarName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Seminar Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="SeminarDate1" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Contact Ticket ID" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactTicketId" />
