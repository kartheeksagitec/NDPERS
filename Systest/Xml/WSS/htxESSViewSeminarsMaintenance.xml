                <sfwLabel sfwEntityField="SeminarStatus" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlSeminarInfo">
      <items>
        <sfwLabel ID="lblSeminarText" Text="Please visit our website to view and register to attend upcoming seminars.&lt;a href=&quot;https://ndpers.nd.gov/events &quot; target=&quot;_blank&quot;&gt;https://ndpers.nd.gov/events &lt;/a&gt;&lt;/br&gt;&lt;/br&gt;" />
