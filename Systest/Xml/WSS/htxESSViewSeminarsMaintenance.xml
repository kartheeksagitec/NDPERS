<form ID="wfmESSViewSeminarsMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2010" sfwType="FormLinkMaintenance" sfwTitle="View Seminars Maintenance" version="3.5" sfwEntity="entESSHome" sfwTemplate="ESSViewSeminarsMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSViewSeminarsLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindESSViewSeminars" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblN" />
        <sfwLiteral ID="ltrlViewSeminars" Text="Training Opportunities" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Seminar Registration" CssClass="hideControl">
      <items>
        <sfwTable ID="tblMain" />
        <sfwButton ID="btnOpenSeminars" sfwActiveForm="wfmESSSeminarMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintContactTicketId=ContactTicketId;aintOrgId=OrgId" sfwRelatedControl="grvSeminars" Text="Open" Visible="False" />
