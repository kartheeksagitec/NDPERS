<form ID="wfmMSSActiveMemberProfileMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Personal Profile" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSActiveMemberProfileMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSActiveMemberProfileLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSMemberProfile" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage3" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Personal Profile" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage3" sfwCaption="Personal Information">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwLabel ID="lblFirstName" Text="First Name :" sfwIsCaption="True" />
        <sfwLabel ID="lblFirstName1" sfwEntityField="objPerson.FirstName" />
        <sfwLabel ID="lblBirthDate" Text="Birth Date :" sfwIsCaption="True" />
        <sfwLabel ID="lblDateOfBirth" sfwDataFormat="{0:d}" sfwEntityField="objPerson.DateOfBirth" />
        <sfwLabel ID="lblMemberID" Text="Member ID :" sfwIsCaption="True" />
        <sfwLabel ID="lblPersonId" sfwEntityField="objPerson.PersonId" />
        <sfwLabel ID="lblMiddleName" Text="Middle Name :" sfwIsCaption="True" />
        <sfwLabel ID="lblMiddleName1" sfwEntityField="objPerson.MiddleName" />
        <sfwLabel ID="lblGender" Text="Gender :" sfwIsCaption="True" />
        <sfwLabel ID="lblGenderDescription" sfwEntityField="objPerson.GenderDescription" />
        <sfwLabel ID="lblLastName" Text="Last Name :" sfwIsCaption="True" />
        <sfwLabel ID="lblLastName1" sfwEntityField="objPerson.LastName" />
        <sfwLabel ID="lblMaritalStatus" Text="Marital Status :" sfwIsCaption="True" />
        <sfwLabel ID="lblMaritalStatusDescription" sfwEntityField="objPerson.MaritalStatusDescription" />
        <sfwLinkButton ID="btnLinkButton22" sfwActiveForm="wfmMaritalChangeWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPersonID=objPerson.PersonId" sfwVisibleRule="VisibleRuleIsNotPSEmployee" />
        <sfwLiteral ID="ltrLabel" sfwVisibleRule="VisibleRuleIsPSEmployee" Text="&lt;font style=&quot;padding-left:5px; text-align:left&quot;&gt;To update your Name and/or Marital Status, please complete&amp;nbsp;&lt;/font&gt;" />
        <sfwLinkButton ID="btnPdfCorrespondenceClick1" sfwNavigationParameter="pdf_file_name=#PDFCorrespondence\Templates\SFN-10766.pdf" sfwMethodName="btnPdfCorrespondence_Click"  sfwVisibleRule="VisibleRuleIsPSEmployee"/>
        <sfwButton ID="btnEditProfile" sfwActiveForm="wfmMSSMemberDetailMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPersonID=objPerson.PersonId" sfwVisibleRule="VisibleRuleIsNotPSEmployee" Text="Edit Personal Information" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage4" sfwCaption="Address">
      <items>
        <sfwTable ID="tblNewPage1" />
        <sfwPanel GroupingText="&lt;u&gt;&lt;b&gt;Current Address&lt;/b&gt;&lt;/u&gt;" ID="pnlNewPage1" sfwCaption="Current Address" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage5" />
            <sfwLabel AssociatedControlID="lblAddrLine1" ID="capAddrLine1" Text="Address Line 1 : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrLine1" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressStreet1CAPS" />
            <sfwLabel AssociatedControlID="lblAddrLine2" ID="capAddrLine2" Text="Address Line 2 : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrLine2" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressStreet2CAPS" />
            <sfwLabel AssociatedControlID="lblAddrCity" ID="capAddrCity" Text="City : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrCity" sfwEntityField="objPerson.objPersonCurrentAddress.IstrAddressCityCAPS" />
            <sfwLabel AssociatedControlID="lblAddrStateDescription" ID="capAddrStateDescription" Text="State : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrStateDescription" sfwEntityField="objPerson.objPersonCurrentAddress.IstrStateDescriptionCAPS" />
            <sfwLabel AssociatedControlID="lblAddrCountryDescription" ID="capAddrCountryDescription" Text="Country : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrCountryDescription" sfwEntityField="objPerson.objPersonCurrentAddress.IstrCountryDescriptionCAPS" />
            <sfwLabel AssociatedControlID="lblAddrZipCode" ID="capAddrZipCode" Text="Zip Code : " sfwIsCaption="True" />
            <sfwLabel ID="lblAddrZipCode" sfwEntityField="objPerson.objPersonCurrentAddress.ZipDescription" />
            <sfwLabel ID="lblPersonAddressId" Visible="False" sfwEntityField="objPerson.objPersonCurrentAddress.PersonAddressId" />
            <sfwButton ID="btnLinkButton" sfwActiveForm="wfmMSSAddressMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="aintPersonID=lblPersonId" Text="Update Your Address" />
          </items>
        </sfwPanel>
        <sfwPanel GroupingText="&lt;u&gt;&lt;b&gt;Other Addresses on File&lt;/b&gt;&lt;/u&gt;" ID="pnlNew" sfwCaption="Other Addresses" sfwCollapsible="False" sfwVisibleRule="VisibleRuleForFutureAddresses">
          <items>
            <sfwTable ID="tblNewPage4" />
            <sfwGridView AllowPaging="True" ID="grvPersonFutureAddress" sfwEntityField="objPerson.lstPersonFutureAddress" sfwGridEntity="entPersonAddress">
              <Columns>
                <TemplateField HeaderText="Address Line1">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IstrAddressStreet1CAPS1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Address Line 2">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IstrAddressStreet2CAPS1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="City">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IstrAddressCityCAPS1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="State">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IstrStateDescriptionCAPS1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Country">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="IstrCountryDescriptionCAPS1" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Zip Code">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="ZipDescription" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Start Date">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="StartDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="End Date">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EndDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Person Address ID" Visible="False">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="PersonAddressId" />
                  </ItemTemplate>
                </TemplateField>
              </Columns>
            </sfwGridView>
          </items>
        </sfwPanel>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage5" sfwCaption="Contact Information">
      <items>
        <sfwTable ID="tblNewPage2" />
        <sfwLabel ID="lblHomePh" Text="Home Phone Number :" sfwIsCaption="True" />
        <sfwTextBox ID="txtHomePh" sfwDataFormat="{0:(###)###-####}" sfwEntityField="objPerson.HomePhoneNo" />
        <sfwLabel ID="lblWorkPh" Text="Work Phone Number :" sfwIsCaption="True" />
        <sfwTextBox ID="txtWorkPh" sfwDataFormat="{0:(###)###-####}" sfwEntityField="objPerson.WorkPhoneNo" />
        <sfwLabel ID="lblCellPh" Text="Cell Phone Number :" sfwIsCaption="True" />
        <sfwTextBox ID="txtCellPh" sfwDataFormat="{0:(###)###-####}" sfwEntityField="objPerson.CellPhoneNo" />
        <sfwLabel ID="lblEmail" Text="Email :" sfwIsCaption="True" />
        <sfwTextBox ID="txtEmail" sfwEntityField="objPerson.EmailAddress" />
        <sfwLabel ID="lblCommunication" Text="Communication Preference :" Visible="False" sfwIsCaption="True" />
        <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlCommunication" Visible="False" sfwDefaultValue="MAIL" sfwLoadType="CodeGroup" sfwLoadSource="303" sfwEntityField="objPerson.CommunicationPreferenceValue" />
        <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintPersonID=objPerson.PersonId" Text="Save" />
        <sfwButton ID="btnRefreshData" sfwMethodName="btnCancel_Click" Text="Cancel" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPerson" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
    <entityfield ID="objPerson.objPersonCurrentAddress" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
    <entityfield ID="objPerson.lstPersonFutureAddress" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>