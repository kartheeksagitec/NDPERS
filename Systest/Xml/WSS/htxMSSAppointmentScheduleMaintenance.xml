        <sfwDropDownList DataTextField="data1" DataValueField="code_value" ID="ddlAppointmentTypeValue" sfwVisibleRule="VisibleRuleForAppointmentAndDeathNotice" sfwLoadType="Query" sfwLoadSource="entContactTicket.LimitedAppointmentTypesForMSS" sfwEntityField="objAppointmentSchedule.AppointmentTypeValue" sfwValidationRules="RULE CHECK APPOINTMENT TYPE IS BLANK" />
        <sfwLabel ID="lblAppointmentTypeDescription" sfwMode="Update" sfwEntityField="objAppointmentSchedule.AppointmentTypeDescription" />
        <sfwLabel ID="lblApptype1" sfwVisibleRule="VisibleRuleForAppointmentAndDeathNotice" SkinID="ReqInformation" Text="&amp;nbsp;*" sfwIsCaption="True" />
        <sfwLabel ID="capNotes" Text="Reason For Appointment : " sfwIsCaption="True" />
        <sfwTextBox ID="txtNotes" Rows="3" TextMode="MultiLine" Width="500px" sfwEntityField="Notes" />
        <sfwLabel ID="capRequestedDateFrom" Text="Date Range : " sfwIsCaption="True" />
        <sfwTextBox ID="txtRequestedDateFrom" sfwDataFormat="{0:d}" sfwEntityField="objAppointmentSchedule.RequestedDateFrom" sfwValidationRules="IsMSSAppointmentRequestDatePastDate;ISRequestedDateFRomGreaterThanRequestedDateTo;RULE APPOINTMENT Requested Date From Required" />
        <sfwLabel ID="lblDate1" SkinID="ReqInformation" Text="&amp;nbsp;*&amp;nbsp;" sfwIsCaption="True" />
        <sfwTextBox ID="txtRequestedDateTo" sfwDataFormat="{0:d}" sfwEntityField="objAppointmentSchedule.RequestedDateTo" sfwValidationRules="IsMSSAppointmentRequestDatePastDate;RULE APPOINTMENT Requested Date To Required" />
        <sfwLabel ID="lblDate2" SkinID="ReqInformation" Text="&amp;nbsp;*&amp;nbsp;" sfwIsCaption="True" />
        <sfwLiteral ID="ltrDisclaimer" Text="(Mon - Fri , Except Holidays)" />
        <CompareValidator ID="cmvRequestedDateRange" />
        <sfwLabel ID="capTimeOfDayValue" Text="Time Of Day : " sfwIsCaption="True" Visible="False" />
        <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlTimeOfDayValue" sfwLoadType="CodeGroup" sfwLoadSource="5006" sfwEntityField="objAppointmentSchedule.TimeOfDayValue" Visible="False" />
        <sfwLabel ID="capCallbackPhone" Text="Callback Phone Number : " sfwIsCaption="True" />
        <sfwTextBox ID="txtCallbackPhone" sfwDataFormat="{0:(###)###-####}" sfwEntityField="CallbackPhone" sfwValidationRules="Rule Check IF CallBack Telephone IS Blank" />
