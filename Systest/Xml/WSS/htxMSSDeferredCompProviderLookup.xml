<form ID="wfmMSSDeferredCompProviderLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkLookup" sfwTitle="Deferred Compensation Provider Lookup" version="3.5" sfwEntity="entOrgContact" sfwGridCollection="busMSSPersonAccountDefCompProviderLookup.icolMSSProviderContact" sfwTemplate="MSSDeferredCompProviderLookup.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSDeferredCompProviderLookup" isBasedOnWfm="true">
  <initialload>
    <query ID="cdoOrgContact" sfwQueryRef="entOrgContact.GetActiveContactForMSS" sfwMethodName="LoadPersons" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlCriteria" sfwCaption="Criteria" Visible="False">
      <items>
        <sfwTable ID="tblCriteria" />
        <sfwLabel ID="capOrgCode" Text="Provider Org Code : " sfwIsCaption="True" />
        <sfwTextBox ID="txtOrgCode" sfwDataField="org_code" sfwOperator="like" sfwQueryID="cdoOrgContact" />
        <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" Visible="False" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlResult" sfwCaption="Search Results">
      <items>
        <sfwTable ID="tblResult" />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmMSSDefCompProviderMaintenance" sfwMethodName="btnOpen_Click" sfwRelatedControl="grvMSSProviderContact" Text="Open" Visible="False" />
        <sfwGridView AllowPaging="True" ID="grvMSSProviderContact" sfwSelection="Many">
          <Columns>
            <TemplateField HeaderText="Contact ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Org Contact ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="OrgContactId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Contact Name">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="objContact.ContactName" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails />
</form>