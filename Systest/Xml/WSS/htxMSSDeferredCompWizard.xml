    <entityfield ID="objPerson" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objDefCompProvider.objProviderOrganization" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objDefCompProvider.objProviderAgentOrgContact.objContact" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objDefCompProvider.objPersonEmployment.objOrganization" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
