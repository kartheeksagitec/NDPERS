<form ID="wfmMSSPaymentHistoryMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Payment History Details" version="3.5" sfwEntity="entMSSPaymentHistory" sfwTemplate="MSSPaymentHistoryMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSPaymentHistoryLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSPaymentHistory" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Payment History Details" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage6" sfwCaption="pnlBack" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage2" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Payment History Details">
      <items>
        <sfwTable ID="tblMain" />
        <sfwLabel ID="capPaymentDate" Text="Payment Date : " sfwIsCaption="True" />
        <sfwLabel ID="lblPaymentDate" sfwDataFormat="{0:d}" sfwEntityField="PaymentDate" />
        <sfwLabel ID="capStatusDescription" Text="Status : " sfwIsCaption="True" />
        <sfwLabel ID="lblStatusDescription" sfwEntityField="StatusDescription" />
        <sfwLabel ID="capGrossAmount" Text="Gross Benefit : " sfwIsCaption="True" />
        <sfwLabel ID="lblGrossAmount" sfwDataFormat="{0:C}" sfwEntityField="GrossAmount" />
        <sfwLabel AssociatedControlID="lblTaxableAmount" ID="capTaxableAmount" Text="Taxable Amount : " sfwIsCaption="True" />
        <sfwLabel ID="lblTaxableAmount" sfwDataFormat="{0:C}" sfwEntityField="TaxableAmount" />
        <sfwLabel ID="capDeductionAmount" Text="Deductions : " sfwIsCaption="True" />
        <sfwLabel ID="lblDeductionAmount" sfwDataFormat="{0:C}" sfwEntityField="DeductionAmount" />
        <sfwLabel AssociatedControlID="lblNonTaxableAmount" ID="capNonTaxableAmount" Text="Non-Taxable Amount : " sfwIsCaption="True" />
        <sfwLabel ID="lblNonTaxableAmount" sfwDataFormat="{0:C}" sfwEntityField="NonTaxableAmount" />
        <sfwLabel ID="capNetAmount" Text="Net Payment : " sfwIsCaption="True" />
        <sfwLabel ID="lblNetAmount" sfwDataFormat="{0:C}" sfwEntityField="NetAmount" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlPaymentDetails" sfwCaption="Payment Details">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwGridView AllowPaging="True" ID="grvPaymentHistoryDetail" sfwSelection="Many" sfwEntityField="lstPaymentHistoryDetail" sfwGridEntity="entPaymentHistoryDetail">
          <Columns>
            <TemplateField HeaderText="Item Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPaymentItemType.ItemTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Check Component Description">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPaymentItemType.CheckComponentDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="Amount" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage" sfwCaption="Payment Method">
      <items>
        <sfwTable ID="tblPaymentHistoryDistribution" />
        <sfwButton ID="btnOpenPaymentHistoryDistribustion" sfwActiveForm="wfmPaymentHistoryDistributionMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintpaymenthistorydistributionid=PaymentHistoryDistributionId" sfwRelatedControl="grvPaymentHistoryDistribustion" Text="Open" Visible="False" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvPaymentHistoryDistribustion" sfwEntityField="lstPaymentHistoryDistribution" sfwGridEntity="entPaymentHistoryDistribution">
          <Columns>
            <TemplateField HeaderText="Control Number" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenPaymentHistoryDistribustion" sfwEntityField="PaymentHistoryDistributionId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payment Method">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PaymentMethodDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="DistributionStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="NetAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PERSLink ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Org Code ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objOrganization.OrgCode" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Bank Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="BankName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Routing Number">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RoutingNumber" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Account Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="AccountTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Account Number" SortExpression="AccountNumber">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ScrambledAccountNumber" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Check#">
              <ItemTemplate>
                <sfwLabel sfwEntityField="CheckNumber" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Original Control Number" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="OldDistributionId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Recipient Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RecipientName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Check Message" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="CheckMessage" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Address">
              <ItemTemplate>
                <sfwLabel sfwEntityField="AddrDescription" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstPaymentHistoryDetail" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstPaymentHistoryDistribution" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>