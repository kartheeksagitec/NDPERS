<form ID="wfmWssAcknowledgementLookupLOB" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2000" sfwType="Lookup" sfwTitle="WSS Acknowledgement Lookup" sfwMaxCount="50" version="3.5" sfwEntity="entWssAcknowledgement" sfwGridCollection="busWssAcknowledgementLookup.iclbWssAcknowledgement">
