      </sfwColumn>
      <sfwColumn>
        <sfwLabel ID="capIdteRequestDateTo" Text="Date To : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="endTime" sfwDataFormat="{0:d}" sfwDataType="DateTime" />
        <RequiredFieldValidator ControlToValidate="endTime" ErrorMessage="Date To is Required" Text="*" />
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwLabel ID="capIstrProcessName" Text="Process Name : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList DataTextField="description" DataValueField="process_id" ID="typeId" sfwLoadType="Query" sfwLoadSource="entBpmProcess.LoadActiveProcesses" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel ID="capActivityTypeId" Text="Activity List: " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList DataTextField="name" DataValueField="activity_id" ID="qualifiedName" sfwLoadType="Query" sfwLoadSource="entBpmActivity.LoadActivityList" />
