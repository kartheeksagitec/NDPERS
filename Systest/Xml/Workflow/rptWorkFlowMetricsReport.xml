    <query sfwQueryRef="entSolBpmActivityInstance.rptWorkFlowMetrics" ID="cdoBpmActivityInstance" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwLabel ID="capIdteRequestDateFrom" Text="Date From : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="startTime" sfwDataFormat="{0:d}" sfwDataType="DateTime" />
