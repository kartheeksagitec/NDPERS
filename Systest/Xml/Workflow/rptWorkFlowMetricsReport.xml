    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwLabel ID="capUserID" Text="User ID : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList DataTextField="user_id" DataValueField="user_id" ID="userID" sfwLoadType="Query" sfwLoadSource="entUser.LOADACTIVEUSER" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel ID="capStatus" Text="Status : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwDropDownList ID="status" sfwLoadType="CodeGroup" sfwLoadSource="20" />
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwLabel ID="capPersonID" Text="PERSLink ID : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="personID" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel ID="lblOrgID" Text="Org Code ID : " sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="orgID" />
      </sfwColumn>
    </sfwRow>
  </sfwTable>
