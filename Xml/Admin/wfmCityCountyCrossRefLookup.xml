<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmCityCountyCrossRefLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="425 " sfwType="Lookup" sfwTitle="City County Cross Ref Lookup" version="3.5" sfwEntity="entCountyRef" sfwGridCollection="busCityCountyLookup.icolCountyRef">
  <initialload>
    <query ID="cdoCountyRef" sfwQueryRef="entCountyRef.GET_COUNTY" sfwMethodName="LoadCityCountys" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCountyRefId" Text="County Ref ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCountyRefId" sfwDataField="county_ref_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoCountyRef" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCity" Text="City : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCity" sfwDataField="city" sfwOperator="like" sfwQueryID="cdoCountyRef" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCounty" Text="County : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCounty" sfwDataField="county" sfwOperator="like" sfwQueryID="cdoCountyRef" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="StoreSearch" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmCityCountyCrossRefMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmCityCountyCrossRefMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintcountyrefid=CountyRefId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="County Ref ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="CountyRefId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="City">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="City" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="County">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="County" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>