<?xml version="1.0" encoding="utf-8"?>
  <form ID="wfmUsersLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="501 " sfwType="Lookup" sfwTitle="Users Lookup" sfwMaxCount="250" sfwHelpFile="Assign Roles=Assign Roles.htm;Create New Roles=Create New Roles.htm;Delete Role=Delete Role.htm;Modify Roles=Modify Roles.htm;Create New User=Create New User.htm;Modify User=Modify User.htm;Delete User=Delete User.htm;Security Overview=Security Overview.htm" version="3.5" sfwEntity="entUser" sfwGridCollection="busUserLookup.iclbLookupResult">
  <initialload>
    <query ID="cdoUser" sfwQueryRef="entUser.Lookup" sfwMethodName="LoadUser" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lbluser_Id" Text="User ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtuser_id" sfwDataField="user_id" sfwQueryID="cdoUser" Width="250px" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lbllast_name" Text="Last Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtlast_name" sfwDataField="last_name" sfwQueryID="cdoUser" Width="250px" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblfirst_name" Text="First Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtfirst_name" sfwDataField="first_name" sfwQueryID="cdoUser" Width="250px" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearchCriteriaRequired_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmUsersMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmUsersMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintUserSerialId=UserSerialId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="UserSerialId" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="User ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="UserId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="User Serial ID" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UserSerialId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="First Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="FirstName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Middle Initial">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="MiddleInitial" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Last Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="LastName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="User Type Description">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UserTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="User Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UserStatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Begin Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="BeginDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="End Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EndDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created By" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created Date" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedDate" sfwDataFormat="{0:d}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified By" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ModifiedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified Date" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ModifiedDate" sfwDataFormat="{0:d}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Update Seq" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="UpdateSeq" sfwShowZero="True" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>