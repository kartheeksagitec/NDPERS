<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmUsersMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="501" sfwType="Maintenance" sfwTitle="Users Maintenance" version="3.5" sfwHelpFile="Assign Roles=Assign Roles.htm;Create New Roles=Create New Roles.htm;Delete Role=Delete Role.htm;Modify Roles=Modify Roles.htm;Create New User=Create New User.htm;Modify User=Modify User.htm;Delete User=Delete User.htm;Security Overview=Security Overview.htm" sfwEntity="entUser">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewUser" />
    <callmethods sfwMode="Update" sfwMethodName="FindUser" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable CellPadding="3" ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintUserSerialId=UserSerialId" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="User Information">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="cap1" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capUserID" Text="User ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtuser_id" sfwMode="New" sfwEntityField="UserId" />
                <sfwLabel ID="lblUserId" sfwMode="Update" sfwEntityField="UserId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capPersonId" Text="PERSLink ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId" sfwEntityField="PersonId" />
                <sfwImageButton ID="btnRetrieve" ImageAlign="AbsMiddle" ImageUrl="~/Image/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtPersonId" sfwSelectedIndex="PersonId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capEmailAddress" Text="Email Address : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEmailAddress" sfwEntityField="EmailAddress" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel SkinID="ReqInformation" Text="*" sfwIsCaption="True" ID="lblLabel1" />
                <sfwLabel ID="capFirstName" Text="First Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtfirst_name" sfwMode="New" sfwEntityField="FirstName" />
                <sfwLabel ID="lblFirstName" sfwMode="Update" sfwEntityField="FirstName" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capMiddleInitial" Text="Middle Initial :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtMiddle_name" sfwMode="New" sfwEntityField="MiddleInitial" />
                <sfwLabel ID="lblMiddleName" sfwMode="Update" sfwEntityField="MiddleInitial" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel SkinID="ReqInformation" Text="*" sfwIsCaption="True" ID="lblLabel2" />
                <sfwLabel ID="capLastName" Text="Last Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txLast_name" sfwMode="New" sfwEntityField="LastName" />
                <sfwLabel ID="lblLastName" sfwMode="Update" sfwEntityField="LastName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel SkinID="ReqInformation" Text="*" sfwIsCaption="True" ID="lblLabel3" />
                <sfwLabel ID="capBeginDate" Text="Begin Date :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtbegin_date" sfwDataFormat="{0:d}" sfwEntityField="BeginDate" />
                <CompareValidator ControlToValidate="txtbegin_date" ErrorMessage="Invalid Begin Date" ID="id" Operator="DataTypeCheck" SetFocusOnError="True" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capEndDate" Text="End Date :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtend_date" sfwDataFormat="{0:d}" sfwEntityField="EndDate" />
                <CompareValidator ControlToValidate="txtend_date" ErrorMessage="Invalid End Date" ID="id1" Operator="DataTypeCheck" SetFocusOnError="True" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capLogActivity" Text="Log Activity :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwRadioButtonList ID="idLogActivityFlag" RepeatDirection="Horizontal" sfwLoadType="Items" sfwEntityField="LogActivityFlag">
                  <ListItem Text="No" Value="N" Selected="True" />
                  <ListItem Text="Yes" Value="Y" />
                </sfwRadioButtonList>
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel SkinID="ReqInformation" Text="*" sfwIsCaption="True" ID="lblLabel4" />
                <sfwLabel ID="capUserType" Text="User Type :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="txtuser_type_value" sfwLoadType="CodeGroup" sfwLoadSource="9" sfwEntityField="UserTypeValue" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn>
                <sfwLabel ID="lblUserSerialId" sfwEntityField="UserSerialId" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capDivision" Text="Division :" Visible="False" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlDivisionValue" Visible="False" sfwLoadType="CodeGroup" sfwLoadSource="53" sfwEntityField="DivisionValue" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capSupervisorId" Text="Supervisor ID : " AssociatedControlID="txtSupervisorId" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtSupervisorId" sfwEntityField="SupervisorId" sfwRetrievalQuery="entUser.GetUserDetailsByUserSerialId" sfwParameters="user_serial_id=txtSupervisorId" sfwRetrievalControls="lblUserFullName=user_full_name" />
                <sfwImageButton ID="btnRetrieve1" sfwMethodName="btnRetrieve_Click" ImageUrl="~/Images/Lookup.jpg" ImageAlign="AbsMiddle" sfwActiveForm="wfmUsersLookup" sfwSelectedIndex="UserSerialId" sfwRelatedControl="txtSupervisorId" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblSupervisorName" Text="Supervisor Name :" AssociatedControlID="txtUserTitle" CssClass="LeftAlignedLabel" SkinID="LeftAlignedLabel" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblUserFullName" sfwEntityField="objSupervisor.UserFullname1" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel SkinID="ReqInformation" Text="*" sfwIsCaption="True" ID="lblLabel5" />
                <sfwLabel ID="capUserStatus" Text="User Status :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="txtuser_status_value" sfwLoadType="CodeGroup" sfwLoadSource="10" sfwEntityField="UserStatusValue" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capColorScheme" Text="Color Scheme :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlColorScheme" sfwLoadType="Items" sfwEntityField="ColorScheme" sfwDefaultValue="Green">
                  <ListItem Text="Green" Value="Green" Selected="True" />
                  <ListItem Text="Blue" Value="Blue" />
                </sfwDropDownList>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlDetails" sfwCaption="Other Details">
          <sfwTable ID="tblDetails">
            <sfwRow>
              <sfwColumn>
                <sfwTabContainer ID="tctDetails">
                  <Tabs>
                    <sfwTabSheet HeaderText="Roles" ID="tshUerRoles">
                      <sfwTable ID="tblUerRoles">
                        <sfwRow>
                          <sfwColumn>
                            <sfwButton ID="btnNew" sfwActiveForm="wfmUserRoleMaintenance" sfwMethodName="btnNew_Click" sfwMode="Update" sfwNavigationParameter="aintUserSerialId=lblUserSerialId" Text="New" />
                            <sfwButton ID="btnOpen" sfwActiveForm="wfmUserRoleMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintUserSerialId=UserSerialId;aintRoleId=RoleId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                            <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="UserSerialId;RoleId" sfwRelatedControl="dgrResult" Text="Delete" />
                          </sfwColumn>
                        </sfwRow>
                        <sfwRow>
                          <sfwColumn>
                            <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many" sfwEntityField="lstUserRoles" sfwGridEntity="entUserRoles">
                              <Columns>
                                <TemplateField HeaderText="Role">
                                  <ItemTemplate>
                                    <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="RoleDescription" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="User Serial Id" Visible="False">
                                  <ItemTemplate>
                                    <sfwLabel sfwEntityField="UserSerialId" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="Role Id" Visible="False">
                                  <ItemTemplate>
                                    <sfwLabel sfwEntityField="RoleId" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="Start Date">
                                  <ItemTemplate>
                                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveStartDate" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="End Date">
                                  <ItemTemplate>
                                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveEndDate" />
                                  </ItemTemplate>
                                </TemplateField>
                              </Columns>
                            </sfwGridView>
                          </sfwColumn>
                        </sfwRow>
                      </sfwTable>
                    </sfwTabSheet>
                    <sfwTabSheet HeaderText="Security" ID="tshUserSecurity">
                      <sfwTable ID="tblUserSecurity">
                        <sfwRow>
                          <sfwColumn>
                            <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrSecurity" sfwEntityField="lstSecurity" sfwGridEntity="entSecurity">
                              <Columns>
                                <TemplateField HeaderText="Resource">
                                  <ItemTemplate>
                                    <sfwLabel sfwEntityField="objResources.ResourceDescription" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="Resource Type">
                                  <ItemTemplate>
                                    <sfwLabel sfwEntityField="objResources.ResourceTypeDescription" />
                                  </ItemTemplate>
                                </TemplateField>
                                <TemplateField HeaderText="Security Level">
                                  <ItemTemplate>
                                    <sfwLabel sfwEntityField="SecurityDescription" />
                                  </ItemTemplate>
                                </TemplateField>
                              </Columns>
                            </sfwGridView>
                          </sfwColumn>
                        </sfwRow>
                      </sfwTable>
                    </sfwTabSheet>
                  </Tabs>
                </sfwTabContainer>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwTwoWayBinding="True" Visible="False" sfwEntityField="UpdateSeq" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="lstUserRoles" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
    <entityfield ID="lstSecurity" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>