                <CompareValidator ControlToCompare="txtDeceasedDateFrom" ControlToValidate="txtDeceasedDateTo" ErrorMessage="Deceased Date To cannot be less than Deceased Date From." ID="valDeceasedDateRange" Operator="GreaterThanEqual" Text="*" Type="Date" />
              </sfwColumn>
