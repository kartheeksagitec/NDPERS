                <sfwButton ID="btnOpen" sfwActiveForm="wfmDeathNotificationMaintenance" sfwCopyActivitySessionVariables="True" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintdeathnotificationid=DeathNotificationId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
