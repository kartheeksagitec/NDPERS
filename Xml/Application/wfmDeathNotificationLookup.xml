                <sfwTextBox ID="txtDeceasedDateFrom" sfwDataField="sdn.date_of_death" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoDeathNotification" sfwRelatedControl="txtDeceasedDateTo" />
                <CompareValidator ControlToValidate="txtDeceasedDateFrom" ErrorMessage="Deceased Date From must be in mm/dd/yyyy format." ID="valDeceasedDateFrom" Operator="DataTypeCheck" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capDeceasedDateTo" Text="Date of Deaths To : " sfwIsCaption="True" />
