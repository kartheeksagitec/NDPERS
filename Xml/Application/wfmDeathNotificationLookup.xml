<?xml version="1.0" encoding="utf-8"?>
  <form ID="wfmDeathNotificationLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvApplication" sfwResource="1906 " sfwType="Lookup" sfwTitle="Death Notification Lookup" sfwMaxCount="300" version="3.5" sfwEntity="entDeathNotification" sfwGridCollection="busDeathNotificationLookup.iclbDeathNotification">
