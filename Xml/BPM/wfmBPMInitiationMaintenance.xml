<form ID="wfmBPMInitiationMaintenance" sfwActive="True" sfwStatus="Valid" sfwType="Maintenance" sfwTitle="Online Case Initiation Maintenance" version="3.5" sfwEntity="entSolBpmRequest" sfwResource="2065">
  <initialload>
    <callmethods sfwMethodName="NewBpmRequest" sfwMode="New" />
    <query />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMessageId="1556" sfwMethodName="btnValidateExecuteBusinessMethod_Click" sfwObjectMethod="InitializeProcess" Text="Initiate Process" sfwSecurityLevel="2" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="Request Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="lblLabel" CssClass="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel CssClass="LeftAlignedLabel" ID="capProcessId" SkinID="LeftAlignedLabel" Text="Process Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="name" DataValueField="process_id" ID="ddlProcessId" AutoPostBack="True" sfwLoadType="Query" sfwLoadSource="entBpmProcess.GetProcessesForOnlineInitialization" sfwEntityField="ProcessId" />
                <sfwButton ID="btnRefreshParameters" sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Refresh Parameters" sfwObjectMethod="LoadRequestParametersForOnlineInitiation" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capPersonId" SkinID="LeftAlignedLabel" Text="PERSLink ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtPersonId" sfwParameters="person_id=txtPersonId" sfwRetrievalControls="lblPersonName=personname" sfwRetrievalQuery="entPerson.GetPersonDetailsByPersonId" sfwDataType="Numeric" sfwEntityField="PersonId" />
                <sfwImageButton ID="btnRetrievePerson" ImageAlign="AbsMiddle" ImageUrl="~/Images/Lookup.jpg" sfwActiveForm="wfmPersonLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtPersonId" sfwSelectedIndex="PersonId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capPersonName" SkinID="LeftAlignedLabel" Text="Person Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblPersonName" sfwEntityField="objPerson.IstrFullName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capOrgId" SkinID="LeftAlignedLabel" Text="Org Code ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtIstrOrgCode" sfwEntityField="IstrOrgCode" sfwRetrievalQuery="entOrganization.GetOrgFromOrgCode" sfwParameters="OrgCode=txtIstrOrgCode" sfwRetrievalControls="txtOrgId=org_id;lblOrgNameRet=org_name" />
                <sfwImageButton ID="btnRetrieveOrganization" ImageAlign="AbsMiddle" ImageUrl="~/Images/Lookup.jpg" sfwActiveForm="wfmOrganizationLookup" sfwMethodName="btnRetrieve_Click" sfwRelatedControl="txtIstrOrgCode" sfwSelectedIndex="OrgCode" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="hideControl" ID="capOrgId" SkinID="LeftAlignedLabel" Text="Organization ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgId" sfwDataType="Numeric" sfwEntityField="OrgId" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capOrgName" SkinID="LeftAlignedLabel" Text="Organization Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblOrgNameRet" sfwEntityField="objOrganization.OrgName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel CssClass="LeftAlignedLabel" ID="capReferenceId" SkinID="LeftAlignedLabel" Text="Reference ID :" sfwIsCaption="True" Visible="False" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtReferenceId" sfwEntityField="ReferenceId" Visible="False" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capIstrOverrideCheckInstance" Text="Allow Multiple Instances" AssociatedControlID="chkIstrOverrideCheckInstance" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="chkIstrOverrideCheckInstance" sfwEntityField="CheckForExistingInst" sfwValueChecked="N" sfwValueUnChecked="Y" sfwDefaultValue="Y" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPanel" sfwCaption="Request Parameters">
          <sfwTable CssClass="Table" ID="tblNewPage">
            <sfwRow>
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvbusSolBpmRequestParameter" sfwTwoWayBinding="True" AllowEditing="True" sfwEntityField="lstBpmRequestParameter" sfwGridEntity="entBpmRequestParameter">
                  <Columns>
                    <TemplateField HeaderText="Parameter Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ParameterName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Parameter">
                      <ItemTemplate>
                        <sfwTextBox sfwEntityField="ParameterValue" sfwVisibleRule="Visible Rule - ShowTextBox" />
                        <sfwDropDownList sfwEntityField="ParameterValue" sfwLoadType="Query" sfwVisibleRule="Visible Rule - ShowDropDown" sfwLoadSource="entFramework.GetCodeValuesForMapParameters" sfwNavigationParameter="code_id=CodeGroupId" DataTextField="description" DataValueField="code_value" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Request Parameter ID" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="RequestParameterId" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="objPerson" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objOrganization" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstBpmRequestParameter" sfwNewAutoLoad="True" sfwUpdateAutoLoad="True" />
  </loaddetails>
</form>