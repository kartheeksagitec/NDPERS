<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmFileHdrMaintenance" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="409 " sfwType="Maintenance" sfwTitle="File Header Maintenance" sfwHelpFile="Batch Overview=Batch Overview.htm;Lookup Update Processed Files=Lookup Update Processed Files.htm" version="3.5" sfwEntity="entFileHdr">
  <initialload>
    <callmethods sfwMethodName="FindFileHdr" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable CellPadding="3" ID="toolbar">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintFileHdrId=FileHdrId" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton ID="btnCorrespondence" sfwMethodName="btnCorrespondence_Click" Visible="False" Text="Correspondence" />
                <sfwButton ID="idsfwImageButton" sfwActiveForm="wfmAuditLogMaintenance" sfwMethodName="btnAuditHistory_Click" sfwNavigationParameter="aintPrimaryKey=FileHdrId;astrFormName=#wfmFileHdrMaintenance" Visible="False" Text="idsfwImageButton" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlMain" sfwCaption="File Details">
          <sfwTable CssClass="Table" ID="tblMain">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capFileType" Text="File Type :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblDescription" sfwEntityField="objFile.Description" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatus" Text="Status :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblStatusDescription" sfwEntityField="StatusDescription" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capOriginalFileName" Text="Original File Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblMailboxFileName" sfwEntityField="MailboxFileName" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capFileName" Text="File Name :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblProcessedFileName" sfwEntityField="ProcessedFileName" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capProcessedDate" Text="Processed Date :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblProcessedDate" sfwEntityField="ProcessedDate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capNumberOfRows" Text="Number Of Rows :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblNoOfRows" sfwEntityField="NoOfRows" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCycleNumber" Text="Cycle Number :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCycleNo" sfwEntityField="CycleNo" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capFileHdrID" Text="File Hdr ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblFileHdrId" sfwEntityField="FileHdrId" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capErrorMessage" Text="Error Message :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn ColumnSpan="3">
                <sfwLabel ID="lblErrorMessage" sfwWrap="True" sfwEntityField="ErrorMessage" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capErrorResolved" Text="Error Resolved :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="idStatusValue" sfwValueChecked="RSLV" sfwValueUnChecked="ERRO" sfwEntityField="StatusValue" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capReprocessFile" Text="Reprocess File :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwCheckBox ID="idStatusValue1" sfwValueChecked="UPLD" sfwEntityField="StatusValue" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capComments" Text="Comments :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn ColumnSpan="3">
                <sfwTextBox ID="txtComments" Rows="5" TextMode="MultiLine" Width="400px" sfwEntityField="Comments" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Status Summary">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnOpenFileDtl" sfwActiveForm="wfmFileDtlLookup" sfwMethodName="btnOpenLookup_Click" sfwNavigationParameter="a.status_value=StatusValue;a.file_hdr_id=FileHdrId" sfwRelatedControl="dgrResult" Visible="False" Text="OpenFileDtl" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwEntityField="lstStatusSummary" sfwGridEntity="entbusStatusSummary">
                  <Columns>
                    <TemplateField HeaderText="Status">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenFileDtl" sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status Count">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusCount" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status" Visible="False">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusValue" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable CssClass="Table" ID="tblAuditInfo">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Create :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwTwoWayBinding="True" Visible="False" sfwEntityField="UpdateSeq" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modify :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails>
    <entityfield ID="objFile" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
    <entityfield ID="lstStatusSummary" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>