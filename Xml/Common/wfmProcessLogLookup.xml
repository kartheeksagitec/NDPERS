<form ID="wfmProcessLogLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="406" sfwType="Lookup" sfwTitle="Process Log Lookup" sfwMaxCount="1000" sfwHelpFile="Batch Overview=Batch Overview.htm;Lookup a Process Log=Lookup a Process Log.htm" version="3.5" sfwEntity="entProcessLog" sfwGridCollection="icolProcessLog">
  <initialload>
    <query ID="cdoProcessLog" sfwQueryRef="entProcessLog.LookUp" sfwMethodName="LoadProcessLog" sfwReturnType="Sagitec.BusinessObjects.busProcessLogLookup" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capProcessLogIdFrom" Text="Process Log ID From : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtProcessLogIdFrom" sfwDataField="process_log_id" sfwDataType="Numeric" sfwOperator="between" sfwQueryID="cdoProcessLog" sfwRelatedControl="txtProcessLogIdTo" />
                <CompareValidator ControlToValidate="txtProcessLogIdFrom" ErrorMessage="Process Log ID From must be an Integer value." ID="valProcessLogIdFrom" Operator="DataTypeCheck" Text="*" Type="Integer" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capProcessLogIdTo" Text="Process Log ID To : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtProcessLogIdTo" sfwDataField="process_log_id" sfwDataType="Numeric" sfwOperator="between" sfwQueryID="cdoProcessLog" />
                <CompareValidator ControlToValidate="txtProcessLogIdTo" ErrorMessage="Process Log ID To must be an Integer value." ID="valProcessLogIdTo" Operator="DataTypeCheck" Text="*" Type="Integer" />
                <CompareValidator ControlToCompare="txtProcessLogIdFrom" ControlToValidate="txtProcessLogIdTo" ErrorMessage="Process Log ID To cannot be less than Process Log ID From." ID="valProcessLogIdRange" Operator="GreaterThanEqual" Text="*" Type="Integer" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capProcessName" Text="Process Name :" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txbProcessName" sfwDataField="process_name" sfwQueryID="cdoProcessLog" Width="250px" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capMessageType" Text="Message Type :" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="txbMessageType" sfwDataField="message_type_value" sfwQueryID="cdoProcessLog" Width="250px" sfwLoadType="CodeGroup" sfwLoadSource="28" DataTextField="description" DataValueField="code_value" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCycleNumber" Text="Cycle Number :" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txbCycleNo" sfwDataField="cycle_no" sfwDataType="Numeric" sfwQueryID="cdoProcessLog" Width="250px" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capUserId" Text="User ID :" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedBy" sfwDataField="created_by" sfwQueryID="cdoProcessLog" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateFrom" Text="Log Date From : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateFrom" sfwDataField="convert(datetime, convert(varchar(10), created_date, 101))" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoProcessLog" sfwRelatedControl="txtCreatedDateTo" />
                <CompareValidator ControlToValidate="txtCreatedDateFrom" ErrorMessage="Created Date From must be in mm/dd/yyyy format." ID="cmv" Operator="DataTypeCheck" Text="*" Type="Date" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCreatedDateTo" Text="Log Date To : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedDateTo" sfwDataField="convert(datetime, convert(varchar(10), created_date, 101))" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="between" sfwQueryID="cdoProcessLog" />
                <CompareValidator ControlToValidate="txtCreatedDateTo" ErrorMessage="Created Date To must be in mm/dd/yyyy format." ID="cmv1" Operator="DataTypeCheck" Text="*" Type="Date" />
                <CompareValidator ControlToCompare="txtCreatedDateFrom" ControlToValidate="txtCreatedDateTo" ErrorMessage="Created Date To cannot be less than Created Date From." ID="cmv2" Operator="GreaterThanEqual" Text="*" Type="Date" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capMessage" Text="Message :" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txbMessage" sfwDataField="message" sfwQueryID="cdoProcessLog" Width="250px" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreatedTimeFrom" Text="Time From : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedTimeFrom" sfwDataField="convert(varchar(10), created_date, 108) " sfwDataFormat="{0:HH:mm:ss}" sfwOperator="between" sfwQueryID="cdoProcessLog" sfwRelatedControl="txtCreatedTimeTo" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capCreatedTimeTo" Text="Time To : " />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtCreatedTimeTo" sfwDataField="convert(varchar(10), created_date, 108) " sfwDataFormat="{0:HH:mm:ss}" sfwOperator="between" sfwQueryID="cdoProcessLog" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" PageSize="200" sfwObjectID="busProcessLogLookup.icolProcessLog" sfwEntityField="busProcessLogLookup.icolProcessLog">
                  <Columns>
                    <TemplateField HeaderText="User ID" SortExpression="created_by">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="DateTime" SortExpression="created_date">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedDate" sfwDataFormat="{0:G}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Process Name" SortExpression="process_name">
                      <ItemTemplate>
                        <sfwLabel Width="100%" sfwEntityField="ProcessName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderStyle.Width="150px" HeaderText="Message Type" SortExpression="message_type_description">
                      <ItemTemplate>
                        <sfwLabel sfwWrap="True" Width="100%" sfwEntityField="MessageTypeDescription" CssClass="noWrap" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Message" SortExpression="message">
                      <ItemTemplate>
                        <sfwLabel sfwWrap="True" sfwEntityField="Message" CssClass="process-log-mesg wrap-comment" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="ID" SortExpression="process_log_id">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ProcessLogId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Cycle No" SortExpression="cycle_no">
                      <ItemTemplate>
                        <sfwLabel sfwDataField="cycle_no" sfwEntityField="CycleNo" sfwShowZero="True" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
</form>