<form ID="wfmReportClientMVVM" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvReports" sfwResource="-1" sfwType="Maintenance" sfwTitle="Report Client" version="3.5" sfwDefaultButtonID="btnGenerateReport">
  <initialload />
  <centerleft>
    <sfwTreeView ID="trvCenterLeft">
      <Nodes />
    </sfwTreeView>
  </centerleft>
  <sfwTable asp="ASPControl" ID="Main" swc="SagitecWebControls">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="ReportsSection" sfwCaption="Reports">
          <sfwTable CssClass="Table" ID="tblNewPage">
            <sfwRow>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlReportCategories" ID="lblReportCategories" Text="Select Category :" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlReportCategories" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel AssociatedControlID="ddlReports" ID="lblLabel" Text="Select Report :" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlReports" />
              </sfwColumn>
              <sfwColumn ColumnSpan="1">
                <sfwButton ID="btnGenerateReport" sfwMethodName="btnGenerateReport_Click" Text="View" OnClientClick="ReportHardErrorShow(e)" />
                <sfwButton ID="btnSaveReport" sfwMethodName="btnSaveReport_Click" Text="Save" Visible="False" CssClass="hideControl" />
                <sfwButton sfwMethodName="btnDownload_Click" Text="SaveReport" ID="btnDownload" sfwExecuteMethodType="ObjectMethod" sfwTriggerPostBack="True" OnClientClick="ReportOffline()" />
              </sfwColumn>
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn />
              <sfwColumn />
              <sfwColumn>
                <sfwLabel ID="lblEmailAddress" Text="Email ID :" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEmailAddress" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblSecureData" Text="Include Secure Data :" CssClass="hideControl" />
                <sfwCheckBox ID="chkSecureDataIncluded" sfwDefaultValue="N" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>