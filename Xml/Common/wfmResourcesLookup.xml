<?xml version="1.0" encoding="utf-8"?>
  <form ID="wfmResourcesLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvAdmin" sfwResource="502 " sfwType="Lookup" sfwTitle="Resources Lookup" sfwHelpFile="Assign Roles=Assign Roles.htm;Create New Roles=Create New Roles.htm;Delete Role=Delete Role.htm;Modify Roles=Modify Roles.htm;Create New User=Create New User.htm;Modify User=Modify User.htm;Delete User=Delete User.htm;Security Overview=Security Overview.htm" version="3.5" sfwEntity="entResources" sfwGridCollection="busResourcesLookup.iclbLookupResult">
  <initialload>
    <query ID="cdoResources" sfwQueryRef="entResources.Lookup" sfwMethodName="LoadResources" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capResourceDescription" Text="Resource Description :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtrole_description" sfwDataField="resource_description" sfwQueryID="cdoResources" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capResourceID" Text="Resource ID :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtrole_id" sfwDataField="resource_id" sfwQueryID="cdoResources" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capType" Text="Type :" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlResourceTypeValue" sfwDataField="resource_type_value" sfwQueryID="cdoResources" sfwLoadType="CodeGroup" sfwLoadSource="12" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearchCriteriaRequired_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmResourcesMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmResourcesMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintResourceId=ResourceId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="ResourceId" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Resource Description">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="ResourceDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ResourceTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Resource ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ResourceId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created By" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="CreatedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Created Date" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified By" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="ModifiedBy" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Modified Date" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="ModifiedDate" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>