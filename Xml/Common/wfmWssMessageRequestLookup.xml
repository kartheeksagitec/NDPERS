<form ID="wfmWssMessageRequestLookup" WebSite="NeoSpinWebClient" sfwResource="2025" sfwRemoteObject="srvNeoSpinWSS" sfwType="Lookup" sfwTitle="Wss Message Request Lookup" sfwActive="True" sfwStatus="Review" sfwEntity="entWssMessageHeader" sfwGridCollection="busWSSMessageHeaderLookup.iclbWssMessageHeader">
  <initialload>
    <query ID="cdoWssMessageHeader" sfwQueryRef="entWssMessageHeader.LoadWssMessageRequest" sfwMethodName="LoadWSSMessageHeaders" />
  </initialload>
  <sfwTable>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capWssMessageId" Text="Wss Message ID : " AssociatedControlID="txtWssMessageId" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtWssMessageId" sfwDataField="HED.wss_message_id" sfwQueryID="cdoWssMessageHeader" sfwDataType="Numeric" sfwOperator="=" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capMemberTypeId" Text="Member Type ID : " AssociatedControlID="ddlMemberTypeId" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlMemberTypeId" sfwDataField="member_type_value" sfwQueryID="cdoWssMessageHeader" DataTextField="description" DataValueField="code_value" sfwLoadType="CodeGroup" sfwLoadSource="7008" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" sfwOperateOn="tblCriteria" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" sfwOperateOn="tblCriteria" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" sfwOperateOn="tblCriteria" Text="Store Search" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable ID="tblResult" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" ImageUrl="~/Image/gNew.gif" sfwMethodName="btnNew_Click" sfwOperateOn="tblCriteria" sfwActiveForm="wfmWssMessageRequestMaintenance" Text="New" sfwNavigationParameter="astrMemberType=ddlMemberTypeId" />
                <sfwButton ID="btnOpen" ImageUrl="~/Image/gOpen.gif" sfwMethodName="btnOpen_Click" sfwRelatedControl="dgrResult" sfwActiveForm="wfmWssMessageRequestMaintenance" Text="Open" sfwNavigationParameter="aintwssmessageid=WssMessageId" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" ImageUrl="~/Image/gDelete.gif" sfwMethodName="btnDelete_Click" sfwRelatedControl="dgrResult" Text="Delete" Visible="False" />
                <sfwButton ID="btnExportExcel" ImageUrl="~/Image/gExport.gif" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView ID="dgrResult" AllowPaging="True" AllowSorting="True" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Wss Message ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="WssMessageId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Message Text">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="MessageText" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Plan ID">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="PlanId" sfwShowZero="True" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Member Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="MemberTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>