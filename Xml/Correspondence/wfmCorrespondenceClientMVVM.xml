<form ID="wfmCorrespondenceClientMVVM" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvCommon" sfwResource="-1" sfwType="Maintenance" sfwTitle="Communication Client" version="3.5">
  <initialload />
  <centerleft>
    <sfwTreeView ID="trvCenterLeft">
      <Nodes />
    </sfwTreeView>
  </centerleft>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPanel" sfwCaption="Communication">
          <sfwTable CssClass="Table" ID="tblNewPage">
            <sfwRow>
              <sfwColumn ColumnSpan="2">
                <sfwLabel AssociatedControlID="ddlCorrespondenceList" ID="lblLabel" Text="Letter :" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlCorrespondenceList" sfwAddEmptyItem="False" OnClientChange="CorrespondenceSelectedIndexChanged" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnGenerateCorrespondence" sfwMethodName="btnGenerateCorrespondence_Click" Text="Generate" OnClientClick="ShowWait();" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnEditCorrespondence" sfwMethodName="btnEditCorrespondence_Click" Text="Edit" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnImageCorrespondence" sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Image" sfwExecuteMethodType="ObjectMethod" OnClientClick="CorrespondenceImage()" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton sfwMethodName="btnDownload_Click" Text="View" ID="btnViewCorrespondence" sfwObjectMethod="DownloadCorTracking" sfwExecuteMethodType="ObjectMethod" CssClass="hideControl" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton sfwMethodName="btnValidateExecuteBusinessMethod_Click" Text="Publish to WSS" ID="btnExecuteRefreshFromObject" sfwExecuteMethodType="ObjectMethod" OnClientClick="PublishToWSS()" CssClass="hideControl" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <sfwTable ID="Main" />
  <loaddetails />
</form>