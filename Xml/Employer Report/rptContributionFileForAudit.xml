<form ID="rptContributionFileForAudit" sfwType="Report" sfwActive="True" sfwStatus="Review" sfwDescription="Contribution File For Audit" sfwReportName="rptContributionFileForAudit.rpt" sfwReportCategory="704" version="4.0" sfwResource="704">
  <initialload>
    <query sfwQueryRef="entEmployerPayrollDetail.GetContributionFileForAudit" ID="entEmployerPayrollDetail" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwLabel Text="Posted Date From (mm/dd/yyyy) :" sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="POSTED_DATE_FROM" sfwDataFormat="{0:d}" Width="100px" />
        <RequiredFieldValidator ControlToValidate="POSTED_DATE_FROM" ErrorMessage="Posted Date From is Required" Text="*" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel Text="Posted Date To (mm/dd/yyyy) :" sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwTextBox ID="POSTED_DATE_TO" sfwDataFormat="{0:d}" Width="100px" />
        <RequiredFieldValidator ControlToValidate="POSTED_DATE_TO" ErrorMessage="Posted Date To is Required" Text="*" />
        <CompareValidator ControlToCompare="POSTED_DATE_FROM" ControlToValidate="POSTED_DATE_TO" ErrorMessage="Posted Date To cannot be less than Posted Date From" Operator="GreaterThanEqual" Text="*" Type="Date" />
      </sfwColumn>
      <sfwColumn>
        <sfwLabel Text="Plan :" sfwIsCaption="True" />
      </sfwColumn>
      <sfwColumn>
        <sfwMultiSelectDropDownList ID="ddlPlan" DataTextField="plan_name" DataValueField="plan_id" sfwLoadSource="entPlan.Lookup" sfwLoadType="Query" />
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <ExtraFields />
</form>