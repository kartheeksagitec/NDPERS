<form ID="wfmContactOrganizationLookup" sfwType="Lookup" sfwEntity="entOrganization" sfwTitle="Organization Lookup" sfwActive="True" sfwStatus="Review" sfwAspNetCaching="False">
  <initialload>
    <query ID="cdoOrganization" sfwQueryRef="entOrganization.Lookup" />
    <query ID="LookupSubQuery" sfwQueryRef="entOrganization.LookupSubQuery" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capOrgCode" Text="Org Code ID: " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgCode" sfwDataField="org_code" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capOrgName" Text="Name : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtOrgName" sfwDataField="org_name" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel BackColor="White" BorderColor="White" ForeColor="Red" ID="lblLabel" SkinID="ReqInformation" Text="*" sfwIsCaption="True" />
                <sfwLabel ID="capOrgTypeValue" Text="Org Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="txtOrgTypeValue" sfwDataField="org_type_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwDefaultValue="EMPL">
                  <ListItem Text="Employer" Value="EMPL" />
                </sfwDropDownList>
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capEmpCategoryValue" Text="Employer Type : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="txtEmpCategoryValue" sfwDataField="emp_category_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwLoadType="CodeGroup" sfwLoadSource="203" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Plan :" sfwIsCaption="True" ID="capPlan" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="plan_name" DataValueField="plan_id" ID="ddlPlan" sfwDataField="plan_id" sfwOperator="=" sfwQueryID="LookupSubQuery" sfwLoadType="Query" sfwLoadSource="entPlan.Lookup" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capStatusValue" Text="Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="a.status_value" sfwOperator="like" sfwQueryID="cdoOrganization" sfwDefaultValue="ACTV">
                  <ListItem Text="Active" Value="ACTV" />
                  <ListItem Text="Pending" Value="PEND" />
                </sfwDropDownList>
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capRoutingNo" Text="Routing No : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtRoutingNo" sfwDataField="routing_no" sfwOperator="like" sfwQueryID="cdoOrganization" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Results">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmOrganizationMaintenance" sfwMethodName="btnNew_Click" sfwNavigationParameter="astrOrgTypeValue=txtOrgTypeValue;astrEmployerTypeValue=txtEmpCategoryValue" Text="New" sfwCopyActivitySessionVariables="True" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmOrganizationMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="Aintorgid=OrgId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="OrgId" sfwRelatedControl="dgrResult" sfwResource="323" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Org ID" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Code ID">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="OrgCode" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Name">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgName" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Address">
                      <ItemTemplate>
                        <sfwLabel sfwWrap="True" sfwEntityField="objOrgPrimaryAddress.AddrDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Org Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="OrgTypeDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Employer Type">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="EmpCategoryDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Main Phone">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:(###)###-####}" sfwEntityField="Telephone" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Routing No">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="RoutingNo" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>