    <query ID="ACHPreNoteVerificationBatch" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT &#xD;&#xA;'PE' AS [TYPE],&#xD;&#xA;PE.PAYEE_ACCOUNT_ACH_DETAIL_ID AS ACH_DETAIL_ID,&#xD;&#xA;PE.BANK_ACCOUNT_TYPE_VALUE AS TRANSACTION_CODE,&#xD;&#xA;ORG.ROUTING_NO,&#xD;&#xA;PE.ACCOUNT_NUMBER AS ACCOUNT_NO,&#xD;&#xA;PER1.PERSON_ID  AS PERSON_ID,&#xD;&#xA;PER1.LAST_NAME + ', ' + PER1.FIRST_NAME AS PERSON_NAME&#xD;&#xA;FROM SGT_PAYEE_ACCOUNT_ACH_DETAIL PE WITH (NOLOCK)&#xD;&#xA;LEFT JOIN SGT_ORGANIZATION ORG ON ORG.ORG_ID=PE.BANK_ORG_ID&#xD;&#xA;LEFT JOIN SGT_PAYEE_ACCOUNT SPE ON SPE.PAYEE_ACCOUNT_ID=PE.PAYEE_ACCOUNT_ID&#xD;&#xA;LEFT JOIN SGT_PERSON PER1 ON PER1.PERSON_ID=SPE.PAYEE_PERSLINK_ID&#xD;&#xA;WHERE PE.PRE_NOTE_FLAG='Y'&#xD;&#xA;&#xD;&#xA;UNION ALL&#xD;&#xA;&#xD;&#xA;SELECT &#xD;&#xA;'PA' AS [TYPE],&#xD;&#xA;PA.PERSON_ACCOUNT_ACH_DETAIL_ID AS ACH_DETAIL_ID,&#xD;&#xA;PA.BANK_ACCOUNT_TYPE_VALUE AS TRANSACTION_CODE,&#xD;&#xA;ORG.ROUTING_NO,&#xD;&#xA;PA.BANK_ACCOUNT_NUMBER AS ACCOUNT_NO,&#xD;&#xA;PER.PERSON_ID AS PERSON_ID,&#xD;&#xA;PER.LAST_NAME + ', ' + PER.FIRST_NAME AS PERSON_NAME&#xD;&#xA;FROM SGT_PERSON_ACCOUNT_ACH_DETAIL PA WITH (NOLOCK)&#xD;&#xA;LEFT JOIN SGT_ORGANIZATION ORG ON ORG.ORG_ID=PA.BANK_ORG_ID&#xD;&#xA;LEFT JOIN SGT_PERSON_ACCOUNT SPA ON SPA.PERSON_ACCOUNT_ID=PA.PERSON_ACCOUNT_ID&#xD;&#xA;LEFT JOIN SGT_PERSON PER ON PER.PERSON_ID=SPA.PERSON_ID&#xD;&#xA;WHERE PA.PRE_NOTE_FLAG='Y'&#xD;&#xA;&#xD;&#xA;">
      <parameters />
    </query>
    <query ID="LoadACHDetailsForNextPaymentDate" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT PACD.* FROM SGT_PAYEE_ACCOUNT_ACH_DETAIL PACD&#xD;&#xA; INNER JOIN (SELECT PA.* FROM SGT_PAYEE_ACCOUNT PA &#xD;&#xA;   INNER JOIN SGT_PAYEE_ACCOUNT_STATUS PAS&#xD;&#xA;       ON PAS.PAYEE_ACCOUNT_ID=PA.PAYEE_ACCOUNT_ID &#xD;&#xA;   INNER JOIN SGS_CODE_VALUE CV WITH(NOLOCK)&#xD;&#xA;     ON PAS.STATUS_ID= CV.CODE_ID AND PAS.STATUS_VALUE = CV.CODE_VALUE&#xD;&#xA;   WHERE PA.BENEFIT_BEGIN_DATE&lt;=@PAYMENTDATE&#xD;&#xA;   AND (CV.DATA2 = 'RECV' OR CV.DATA2 = 'APRD' ) &#xD;&#xA;    AND PAS.PAYEE_ACCOUNT_STATUS_ID  = &#xD;&#xA;    (&#xD;&#xA;     SELECT TOP 1 PAYEE_ACCOUNT_STATUS_ID&#xD;&#xA;       FROM SGT_PAYEE_ACCOUNT_STATUS PAS1 (NOLOCK)&#xD;&#xA;       WHERE PAS1.PAYEE_ACCOUNT_ID = PA.PAYEE_ACCOUNT_ID&#xD;&#xA;       AND PAS1.STATUS_EFFECTIVE_DATE &lt;= @PAYMENTDATE&#xD;&#xA;       ORDER BY PAS1.STATUS_EFFECTIVE_DATE DESC,PAS1.PAYEE_ACCOUNT_STATUS_ID DESC&#xD;&#xA;    )&#xD;&#xA;    )PAYEE ON PAYEE.PAYEE_ACCOUNT_ID=PACD.PAYEE_ACCOUNT_ID&#xD;&#xA;  ">
      <parameters>
        <parameter ID="@PAYMENTDATE" sfwDataType="DateTime" />
      </parameters>
    </query>
    <query ID="IsEmailAddressWaived" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT * FROM SGT_PERSON PER&#xD;&#xA;INNER JOIN SGT_PAYEE_ACCOUNT PE ON PE.PAYEE_PERSLINK_ID = PER.PERSON_ID&#xD;&#xA;WHERE PE.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID&#xD;&#xA;AND (ISNULL(PER.EMAIL_WAIVER_FLAG, 'N') = 'N' &#xD;&#xA;AND PER.EMAIL_ADDRESS IS NOT NULL)&#xD;&#xA;">
