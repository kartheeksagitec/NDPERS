    <query ID="GetPaymentItemTypeForRetroItem" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="Select b.* from sgt_retro_Item_type a inner join sgt_payment_item_type b &#xD;&#xA;on a.to_item_type=b.item_type_code where a.FROM_ITEM_TYPE=@retroitem and a.RETRO_PAYMENT_TYPE_VALUE=@retropaymenttype">
