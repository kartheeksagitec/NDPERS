    <query ID="LoadRetroPaymentStateAndFedTaxAmount" sfwQueryType="ScalarQuery" sfwDataType="int" sfwSql="SELECT ISNULL(SUM(PAPD.AMOUNT),0) AS AMOUNT FROM SGT_PAYEE_ACCOUNT_RETRO_PAYMENT_DETAIL PAPD WITH(NOLOCK)&#xA;INNER JOIN SGT_PAYEE_ACCOUNT_RETRO_PAYMENT PARP WITH(NOLOCK) ON PAPD.PAYEE_ACCOUNT_RETRO_PAYMENT_ID = PARP.PAYEE_ACCOUNT_RETRO_PAYMENT_ID&#xA;INNER JOIN SGT_PAYMENT_ITEM_TYPE PIT WITH(NOLOCK) ON PAPD.PAYMENT_ITEM_TYPE_ID = PIT.PAYMENT_ITEM_TYPE_ID&#xA;WHERE PIT.ITEM_TYPE_DIRECTION = 1 AND PIT.TAXABLE_ITEM_FLAG = 'Y' AND PARP.PAYEE_ACCOUNT_ID = @PAYEE_ACCOUNT_ID">
