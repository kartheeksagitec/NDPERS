                    </TemplateField>
                    <TemplateField HeaderText="Percentage" ItemStyle.HorizontalAlign="Right">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="Percentage" CssClass="RightAlign" sfwDataFormat="{0:N2}" />
