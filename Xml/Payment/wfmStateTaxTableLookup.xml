<?xml version="1.0" encoding="utf-8"?>
  <form ID="wfmStateTaxTableLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvPayment" sfwResource="1201 " sfwType="Lookup" sfwTitle="State Tax Table Lookup" version="3.5" sfwEntity="entFedStateTaxRate" sfwGridCollection="busFedStateTaxRateLookup.iclbFedStateTaxRate">
