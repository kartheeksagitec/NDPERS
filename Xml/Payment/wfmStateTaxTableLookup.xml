              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" AutoGenerateColumns="False" ID="grvFedStateTax" sfwSelection="Many" sfwSortExpression="EffectiveDate desc" SortExpressionUpdated="true">
                  <Columns>
                    <TemplateField HeaderText="Effective Date">
                      <ItemTemplate>
                        <sfwLabel sfwDataFormat="{0:d}" sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="EffectiveDate" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Marital Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="MaritalStatusDescription" />
