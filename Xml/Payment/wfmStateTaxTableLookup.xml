  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capFedStateTaxId" Text="State Tax ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtFedStateTaxId" sfwDataField="fed_state_tax_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoFedStateTaxRate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capEffectiveDate" Text="Effective Date From : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox ID="txtEffectiveDate" sfwDataField="effective_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="&gt;=" sfwQueryID="cdoFedStateTaxRate" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capMaritalStatusValue" Text="Marital Status : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlMaritalStatusValue" sfwDataField="marital_status_value" sfwOperator="like" sfwQueryID="cdoFedStateTaxRate" sfwLoadType="Query" sfwLoadSource="entFedStateTaxRate.LoadMaritalStatusForPaymentTaxRatesTable" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
