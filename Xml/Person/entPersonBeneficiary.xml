    <query ID="LoadPlanEnrolledByPerson" sfwQueryType="SelectQuery" sfwDataType="EntityTable" sfwSql="SELECT CAST(ROW_NUMBER() OVER(ORDER BY SPA.PERSON_ACCOUNT_ID) AS INT) AS PERSON_ACCOUNT_BENEFICIARY_ID,&#xD;&#xA; SPA.PERSON_ACCOUNT_ID ,PL.PLAN_NAME AS istrPlanName ,&#xD;&#xA;100.00 as dist_percent, SPA.START_DATE as idtInitialPlanStartDate,&#xD;&#xA;cv1.description as istrPlanParticipationStatus  FROM SGT_PERSON_ACCOUNT SPA&#xD;&#xA;INNER JOIN SGT_PLAN PL ON PL.PLAN_ID=SPA.PLAN_ID&#xD;&#xA;inner join sgs_code_value cv1 on cv1.code_id =PLAN_PARTICIPATION_STATUS_ID and cv1.code_value =  PLAN_PARTICIPATION_STATUS_VALUE &#xD;&#xA;WHERE SPA.PERSON_ID=@person_id AND &#xD;&#xA;PL.PLAN_ID IN (1,2,3,4,5,6,7,10,11,20,21,13,26,27,28,29,30)&#xD;&#xA;and (&#xD;&#xA; (pl.benefit_type_value = 'INSR' AND cv1.data2 in ('ERLD','SPND') )&#xD;&#xA; OR&#xD;&#xA; (pl.benefit_type_value = 'RETR' AND cv1.data2 in ('ERLD','SPND','RTRD'))&#xD;&#xA; )">
      <parameters>
