<?xml version="1.0" encoding="utf-8"?>
<form ID="wfmTestcaseLookup" sfwActive="True" sfwStatus="Valid" sfwRemoteObject="srvScout" sfwResource="805 " sfwType="Lookup" sfwTitle="Testcase Lookup" sfwMaxCount="200" version="3.5" sfwEntity="entTestcase" sfwGridCollection="busTestcaseLookup.iclbLookupResult">
  <initialload>
    <query ID="cdoTestcase" sfwQueryRef="entTestcase.Lookup" sfwMethodName="LoadTestcases" />
  </initialload>
  <sfwTable ID="Main">
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
          <sfwTable CssClass="Table" ID="tblCriteria">
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Testcase Key :" sfwIsCaption="True" ID="capTestCaseKey" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox sfwDataField="testcase_key" sfwQueryID="cdoTestcase" ID="txtTestCaseKey" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblStatus" Text="Status :" Width="50px" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwDropDownList ID="ddlStatusValue" sfwDataField="status_value" sfwQueryID="cdoTestcase" sfwLoadType="CodeGroup" sfwLoadSource="43" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Description :" sfwIsCaption="True" ID="capDescription" />
              </sfwColumn>
              <sfwColumn>
                <sfwTextBox sfwDataField="testcase_desc" sfwQueryID="cdoTestcase" ID="txtTestCaseDesc" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel Text="Usecase :" sfwIsCaption="True" ID="capUsecase" />
              </sfwColumn>
              <sfwColumn ColumnSpan="4">
                <sfwDropDownList DataTextField="usecase_desc" DataValueField="usecase_id" ID="ddlUsecaseId" sfwDataField="usecase_id" sfwQueryID="cdoTestcase" sfwLoadType="Query" sfwLoadSource="entUsecase.Lookup" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
                <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
              </sfwColumn>
              <sfwColumn>
                <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
              </sfwColumn>
              <sfwColumn />
              <sfwColumn />
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlResult" sfwCaption="Search Result">
          <sfwTable CssClass="Table" ID="tblResult">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnNew" sfwActiveForm="wfmTestcaseMaintenance" sfwMethodName="btnNew_Click" Text="New" />
                <sfwButton ID="btnOpen" sfwActiveForm="wfmTestcaseMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintTestcaseId=TestcaseId" sfwRelatedControl="dgrResult" Text="Open" sfwDisableViewOnly="True" />
                <sfwButton ID="btnDelete" sfwMethodName="btnDelete_Click" sfwNavigationParameter="TestcaseId" sfwRelatedControl="dgrResult" Text="Delete" />
                <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
                  <Columns>
                    <TemplateField HeaderText="Testcase Id" Visible="False" sfwShowInExportToExcel="True">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="TestcaseId" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Testcase Key">
                      <ItemTemplate>
                        <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="TestcaseKey" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Description">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="TestcaseDesc" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Status">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="StatusDescription" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <loaddetails />
</form>