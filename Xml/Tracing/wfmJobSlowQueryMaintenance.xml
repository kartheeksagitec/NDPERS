<form ID="wfmJobSlowQueryMaintenance" sfwType="Maintenance" sfwEntity="entJobServiceMonitoring" sfwTitle="Job Schedule Slow Queries" sfwActive="True" sfwStatus="Review">
  <initialload>
    <callmethods sfwMode="Update" sfwMethodName="LoadSlowQueries" />
  </initialload>
  <sfwTable>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnltoolbar">
          <sfwTable ID="toolbar" CellPadding="3">
            <sfwRow>
              <sfwColumn>
                <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" Text="Save" />
                <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
                <sfwButton ID="btnPrev" sfwMethodName="btnPrev_Click" Text="Prev" />
                <sfwButton ID="btnNext" sfwMethodName="btnNext_Click" Text="Next" />
                <sfwButton ID="btnCorrespondence" sfwMethodName="btnCorrespondence_Click" Text="Correspondence" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPanel" sfwCaption="Search Parameters">
          <sfwTable ID="tblNewPage">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capJobScheduleDetailID" Text="Job Schedule Detail ID : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblJobScheduleDetailID" sfwEntityField="JobScheduleDetailID" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capFromDate" Text="FromDate : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblFromDate" sfwEntityField="FromDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capToDate" Text="ToDate : " AssociatedControlID="lblToDate" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblToDate" sfwEntityField="ToDate" sfwDataFormat="{0:G}" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capQueryHash" Text="Query Hash : " AssociatedControlID="lblQueryHash" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn ColumnSpan="5">
                <sfwLabel ID="lblQueryHash" sfwEntityField="QueryHash" />
              </sfwColumn>
            </sfwRow>
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capFirstQuery2" Text="Query : " AssociatedControlID="lblFirstQuery" sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn ColumnSpan="5">
                <sfwLabel ID="lblFirstQuery" sfwEntityField="FirstQuery" ReadOnly="True" Rows="5" TextMode="MultiLine" Width="750px" sfwDataFormat="{0:SQL}" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlNewPanel1" sfwCaption="Job Detail">
          <sfwTable ID="tblNewPage1">
            <sfwRow>
              <sfwColumn>
                <sfwGridView sfwBoundToQuery="True" sfwQueryLoadType="Update" sfwBaseQuery="entJobDetailThQuery.GetSlowQueryDetails" ID="grvSlowQueryDetails" AllowPaging="True" AllowSorting="True" AllowEditing="True" sfwTwoWayBinding="True" sfwDataKeyNames="job_detail_id">
                  <Columns>
                    <TemplateField HeaderText="Job Detail ID" ItemStyle.HorizontalAlign="Center">
                      <ItemTemplate>
                        <sfwLinkButton ID="btnOpen" sfwMethodName="btnOpen_Click" sfwEntityField="job_detail_id" sfwActiveForm="wfmJobDetailTraceMaintenance" sfwNavigationParameter="job_detail_id" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Parameter Details">
                      <ItemTemplate>
                        <sfwJSONData sfwEntityField="parameter_details" RenderAsInline="True" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Start Time" HeaderStyle.HorizontalAlign="Center">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="start_time" sfwDataFormat="{0:G}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="End Time" HeaderStyle.HorizontalAlign="Center">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="end_time" sfwDataFormat="{0:G}" />
                      </ItemTemplate>
                    </TemplateField>
                    <TemplateField HeaderText="Time In (ms)">
                      <ItemTemplate>
                        <sfwLabel sfwEntityField="time_in_ms" />
                      </ItemTemplate>
                    </TemplateField>
                  </Columns>
                  <Parameters>
                    <parameter ID="@start_time" sfwEntityField="FromDate" />
                    <parameter ID="@end_time" sfwEntityField="ToDate" sfwDataType="datetime" />
                    <parameter ID="@job_schedule_detail_id" sfwEntityField="JobScheduleDetailID" />
                    <parameter ID="@query_hash" sfwEntityField="QueryHash" />
                  </Parameters>
                </sfwGridView>
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
    <sfwRow>
      <sfwColumn>
        <sfwPanel ID="pnlAuditInfo" sfwCaption="Audit Information">
          <sfwTable ID="tblAuditInfo" CssClass="Table">
            <sfwRow>
              <sfwColumn>
                <sfwLabel ID="capCreate" Text="Created : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedBy" sfwEntityField="CreatedBy" />
                <sfwLabel ID="lblUpdateSeq" sfwEntityField="UpdateSeq" sfwTwoWayBinding="True" Visible="False" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblCreatedDate" sfwEntityField="CreatedDate" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="capModify" Text="Modified : " sfwIsCaption="True" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedBy" sfwEntityField="ModifiedBy" />
              </sfwColumn>
              <sfwColumn>
                <sfwLabel ID="lblModifiedDate" sfwEntityField="ModifiedDate" />
              </sfwColumn>
            </sfwRow>
          </sfwTable>
        </sfwPanel>
      </sfwColumn>
    </sfwRow>
  </sfwTable>
  <ExtraFields />
  <loaddetails />
</form>