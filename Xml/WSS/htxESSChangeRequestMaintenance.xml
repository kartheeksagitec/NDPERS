<form ID="wfmESSChangeRequestMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2003" sfwType="FormLinkMaintenance" sfwTitle="ESS Change Request Maintenance" version="3.5" sfwDefaultButtonID="btnExecuteRefreshFromObject" sfwEntity="entESSHome" sfwTemplate="ESSChangeRequestMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSHomeLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="FindESSHome" />
    <callmethods sfwMode="Update" sfwMethodName="FindESSHome" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlEmpChangeRequests" sfwCaption="Employee Change Requests">
      <items>
        <sfwTable ID="tblEmpChangeRequests" />
        <sfwButton ID="btnEmpRequestOpen" sfwActiveForm="LOAR=wfmUpdateTermEmploymentLOARecertificationWizard;TEEM=wfmTerminateEmploymentWizard;LOAM=wfmUpdateTermEmploymentLOAMilitaryWizard;CLSC=wfmUpdateTermEmploymentWizard;LOAL=wfmUpdateTermEmploymentLOAWithoutPayWizard;ETCG=wfmUpdateTermEmploymentWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintEmploymentChangeRequestID=EmploymentChangeRequestId;aintemploymentId=PersonEmploymentId;aintemploymentdetailid=PersonEmploymentDetailId;ChangeTypeValue" sfwRelatedControl="grvEmployeeChangeRequests" Text="Open" Visible="False" sfwEntityField="ChangeTypeValue" />
        <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="grvEmployeeChangeRequests" Text="Export To Excel" />
        <sfwGridView ID="grvEmployeeChangeRequests" AllowPaging="True" AllowSorting="True" sfwEntityField="lstWssEmploymentChangeRequest" sfwGridEntity="entWssEmploymentChangeRequest">
          <Columns>
            <TemplateField HeaderText="Request ID">
              <ItemTemplate>
                <sfwLabel sfwRelatedControl="btnEmpRequestOpen" sfwLinkable="True" sfwEntityField="EmploymentChangeRequestId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PERSLink ID">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Last Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPerson.LastName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="First Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPerson.FirstName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Last 4 Digits&lt;/br&gt; of SSN">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPerson.LastFourDigitsOfSSN" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status" SortExpression="StatusValue">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Type Of Change">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ChangeTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Type of Change Value" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ChangeTypeValue" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejection Reason">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RejectionReason" sfwWrap="True" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejected By">
              <ItemTemplate>
                <sfwLabel sfwEntityField="lstrRejectedByName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejected Date">
              <ItemTemplate>
                <sfwLabel sfwEntityField="lstrRejectedDate" sfwDataFormat="{0:d}" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="person_employment_id" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonEmploymentId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="person_employment_detail_id" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonEmploymentDetailId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstWssEmploymentChangeRequest" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>