<form ID="wfmESSContactNDPERSMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2006" sfwType="FormLinkMaintenance" sfwTitle="Contact NDPERS" version="3.5" sfwEntity="entContactTicket" sfwTemplate="ESSContactNDPERSMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSContactNDPERSLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="NewESSContactNDPERS" />
    <callmethods sfwMode="Update" sfwMethodName="FindESSContactNDPERS" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar">
      <items>
        <sfwTable ID="toolbar" />
        <sfwButton ID="btnSave" sfwMethodName="btnSave_Click" sfwNavigationParameter="aintContactTicketId=ContactTicketId" Text="Save" sfwMessageId="10395" />
        <sfwButton ID="btnCancel" sfwMethodName="btnCancel_Click" Text="Refresh" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Contact Information">
      <items>
        <sfwTable ID="tblMain" />
        <sfwLabel ID="capContactTypeId" Text="Ticket Number : " sfwIsCaption="True" />
        <sfwLabel ID="lblContactTypeId" sfwEntityField="ContactTicketId" />
        <sfwLabel ID="capStatusDescription" Text="Ticket Status : " sfwIsCaption="True" />
        <sfwLabel ID="lblStatusDescription" sfwEntityField="StatusDescription" />
        <sfwLabel ID="capCallbackPhone" Text="Callback Phone Number : " sfwIsCaption="True" />
        <sfwTextBox ID="txtCallbackPhone" sfwDataFormat="{0:(###)###-####}" sfwEntityField="CallbackPhone" />
        <sfwLabel ID="capTimeOfDayValue" Text="Time Of Day to Contact : " sfwIsCaption="True" Visible="False" />
        <sfwDropDownList DataTextField="description" DataValueField="code_value" ID="ddlTimeOfDayValue" sfwLoadType="CodeGroup" sfwLoadSource="5006" sfwEntityField="TimeOfDayValue" Visible="False" />
        <sfwLabel ID="capNotes" Text="Notes : " sfwIsCaption="True" />
        <sfwTextBox ID="txtNotes" Rows="4" TextMode="MultiLine" Width="800px" sfwEntityField="Notes" />
        <sfwLabel ID="lblLabel1" Text="Your request will be processed in the order it was received.  The status of your request will be posted to the PERSLink message board." sfwIsCaption="True" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHistory" sfwCaption="History">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvContactTicketHistory" sfwSortExpression="ContactTicketHistoryId desc" sfwEntityField="lstContactTicketHistory" sfwGridEntity="entContactTicketHistory">
          <Columns>
            <TemplateField HeaderText="Contact Ticket History ID" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactTicketHistoryId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Created Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="CreatedDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Created By">
              <ItemTemplate>
                <sfwLabel sfwEntityField="CreatedBy" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Notes">
              <ItemTemplate>
                <sfwLabel sfwEntityField="Notes" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Ticket Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstContactTicketHistory" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>