<form ID="wfmESSContactTicketMaintenance" WebSite="NeoSpinWSS" sfwResource="2019" sfwRemoteObject="srvNeoSpinWSS" sfwType="FormLinkMaintenance" sfwTitle="ESSContact Ticket Maintenance" sfwActive="True" sfwStatus="Review" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSContactTicketLookup" sfwEntity="entESSHome" sfwTemplate="ESSContactTicketMaintenance.html" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindESSContactTickets" />
  </initialload>
  <items>
    <sfwPanel ID="pnlHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblN" />
        <sfwLiteral ID="ltrlContactTicketMessage" Text="Contact Tickets" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwGroupCaption="ESSContactTicket Details" sfwCaption="Contact Tickets">
      <items>
        <sfwTable ID="tblMain" />
        <sfwButton ID="btnOpenBusContactTicket" sfwMethodName="btnOpen_Click" sfwActiveForm="99=wfmESSContactNDPERSMaintenance;1=wfmESSDeathNoticeMaintenance;3=wfmESSBenefitEstimateMaintenance;2=wfmESSAppointmentScheduleMaintenance;4=wfmESSServicePurchaseEstimateMaintenance;6=wfmESSReportAProblemMaintenance" sfwRelatedControl="grvBusContactTicket" Text="Open" sfwNavigationParameter="aintContactTicketId=ContactTicketId;PortalLaunchScreenID" Visible="False" sfwEntityField="PortalLaunchScreenID" />
        <sfwGridView ID="grvBusContactTicket" AllowPaging="True" AllowSorting="True" sfwSortExpression="ContactTicketId desc" sfwEntityField="lstContactTicket" sfwGridEntity="entContactTicket">
          <Columns>
            <TemplateField HeaderText="Ticket Number">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenBusContactTicket" sfwEntityField="ContactTicketId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Contact Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Ticket Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Appointment Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objAppointmentSchedule.AppointmentTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Appointment Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objAppointmentSchedule.AppointmentDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Counselor">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objCounselor.UserFullName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PortalLaunchScreenID " SortExpression="PortalLaunchScreenID" Visible="False" sfwUseAsSortExpression="True">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PortalLaunchScreenID" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstContactTicket" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>