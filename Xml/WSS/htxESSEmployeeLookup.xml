            <TemplateField HeaderText="First Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="FirstName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Middle Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="MiddleName" />
