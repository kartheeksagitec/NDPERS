<form ID="wfmESSEmployeeLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2021" sfwType="FormLinkLookup" sfwTitle="ESSEmployee Lookup" version="3.5" WebSite="NeoSpinWSS" sfwEntity="entPerson" sfwGridCollection="busPersonLookup.icolPerson" sfwTemplate="ESSEmployeeLookup.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSEmployeeLookup" isBasedOnWfm="true" sfwDefaultButtonID="btnSearch" sfwMaxCount="10000">
  <initialload>
    <query ID="cdoWssMemberRecordRequest" sfwQueryRef="entWssMemberRecordRequest.LookupEmployee" sfwMethodName="LoadEmployees" />
    <query ID="cdoWssMemberRecordRequest1" sfwQueryRef="entWssMemberRecordRequest.subLookupEmployee" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblN" />
        <sfwLiteral ID="ltrlEmployee" Text="Employee Lookup" />
        <sfwLabel ID="lblEnrollInOther457" Text="Enroll Any Employee In Other 457/403(b)." CssClass="lblTextAlignLeft hideControl" ForeColor="Red" sfwIsCaption="True" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
      <items>
        <sfwTable ID="tblCriteria" />
        <sfwLabel ID="capLastName" Text="Last Name : " sfwIsCaption="True" />
        <sfwTextBox ID="txtLastName" sfwDataField="p.last_name" sfwOperator="like" sfwQueryID="cdoWssMemberRecordRequest" />
        <sfwLabel ID="capFirstName" Text="First Name : " sfwIsCaption="True" />
        <sfwTextBox ID="txtFirstName" sfwDataField="p.first_name" sfwOperator="like" sfwQueryID="cdoWssMemberRecordRequest" />
        <sfwLabel ID="capPersonId" Text="PERSLink ID : " sfwIsCaption="True" />
        <sfwTextBox ID="txtPersonIdLookUp" sfwDataField="p.person_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoWssMemberRecordRequest" />
        <sfwLabel ID="capSsn" Text="Last 4 Digits of SSN : " sfwIsCaption="True" />
        <sfwTextBox ID="txtSsn" sfwDataField="substring(p.ssn, 6, 4)" sfwOperator="like" sfwQueryID="cdoWssMemberRecordRequest" />
        <sfwLabel ID="capPlanId" Text="Plan : " sfwIsCaption="True" />
        <sfwDropDownList ID="ddlPlanId" DataTextField="plan_name" DataValueField="plan_id" sfwDataField="spa.plan_id" sfwQueryID="cdoWssMemberRecordRequest1" sfwLoadType="ServerMethod" sfwLoadSource="GetPlanByOrganization" />
        <sfwLabel ID="lblMsgDisplay" Text="To display ALL employees click ‘Search’ without entering any criteria." sfwIsCaption="True" />
        <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
        <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
        <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" CssClass="hideControl" />
        <sfwTextBox ID="txbOrgId" sfwDataField="b.org_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="SubQueryByOrg" Visible="False" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlResult" sfwCaption="Search Results">
      <items>
        <sfwTable ID="tblResult" />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmESSEmployeeMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintpersonid=PersonId;aintOrgId=objCurrentEmployment.OrgId" sfwRelatedControl="dgrResult" Text="Open" />
        <sfwButton ID="btnUpdateTerminateButton" sfwActiveForm="wfmUpdateTermEmploymentMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintemploymentId=objCurrentEmployment.PersonEmploymentId;aintemploymentdetailid=objCurrentEmployment.objLatestEmploymentDetail.PersonEmploymentDtlId" sfwRelatedControl="dgrResult" Text="Open" />
        <sfwButton ID="btnEnrollOther" sfwMethodName="btnOpen_Click" Text="Open" sfwActiveForm="wfmESSEnrollFourFiveSevenMaintenance" sfwRelatedControl="dgrResult" sfwNavigationParameter="aintemploymentId=objCurrentEmployment.PersonEmploymentId;aintemploymentdetailid=objCurrentEmployment.objLatestEmploymentDetail.PersonEmploymentDtlId" />
        <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
        <sfwButton ID="btnTerminateButton" sfwActiveForm="wfmTerminateEmploymentWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintemploymentId=objCurrentEmployment.PersonEmploymentId;aintemploymentdetailid=objCurrentEmployment.objLatestEmploymentDetail.PersonEmploymentDtlId" sfwRelatedControl="dgrResult" Text="Open" Visible="False" />
