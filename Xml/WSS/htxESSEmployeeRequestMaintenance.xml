<form ID="wfmESSEmployeeRequestMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2003" sfwType="FormLinkMaintenance" sfwTitle="ESS Employee Request Maintenance" version="3.5" sfwDefaultButtonID="btnExecuteRefreshFromObject" sfwEntity="entESSHome" sfwTemplate="ESSEmployeeRequestMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmESSHomeLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMode="New" sfwMethodName="FindESSHome" />
    <callmethods sfwMode="Update" sfwMethodName="FindESSHome" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlPanel" sfwCaption="New Employee Requests">
      <items>
        <sfwTable ID="tblNewEmpRequest" />
        <sfwButton ID="btnNewEmpRequestOpen" sfwActiveForm="wfmESSMemberRecordDataWizard" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintMemberRecordRequestID=MemberRecordRequestId" sfwRelatedControl="grvNewEmployeeRequests" Text="Open" Visible="False" />
        <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="grvNewEmployeeRequests" Text="Export To Excel" />
        <sfwGridView ID="grvNewEmployeeRequests" AllowPaging="True" AllowSorting="True" sfwEntityField="lstWssMemberRecordRequest" sfwGridEntity="entWssMemberRecordRequest">
          <Columns>
            <TemplateField HeaderText="Request ID">
              <ItemTemplate>
                <sfwLabel sfwRelatedControl="btnNewEmpRequestOpen" sfwLinkable="True" sfwEntityField="MemberRecordRequestId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PERSLink  ID">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Last Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LastName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="First Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="FirstName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Last 4 Digits&lt;/br&gt; of SSN">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrLastFourDigitsSSN" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status" SortExpression="StatusValue">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejection Reason">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RejectionReason" sfwWrap="True" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejected By">
              <ItemTemplate>
                <sfwLabel sfwEntityField="lstrRejectedByName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Rejected Date">
              <ItemTemplate>
                <sfwLabel sfwEntityField="lstrRejectedDate" sfwDataFormat="{0:d}" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstWssMemberRecordRequest" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>