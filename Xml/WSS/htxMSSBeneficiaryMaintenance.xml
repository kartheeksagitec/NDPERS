<form ID="wfmMSSBeneficiaryMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Your Beneficiaries" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSBeneficiaryMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSBeneficiaryLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSMemberBeneficiary" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Your Beneficiaries" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlSection0" sfwCaption="Beneficiaries">
      <items>
        <sfwTable ID="Section0" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvPersonBeneficiary" sfwEntityField="objPerson.lstBeneficiariesByPersonAccount" sfwGridEntity="entPersonBeneficiary">
          <Columns>
            <TemplateField HeaderText="Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="BeneficiaryName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Birth Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="BeneficiaryDOB" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPersonAccountBeneficiary.BeneficiaryTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Relationship">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RelationshipDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Plan">
              <ItemTemplate>
                <sfwLabel sfwEntityField="IstrMSSPlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Percentage" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:#0.00'%}" sfwEntityField="objPersonAccountBeneficiary.DistPercent" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Start Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objPersonAccountBeneficiary.StartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="End Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="objPersonAccountBeneficiary.EndDate" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage1" sfwCaption="Instructions">
      <items>
        <sfwTable ID="tblNewPage1" />
        <sfwLiteral ID="ltrLiteral" Text="&lt;font style=&quot;padding-left:5px&quot;&gt;To update or change your designation of beneficiary , you must complete and mail a&amp;nbsp;&lt;/font&gt;" />
        <sfwLinkButton ID="btnPdfCorrespondenceClick1" sfwMethodName="btn_OpenPDF" sfwNavigationParameter="pdf_file_name=#MSSHelpFiles\SFN-2560.pdf" />
        <sfwLiteral ID="ltrLiteral1" Text="&amp;nbsp;to NDPERS." />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPerson.lstBeneficiariesByPersonAccount" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
</form>