<form ID="wfmMSSFlexCompEnrollmentMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Flex Comp Enrollment Maintenance" version="3.5" sfwEntity="entWssPersonAccountEnrollmentRequest" sfwTemplate="MSSFlexCompEnrollmentMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSFlexCompEnrollmentLookup" sfwFormObjectType="None" isBasedOnWfm="true" sfwDefaultButtonID="btnSave">
