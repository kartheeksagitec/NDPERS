<form ID="wfmMSSLTCMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="LTC Maintenance" version="3.5" sfwEntity="entMSSLTCWeb" sfwTemplate="MSSLTCMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSLTCLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindInsurancePlanLTC" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnltoolbar" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage2" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
        <sfwImageButton ID="btnPrint" ImageUrl="~/Images/print.png" ImageAlign="AbsMiddle" OnClientClick="PrintCenterMiddle(e)" sfwMethodName="btnOpen_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage1" />
        <sfwLiteral ID="ltrlMessage" Text="Long Term Care Insurance Plan" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Plan Details">
      <items>
        <sfwTable ID="tblMain" />
        <sfwLabel ID="capPlanName" Text="Plan Name : " sfwIsCaption="True" />
        <sfwLabel ID="lblPlanName" sfwEntityField="objPlan.MssPlanName" />
        <sfwLabel ID="capPlanParticipationStatusDescription" Text="Plan Participation Status : " sfwIsCaption="True" />
        <sfwLabel ID="lblPlanParticipationStatusDescription" sfwEntityField="IstrPlanParticipation" />
        <sfwLabel ID="capStartDate" Text="Start Date : " sfwIsCaption="True" />
        <sfwLabel ID="lblStartDate" sfwDataFormat="{0:d}" sfwEntityField="IdtStartDate" />
        <sfwLabel ID="capEndDateNoNull" Text="End Date : " sfwIsCaption="True" />
        <sfwLabel ID="lblEndDateNoNull" sfwDataFormat="{0:d}" sfwEntityField="IdtEndDate" />
        <sfwLabel ID="lblPersonEmploymentDtlId" sfwEntityField="PersonEmploymentDtlId" />
        <sfwLabel ID="lblPersonId" Visible="False" sfwEntityField="PersonId" />
        <sfwLabel ID="lblPersonAccountId" Visible="False" sfwEntityField="PersonAccountId" />
        <sfwLabel ID="lblPlanId" Visible="False" sfwEntityField="PlanId" />
        <sfwLabel AssociatedControlID="lblOrgName" ID="capOrgName" Text="Provider :" sfwIsCaption="True" />
        <sfwLabel ID="lblOrgName" sfwEntityField="objProvider.OrgName" />
        <sfwLabel ID="lblIintEnrollmentRequestId" Visible="False" sfwEntityField="IintEnrollmentRequestID" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlLevelofCoverage" sfwCaption="Level of Coverage">
      <items>
        <sfwTable ID="tblNewPage3" />
        <sfwGridView AllowPaging="True" ID="grvLtcOptionMember" sfwEntityField="lstLTCOptions" sfwGridEntity="entPersonAccountLtcOption">
          <Columns>
            <TemplateField HeaderText="Level Of Coverage">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LevelOfCoverageDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective Start Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveStartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective End Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveEndDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Insurance Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LtcInsuranceTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Account Ltc Option ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonAccountLtcOptionId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Plan Option Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanOptionStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Monthly Premium Amount">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="IdecMonthlyPremium" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlHistory" sfwCaption="History">
      <items>
        <sfwTable ID="tblNewPage5" />
        <sfwGridView AllowPaging="True" ID="grvPreviousHistory" sfwEntityField="lstPreviousHistory" sfwGridEntity="entPersonAccountLtcOptionHistory">
          <Columns>
            <TemplateField HeaderText="Plan Participation Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanParticipationStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Level Of Coverage">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LevelOfCoverageDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective Start Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveStartDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective End Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveEndDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Ltc Insurance Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LtcInsuranceTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Ltc Relationship">
              <ItemTemplate>
                <sfwLabel sfwEntityField="LtcRelationshipDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Plan Option Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PlanOptionStatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Account Ltc Option History ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonAccountLtcOptionHistoryId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage" sfwCaption="New Page" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage4" />
        <sfwLinkButton ID="btnLinkButton" sfwActiveForm="wfmMSSLTCEnrollmentMaintenance" sfwMethodName="btnOpen_Click" sfwVisibleRule="IblnIsEnrollmentRequestRejectedOrPending" />
        <sfwButton ID="btnNew" sfwActiveForm="wfmMSSLTCEnrollmentMaintenance" sfwMethodName="btnNew_Click" sfwVisibleRule="IblnNoEnrollmentRequestExits" Text="Update" />
        <sfwButton ID="btnPdfCorrespondenceClick" sfwNavigationParameter="pdf_file_name=#PDFCorrespondence\Templates\SFN-50134.pdf" sfwMethodName="btnPdfCorrespondence_Click" sfwVisibleRule="IsIbsFlagChecked" Text="SFN-50134 Authorization for Automatic Premium Deduction" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlDependents" sfwCaption="Dependents" Visible="False">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwGridView AllowPaging="True" ID="grvMSSPersonDependent" sfwEntityField="lstMSSPersonDependent" sfwGridEntity="entPersonDependent">
          <Columns>
            <TemplateField HeaderText="Dependent Name">
              <ItemTemplate>
                <sfwLabel sfwEntityField="DependentName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Person Dependent ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonDependentId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Relationship">
              <ItemTemplate>
                <sfwLabel sfwEntityField="RelationshipDescription" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPlan" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="objProvider" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstLTCOptions" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstPreviousHistory" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
    <entityfield ID="lstMSSPersonDependent" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
