<form ID="wfmMSSPaymentHistoryInformationMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Payment History Information" version="3.5" sfwEntity="entPayeeAccount" sfwTemplate="MSSPaymentHistoryInformationMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSPaymentHistoryInformationLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindPaymentHistory" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Payment History Information" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage6" sfwCaption="pnlBack" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage2" />
        <sfwImageButton ID="btnBack" ImageAlign="AbsMiddle" ImageUrl="~/Images/back.gif" sfwMethodName="btnBack_Click" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage3" sfwCaption="Payment History">
      <items>
        <sfwTable ID="tblNewsPage34" />
        <sfwButton ID="btnOpenPaymentDetailsLookup" sfwActiveForm="wfmMSSPaymentHistoryMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPayeePaymentHistoryId=PaymentHistoryHeaderId" sfwRelatedControl="dgrResult" Text="Open Lookup" Visible="False" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" PageSize="12" sfwEntityField="lstPaymentDetailsThatYear" sfwGridEntity="entPaymentHistoryHeader">
          <Columns>
            <TemplateField HeaderText="Payment Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwLinkable="True" sfwRelatedControl="btnOpenPaymentDetailsLookup" sfwEntityField="PaymentDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Status">
              <ItemTemplate>
                <sfwLabel sfwEntityField="StatusDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Gross Benefit" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="GrossAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Taxable Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="TaxableAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Non-Taxable Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="NonTaxableAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Federal Tax" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="FedAdditionalTaxAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="State Tax" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="StateAdditionalTaxAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Other Deductions" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="DeductionAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Net Payment" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="NetAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payment ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="PaymentHistoryHeaderId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Mss Plan Name" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPlan.MssPlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Plan" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objPlan.PlanName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payee Account ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PayeeAccountId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PERSLink ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PersonId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Org Code" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="objOrganization.OrgCode" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLiteral ID="ltrLiteral" Text="&lt;font style=&quot;padding-left:5px&quot;&gt;Select the Payment Date to view payment details.  Please note that Payment Details prior to 1/1/2011 are not available&lt;/font&gt;" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstPaymentDetailsThatYear" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>