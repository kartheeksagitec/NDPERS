<form ID="wfmMSSPaymentHistorySummaryMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Payment History Summary" version="3.5" sfwEntity="entPayeeAccount" sfwTemplate="MSSPaymentHistorySummaryMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSPaymentHistorySummaryLookup" sfwFormObjectType="None" isBasedOnWfm="true">
