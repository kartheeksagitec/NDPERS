            <sfwLabel ID="capIdecpaidgrossamount2" Text="Total Gross Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidgrossamount2" sfwDataFormat="{0:C}" sfwEntityField="IdecMSSYTDpaidgrossamount" />
            <sfwLabel ID="capIdecpaidtaxableamount2" Text="Total Taxable Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidtaxableamount2" sfwDataFormat="{0:C}" sfwEntityField="IdecMSSYTDpaidtaxableamount" />
            <sfwLabel ID="capIdecpaidnontaxableamount2" Text="Total Non-Taxable Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidnontaxableamount2" sfwDataFormat="{0:C}" sfwEntityField="IdecMSSYTDpaidnontaxableamount" />
            <sfwLabel ID="capIdectotalfedtaxamount" Text="Total Federal Tax Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdectotalfedtaxamount" sfwDataFormat="{0:C}" sfwEntityField="IdecMSSYTDTotalFederalTaxAmount" />
            <sfwLabel ID="capIdectotalNDstatetaxamount" Text="Total ND State Tax Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdectotalNDstatetaxamount" sfwDataFormat="{0:C}" sfwEntityField="IdecMSSYTDTotalNDStateTaxAmount" />
          </items>
        </sfwPanel>
        <sfwPanel ID="pnlNewPage1" sfwCaption="New Page" sfwCollapsible="False">
          <items>
            <sfwTable ID="tblNewPage1" />
            <sfwLabel ID="capLTD1" Text="Lifetime Benefit Paid To Date (LTD):" sfwIsCaption="True" />
            <sfwLabel ID="capIdecpaidgrossamount1" Text="Total Gross Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidgrossamount" sfwDataFormat="{0:C}" sfwEntityField="Idecpaidgrossamount" />
            <sfwLabel ID="capIdecpaidtaxableamount1" Text="Total Taxable Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidtaxableamount" sfwDataFormat="{0:C}" sfwEntityField="Idecpaidtaxableamount" />
            <sfwLabel ID="capIdecpaidnontaxableamount1" Text="Total Non-Taxable Amount : " sfwIsCaption="True" />
            <sfwLabel ID="lblIdecpaidnontaxableamount" sfwDataFormat="{0:C}" sfwEntityField="Idecpaidnontaxableamount" />
          </items>
        </sfwPanel>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlNewPage3" sfwCaption="Payment History">
      <items>
        <sfwTable ID="tblNewsPage34" />
        <sfwButton ID="btnOpenPaymentDetailsLookup" sfwActiveForm="wfmMSSPaymentHistoryInformationMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPayeeAccountID=PayeeAccountId;ldatePaymentDateFrom=PaymentYearStartDate;ldatePaymentDateTo=PaymentYearEndDate" sfwRelatedControl="grvPaymentDetails" Text="Open Lookup" Visible="False" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="grvPaymentDetails" sfwEntityField="lstMSSPaymentHistoryHeader" sfwGridEntity="entPaymentHistoryHeader">
          <Columns>
            <TemplateField HeaderText="Payment Year" ItemStyle.HorizontalAlign="Left">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenPaymentDetailsLookup" sfwEntityField="PaymentYear" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Gross Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="GrossAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Taxable Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="TaxableAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Non-Taxable Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="NonTaxableAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Deduction Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="DeductionAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Net Amount" ItemStyle.HorizontalAlign="Right">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:C}" sfwEntityField="NetAmount" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payment History Header ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PaymentHistoryHeaderId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Payee Account ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="PayeeAccountId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PaymentYearEndDate" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="PaymentYearEndDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="PaymentYearStartDate" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="PaymentYearStartDate" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
        <sfwLiteral ID="ltrLiteral" Text="&lt;font style=&quot;padding-left:5px&quot;&gt;Select the Payment Year to view payment details for that particular year.&lt;/font&gt;" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstMSSPaymentHistoryHeader" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>