<form ID="wfmMSSPensionPaymentDetailsMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="Your Retirement Payment Account(s)" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSPensionPaymentDetailsMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSPensionPaymentDetailsLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSAllPayeeAccounts" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlHeading" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblHeading" />
        <sfwLiteral Text="Your Retirement Payment Account(s)" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="Payee Accounts" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblMain" />
        <sfwLiteral ID="ltrLabel" Text="&lt;font style=&quot;padding-left:5px&quot;&gt;Select the Benefit Type link below to view the details of your benefit payment&lt;/font&gt;" />
        <sfwPanel ID="pnlNewPage" sfwCaption="Select an Account" CssClass="GridColumnHide">
          <items>
            <sfwTable ID="tblNewPage" />
            <sfwGridView AllowPaging="True" ID="grvMemberPlusAlternatePayeeAccounts" sfwEntityField="lstMemberPlusAlternateActivePayeeAccounts" sfwGridEntity="entPayeeAccount">
              <Columns>
                <TemplateField HeaderText="Benefit Type">
                  <ItemTemplate>
                    <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpenMemberPlusAlternatePayeeAccount" sfwEntityField="BenefitAccountTypeDescription" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Plan" SortExpression="objPlan.MssPlanName">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="objPlan.PlanName" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Benefit Sub Type">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="BenefitAccountSubTypeDescription" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Benefit Option Plan">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="BenefitOptionDescription" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Benefit Begin Date">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="BenefitBeginDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Benefit End Date">
                  <ItemTemplate>
                    <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="BenefitEndDate" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Payment Account Status">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="objPayeeAccountActiveStatus.StatusDescription" />
                  </ItemTemplate>
                </TemplateField>
                <TemplateField HeaderText="Payee Account ID">
                  <ItemTemplate>
                    <sfwLabel sfwEntityField="PayeeAccountId" CssClass="hideControl" />
                  </ItemTemplate>
                </TemplateField>
              </Columns>
            </sfwGridView>
            <sfwButton ID="btnOpenMemberPlusAlternatePayeeAccounts" sfwActiveForm="wfmMSSPayeeAccountsMaintenance" sfwMethodName="btnOpenPayee_Click" sfwNavigationParameter="aint_payee_account_id=PayeeAccountId" sfwRelatedControl="grvMemberPlusAlternatePayeeAccounts" Text="Open" Visible="False" CssClass="hideControl" />
            <sfwButton ID="btnOpenMemberPlusAlternatePayeeAccount" sfwActiveForm="wfmMSSPayeeAccountsMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintPayeeAccountId=PayeeAccountId" sfwRelatedControl="grvMemberPlusAlternatePayeeAccounts" Text="Open" CssClass="hideControl" />
          </items>
        </sfwPanel>
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="lstMemberPlusAlternateActivePayeeAccounts" sfwNewAutoLoad="False" sfwUpdateAutoLoad="False" />
  </loaddetails>
  <ExtraFields />
</form>