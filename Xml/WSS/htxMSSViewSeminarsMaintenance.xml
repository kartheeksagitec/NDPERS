<form ID="wfmMSSViewSeminarsMaintenance" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2001" sfwType="FormLinkMaintenance" sfwTitle="View Seminars Maintenance" version="3.5" sfwEntity="entMSSHome" sfwTemplate="MSSViewSeminarsMaintenance.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmMSSViewSeminarsLookup" sfwFormObjectType="None" isBasedOnWfm="true">
  <initialload>
    <callmethods sfwMethodName="FindMSSViewSeminars" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlNHeader" sfwCollapsible="False">
      <items>
        <sfwTable ID="tblNewPage" />
        <sfwLiteral ID="ltrlMessage" Text="Seminar Information" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlMain" sfwCaption="View Seminars" CssClass="hideControl">
      <items>
        <sfwTable ID="tblMain" />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmMSSSeminarMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintContactTicketId=ContactTicketId;aintPersonID=objPerson.PersonId" Text="Open" Visible="False" sfwRelatedControl="grvSeminars" />
        <sfwLabel ID="lblPersonId" sfwEntityField="objPerson.PersonId" />
        <sfwGridView AllowPaging="True" ID="grvSeminars" sfwEntityField="lstSeminars" sfwGridEntity="entSeminarSchedule">
          <Columns>
            <TemplateField HeaderText="Seminar Name">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="SeminarName" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Seminar Type">
              <ItemTemplate>
                <sfwLabel sfwEntityField="SeminarTypeDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Seminar Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="SeminarDate1" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Contact Ticket ID" Visible="False">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ContactTicketId" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlSeminarInfo">
      <items>
        <sfwLabel ID="lblSeminarText" Text="Please visit our website to view upcoming seminars.&lt;a href=&quot;https://ndpers.nd.gov/events &quot; target=&quot;_blank&quot;&gt;https://ndpers.nd.gov/events &lt;/a&gt;&lt;/br&gt;&lt;/br&gt;" />
      </items>
    </sfwPanel>
  </items>
  <loaddetails>
    <entityfield ID="objPerson" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
    <entityfield ID="lstSeminars" sfwUpdateAutoLoad="False" sfwNewAutoLoad="False" />
  </loaddetails>
</form>