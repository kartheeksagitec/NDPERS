<form ID="wfmWssAcknowledgementLookup" sfwActive="True" sfwStatus="Review" sfwRemoteObject="srvNeoSpinWSS" sfwResource="2000" sfwType="FormLinkLookup" sfwTitle="WSS Acknowledgement Lookup" version="3.5" sfwEntity="entWssAcknowledgement" sfwGridCollection="busWssAcknowledgementLookup.iclbWssAcknowledgement" sfwTemplate="WssAcknowledgementLookup.html" sfwAspNetCaching="False" sfwMenuItemID="wfmDefaultLookup.aspx?FormID=wfmWssAcknowledgementLookup" isBasedOnWfm="true">
  <initialload>
    <query ID="cdoWssAcknowledgement" sfwQueryRef="entWssAcknowledgement.Lookup" sfwMethodName="LoadWssAcknowledgements" />
  </initialload>
  <items>
    <sfwTable ID="Main" />
    <sfwPanel ID="pnlCriteria" sfwCaption="Criteria">
      <items>
        <sfwTable ID="tblCriteria" />
        <sfwLabel AssociatedControlID="txtAcknowledgementId" ID="capAcknowledgementId" Text="Acknowledgement ID : " sfwIsCaption="True" />
        <sfwTextBox ID="txtAcknowledgementId" sfwDataField="acknowledgement_id" sfwDataType="Numeric" sfwOperator="=" sfwQueryID="cdoWssAcknowledgement" />
        <sfwLabel AssociatedControlID="txtEffectiveDate" ID="capEffectiveDate" Text="Effective Date : " sfwIsCaption="True" />
        <sfwTextBox ID="txtEffectiveDate" sfwDataField="effective_date" sfwDataFormat="{0:d}" sfwDataType="DateTime" sfwOperator="=" sfwQueryID="cdoWssAcknowledgement" />
        <sfwLabel AssociatedControlID="txtAcknowledgementText" ID="capAcknowledgementText" Text="Acknowledgement Text : " sfwIsCaption="True" />
        <sfwTextBox ID="txtAcknowledgementText" sfwDataField="acknowledgement_text" sfwOperator="like" sfwQueryID="cdoWssAcknowledgement" />
        <sfwLabel AssociatedControlID="ddlScreenStepValue" ID="capScreenStepValue" Text="Screen Step : " sfwIsCaption="True" />
        <sfwDropDownList ID="ddlScreenStepValue" sfwDataField="screen_step_value" sfwOperator="like" sfwQueryID="cdoWssAcknowledgement" sfwLoadType="CodeGroup" sfwLoadSource="6000" />
        <sfwButton ID="btnSearch" sfwMethodName="btnSearch_Click" Text="Search" />
        <sfwButton ID="btnReset" sfwMethodName="btnReset_Click" Text="Reset" />
        <sfwButton ID="btnStoreSearch" sfwMethodName="btnStoreUserDefaults_Click" Text="Store Search" />
      </items>
    </sfwPanel>
    <sfwPanel ID="pnlResult" sfwCaption="Search Results">
      <items>
        <sfwTable ID="tblResult" />
        <sfwButton ID="btnNew" sfwActiveForm="wfmWssAcknowledgementMaintenance" sfwMethodName="btnNew_Click" Text="New" />
        <sfwButton ID="btnOpen" sfwActiveForm="wfmWssAcknowledgementMaintenance" sfwMethodName="btnOpen_Click" sfwNavigationParameter="aintacknowledgementid=AcknowledgementId" sfwRelatedControl="dgrResult" sfwSelection="Many" Text="Open" />
        <sfwButton ID="btnExportExcel" sfwMethodName="btnColumnsToExport_Click" sfwRelatedControl="dgrResult" Text="Export To Excel" />
        <sfwGridView AllowPaging="True" AllowSorting="True" ID="dgrResult" sfwSelection="Many">
          <Columns>
            <TemplateField HeaderText="Acknowledgement ID">
              <ItemTemplate>
                <sfwLabel sfwLinkable="True" sfwRelatedControl="btnOpen" sfwEntityField="AcknowledgementId" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Effective Date">
              <ItemTemplate>
                <sfwLabel sfwDataFormat="{0:d}" sfwEntityField="EffectiveDate" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Screen Step Value">
              <ItemTemplate>
                <sfwLabel sfwEntityField="ScreenStepDescription" />
              </ItemTemplate>
            </TemplateField>
            <TemplateField HeaderText="Acknowledgement Text">
              <ItemTemplate>
                <sfwLabel sfwEntityField="AcknowledgementText" />
              </ItemTemplate>
            </TemplateField>
          </Columns>
        </sfwGridView>
      </items>
    </sfwPanel>
  </items>
  <loaddetails />
</form>